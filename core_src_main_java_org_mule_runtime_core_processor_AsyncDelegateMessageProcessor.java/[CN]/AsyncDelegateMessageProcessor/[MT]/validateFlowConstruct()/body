{
  if (flowConstruct == null) {
    throw new IllegalArgumentException("FlowConstruct cannot be null");
  }
 else   if (!(flowConstruct instanceof StageNameSourceProvider)) {
    throw new IllegalArgumentException(String.format("FlowConstuct must implement the %s interface. However, the type %s does not implement it",StageNameSourceProvider.class.getCanonicalName(),flowConstruct.getClass().getCanonicalName()));
  }
}
