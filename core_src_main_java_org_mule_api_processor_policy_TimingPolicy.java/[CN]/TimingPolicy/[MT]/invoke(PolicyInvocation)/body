{
  final MuleEvent invocationEvent=invocation.getEvent();
  long startTime=System.currentTimeMillis();
  final MuleEvent result=invocation.proceed();
  long executionTime=System.currentTimeMillis() - startTime;
  logger.info(String.format("%s took %dms to process event [%s]",invocationEvent.getFlowConstruct().getName(),executionTime,invocationEvent.getId()));
  return result;
}
