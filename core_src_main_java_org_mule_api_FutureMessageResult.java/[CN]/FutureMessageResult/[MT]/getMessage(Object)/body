{
  MuleMessage result=null;
  if (obj != null) {
    if (obj instanceof MuleMessage) {
      result=(MuleMessage)obj;
    }
 else {
      result=new DefaultMuleMessage(obj,muleContext);
    }
synchronized (this) {
      if (transformers != null) {
        result.applyTransformers(null,transformers,null);
      }
    }
  }
  return result;
}
