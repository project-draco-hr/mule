{
  MuleMessage result=null;
  if (obj != null) {
    if (obj instanceof MuleMessage) {
      result=(MuleMessage)obj;
    }
 else {
      result=new DefaultMuleMessage(obj);
    }
synchronized (this) {
      if (transformers != null) {
        result.applyTransformers(transformers);
      }
    }
  }
  return result;
}
