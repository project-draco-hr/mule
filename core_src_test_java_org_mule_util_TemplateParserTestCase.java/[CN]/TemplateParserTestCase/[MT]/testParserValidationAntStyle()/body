{
  TemplateParser tp=TemplateParser.createAntStyleParser();
  assertTrue(tp.isValid("${}"));
  assertTrue(tp.isValid("${}${}"));
  assertFalse(tp.isValid("${}&{}"));
  assertFalse(tp.isValid("{}${}"));
  assertFalse(tp.isValid("${$}${}"));
  assertFalse(tp.isValid("${${}}${}"));
  assertFalse(tp.isValid("$ {}"));
}
