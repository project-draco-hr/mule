{
  super.initialise();
  try {
    UMOModel systemModel=ModelFactory.createModel(SYSTEM_MODEL_TYPE);
    systemModel.setName(SYSTEM_MODEL);
    systemModel.initialise();
    registerModel(systemModel);
    boolean disableAdmin=MapUtils.getBooleanValue(System.getProperties(),MuleProperties.DISABLE_SERVER_CONNECTIONS_SYSTEM_PROPERTY,false);
    if (!disableAdmin) {
      UMOAgent agent=new MuleAdminAgent();
      agent.initialise();
      registerAgent(agent);
    }
  }
 catch (  UMOException e) {
    throw new InitialisationException(e,this);
  }
}
