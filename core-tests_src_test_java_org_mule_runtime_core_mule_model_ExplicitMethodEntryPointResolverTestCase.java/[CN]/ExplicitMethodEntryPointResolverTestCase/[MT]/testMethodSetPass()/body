{
  ExplicitMethodEntryPointResolver resolver=new ExplicitMethodEntryPointResolver();
  resolver.addMethod("someBusinessMethod");
  InvocationResult result=resolver.invoke(new MultiplePayloadsTestObject(),MuleTestUtils.getTestEventContext("blah",REQUEST_RESPONSE,muleContext));
  assertEquals(result.getState(),InvocationResult.State.SUCCESSFUL);
}
