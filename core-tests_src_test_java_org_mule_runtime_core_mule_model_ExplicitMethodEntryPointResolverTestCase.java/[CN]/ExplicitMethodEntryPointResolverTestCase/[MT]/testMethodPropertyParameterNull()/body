{
  ExplicitMethodEntryPointResolver resolver=new ExplicitMethodEntryPointResolver();
  resolver.addMethod("someOtherBusinessMethod");
  InvocationResult result=resolver.invoke(new MultiplePayloadsTestObject(),MuleTestUtils.getTestEventContext(new Object[]{null,"blah"},REQUEST_RESPONSE,muleContext));
  assertEquals(result.getState(),InvocationResult.State.SUCCESSFUL);
}
