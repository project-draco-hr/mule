{
  byte[] bytes=(byte[])src;
  if (bytes[0] == (byte)((ObjectStreamConstants.STREAM_MAGIC >>> 8) & 0xFF)) {
    return super.doTransform(src,encoding);
  }
 else {
    try {
      return new String(bytes,encoding);
    }
 catch (    Exception e) {
      throw new TransformerException(this,e);
    }
  }
}
