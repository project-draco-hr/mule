{
  Callable c=new Callable(){
    public Object call() throws InterruptedException {
      Thread.sleep(3000L);
      wasCalled=true;
      return null;
    }
  }
;
  FutureMessageResult f=new FutureMessageResult(c,muleContext);
  f.execute();
  try {
    f.getMessage(500L);
    fail();
  }
 catch (  TimeoutException tex) {
    f.cancel(true);
  }
 finally {
    assertFalse(wasCalled);
  }
}
