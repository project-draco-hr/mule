{
  try {
    startLatch.await();
  }
 catch (  InterruptedException e) {
    logger.warn("Retry thread interupted for callback: " + callback.getWorkDescription());
    return;
  }
  try {
    context.setDelegateContext(delegate.execute(callback));
  }
 catch (  FatalConnectException e) {
    this.exception=e;
    logger.fatal(e,e);
  }
}
