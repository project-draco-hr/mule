{
  String title="";
  int quantity=0;
  Iterator it=sales.entrySet().iterator();
  Entry<String,Integer> entry;
  while (it.hasNext()) {
    entry=(Entry<String,Integer>)it.next();
    if (entry.getValue() > quantity) {
      title=entry.getKey();
      quantity=entry.getValue();
    }
  }
  return title;
}
