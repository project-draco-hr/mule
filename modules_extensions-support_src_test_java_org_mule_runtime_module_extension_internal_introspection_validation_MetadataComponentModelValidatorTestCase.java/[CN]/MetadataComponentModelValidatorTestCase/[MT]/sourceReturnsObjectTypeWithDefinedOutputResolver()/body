{
  when(sourceModel.getOutput()).thenReturn(new ImmutableOutputModel("",toMetadataType(Object.class),false,emptySet()));
  when(sourceModel.getMetadataResolverFactory()).thenReturn(new DefaultMetadataResolverFactory(NullMetadataResolver.class,emptyMap(),SimpleOutputResolver.class,SimpleOutputResolver.class));
  validator.validate(extensionModel);
}
