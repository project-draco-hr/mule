{
  if (session != null) {
    try {
      ((AQjmsSession)session).getDBConnection().close();
      session.close();
    }
 catch (    SQLException e) {
      log.error("Failed to close the Oracle JMS session's underlying JDBC connection",e);
    }
catch (    JMSException e) {
      log.error("Failed to close the Oracle JMS session's underlying JDBC connection",e);
    }
  }
  super.closeQuietly(session);
}
