{
  beanDefinitionRegistry.registerBeanDefinition("_muleParentContextPropertyPlaceholderProcessor",getBeanDefinition(ParentContextPropertyPlaceholderProcessor.class));
  HashMap<Object,Object> factories=new HashMap<>();
  factories.put("hostname",new HostNameFactory());
  beanDefinitionRegistry.registerBeanDefinition("_mulePropertyPlaceholderProcessor",getBeanDefinitionBuilder(PropertyPlaceholderProcessor.class).addPropertyValue("factories",factories).addPropertyValue("ignoreUnresolvablePlaceholders",true).getBeanDefinition());
}
