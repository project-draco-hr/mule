{
  Set<String> exported=new HashSet<>();
  if (StringUtils.isNotBlank(exportedPackages)) {
    final String[] exports=exportedPackages.split(",");
    for (    String export : exports) {
      export=export.trim();
      if (export.startsWith("/")) {
        export=export.substring(1);
      }
      exported.add(export);
    }
  }
  return exported;
}
