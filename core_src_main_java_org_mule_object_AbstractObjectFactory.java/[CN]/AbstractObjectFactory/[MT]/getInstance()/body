{
  if (objectClass == null) {
    throw new InitialisationException(MessageFactory.createStaticMessage("Object factory has not been initialized."),this);
  }
  Object object=ClassUtils.instanciateClass(objectClass,ClassUtils.NO_ARGS);
  if (properties != null) {
    BeanUtils.populate(object,properties);
  }
  fireInitialisationCallbacks(object);
  return object;
}
