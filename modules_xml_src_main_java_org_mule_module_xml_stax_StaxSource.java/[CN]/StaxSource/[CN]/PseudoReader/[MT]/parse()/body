{
  try {
    handler.bridge();
    reader.close();
  }
 catch (  XMLStreamException e) {
    SAXParseException se=new SAXParseException(e.getMessage(),null,null,e.getLocation().getLineNumber(),e.getLocation().getColumnNumber(),e);
    if (errorHandler != null)     errorHandler.fatalError(se);
    throw se;
  }
}
