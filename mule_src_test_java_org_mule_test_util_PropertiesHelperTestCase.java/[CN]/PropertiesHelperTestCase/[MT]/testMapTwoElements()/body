{
  Map props=CollectionUtil.mapWithKeysAndValues(HashMap.class,new Object[]{"foo","foozle"},new Object[]{"bar","doozle"});
  assertEquals("{foo=bar, foozle=doozle}",PropertiesHelper.propertiesToString(props,false));
  assertEquals("{" + Utility.CRLF + "foo=bar"+ Utility.CRLF+ "foozle=doozle"+ Utility.CRLF+ "}",PropertiesHelper.propertiesToString(props,true));
}
