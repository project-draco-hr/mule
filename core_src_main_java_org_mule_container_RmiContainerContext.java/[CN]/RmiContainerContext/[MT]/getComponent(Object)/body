{
  Object object;
  if (key == null) {
    throw new ObjectNotFoundException("null");
  }
  try {
    object=context.lookup(key.toString());
  }
 catch (  NamingException e) {
    throw new ObjectNotFoundException(key.toString(),e);
  }
  if (object == null) {
    throw new ObjectNotFoundException(key.toString());
  }
 else {
    return object;
  }
}
