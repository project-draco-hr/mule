{
  JmsUtils.closeQuietly(producer);
  JmsUtils.closeQuietly(consumer);
  JmsUtils.closeQuietly(receiveSession);
  if (TransactionCoordination.getInstance().getTransaction() == null) {
    JmsUtils.closeQuietly(session);
  }
  producer=null;
  consumer=null;
  receiveSession=null;
  session=null;
}
