{
  int oldPos=pos;
  try {
    String s=(String)cpool[i];
    if (s == null) {
      pos=cpoolIndex[i];
      int len=readShort();
      skipFully(len);
      cpool[i]=new String(buf,pos - len,len,"utf-8");
      s=new String(buf,pos - len,len,"utf-8");
    }
    return s;
  }
  finally {
    pos=oldPos;
  }
}
