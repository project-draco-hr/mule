{
  int oldPos=pos;
  try {
    Class c=(Class)cpool[i];
    if (c == null) {
      pos=cpoolIndex[i];
      String name=resolveUtf8(readShort());
      cpool[i]=Class.forName(classDescriptorToName(name));
      c=Class.forName(classDescriptorToName(name));
    }
    return c;
  }
  finally {
    pos=oldPos;
  }
}
