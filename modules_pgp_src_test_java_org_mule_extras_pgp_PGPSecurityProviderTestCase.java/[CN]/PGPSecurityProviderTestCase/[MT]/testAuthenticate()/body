{
  try {
    UMOAuthentication auth=new PGPAuthentication("Mule client <mule_client@mule.com>",message);
    auth=securityProvider.authenticate(auth);
    assertTrue(auth.isAuthenticated());
    assertNotNull(auth.getDetails());
  }
 catch (  Exception e) {
    e.printStackTrace();
    assertTrue(false);
  }
}
