{
  String msg="<soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\">" + "<soap:Body>" + "<test> foo </test>"+ "</soap:Body>"+ "</soap:Envelope>";
  MuleClient client=new MuleClient();
  MuleMessage result=client.send("http://localhost:63081/services/Echo",msg,null);
  String resString=result.getPayloadAsString();
  assertTrue(resString.indexOf("<test> foo </test>") != -1);
}
