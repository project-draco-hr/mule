{
  StringBuffer sb=new StringBuffer();
  sb.append("<" + getValueElementName() + " type=\"");
  if (JsonUtils.isString(value)) {
    sb.append("string");
  }
 else   if (JsonUtils.isBoolean(value)) {
    sb.append("boolean");
  }
 else   if (JsonUtils.isNumber(value)) {
    sb.append("number");
  }
 else {
    sb.append("unknown");
  }
  sb.append("\">");
  sb.append(value);
  sb.append("</" + getValueElementName() + ">");
  return sb.toString();
}
