{
  try {
    URI uri=event.getEndpoint().getEndpointURI().getUri();
    this.processCookies(event);
    client.executeMethod(getHostConfig(uri),httpMethod);
    return httpMethod;
  }
 catch (  IOException e) {
    throw new DispatchException(event.getMessage(),event.getEndpoint(),e);
  }
catch (  Exception e) {
    throw new DispatchException(event.getMessage(),event.getEndpoint(),e);
  }
}
