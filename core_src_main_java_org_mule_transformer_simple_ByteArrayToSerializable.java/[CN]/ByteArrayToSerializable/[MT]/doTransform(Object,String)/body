{
  try {
    final Object result;
    if (src instanceof byte[]) {
      result=SerializationUtils.deserialize((byte[])src,muleContext);
    }
 else {
      result=SerializationUtils.deserialize((InputStream)src,muleContext);
    }
    return result;
  }
 catch (  Exception e) {
    throw new TransformerException(CoreMessages.transformFailed("byte[]","Object"),this,e);
  }
}
