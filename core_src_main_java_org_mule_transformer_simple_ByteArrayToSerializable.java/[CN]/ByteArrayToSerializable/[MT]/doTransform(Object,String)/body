{
  try {
    if (src instanceof byte[]) {
      return SerializationUtils.deserialize((byte[])src);
    }
 else {
      return SerializationUtils.deserialize((InputStream)src);
    }
  }
 catch (  Exception e) {
    throw new TransformerException(CoreMessages.transformFailed("byte[]","Object"),this,e);
  }
}
