{
  Element replyToElement=router.getChild("reply-to");
  if (replyToElement != null) {
    String replyTo=replyToElement.getAttributeValue(MuleTag.TAG_ATTRIBUTE_ADDRESS);
    if (replyTo != null) {
      GraphNode out=(GraphNode)endpointRegistry.getEndpoint(replyTo,componentName);
      graph.addEdge(routerNode,out).getInfo().setCaption("sets");
    }
  }
}
