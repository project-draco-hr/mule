{
  StringBuilder buf=new StringBuilder(128);
  buf.append("Properties{");
  buf.append(PropertyScope.INBOUND_NAME).append(":");
  buf.append(MapUtils.toString(inboundMap,false));
  buf.append(", ");
  buf.append(PropertyScope.OUTBOUND_NAME).append(":");
  buf.append(MapUtils.toString(inboundMap,false));
  buf.append("}");
  return buf.toString();
}
