{
  UMOImmutableEndpoint ep1=managementContext.getRegistry().lookupEndpointFactory().getInboundEndpoint("ep1",managementContext);
  ep1.dispatch(getTestEvent("testing"));
  UMOImmutableEndpoint ep2=managementContext.getRegistry().lookupEndpointFactory().getInboundEndpoint("ep2",managementContext);
  ep2.dispatch(getTestEvent("testing"));
  Thread.sleep(3000);
  assertEquals(2,managementContext.getRegistry().getConnectors().size());
}
