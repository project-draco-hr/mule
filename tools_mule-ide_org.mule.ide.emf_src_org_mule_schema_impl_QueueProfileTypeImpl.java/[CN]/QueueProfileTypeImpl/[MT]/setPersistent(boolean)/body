{
  boolean oldPersistent=persistent;
  persistent=newPersistent;
  boolean oldPersistentESet=persistentESet;
  persistentESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.QUEUE_PROFILE_TYPE__PERSISTENT,oldPersistent,persistent,!oldPersistentESet));
}
