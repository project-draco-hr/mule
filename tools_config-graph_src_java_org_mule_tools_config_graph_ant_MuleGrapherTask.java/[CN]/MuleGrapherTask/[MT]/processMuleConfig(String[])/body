{
  GraphConfig config=new GraphConfig();
  for (int i=0; i < srcFiles.length; i++) {
    config.getFiles().add(srcFiles[i]);
  }
  config.setOutputDirectory(outputdir);
  MuleGrapher grapher=new MuleGrapher(config);
  grapher.run();
  try {
    generatedFiles.add(srcFiles);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
