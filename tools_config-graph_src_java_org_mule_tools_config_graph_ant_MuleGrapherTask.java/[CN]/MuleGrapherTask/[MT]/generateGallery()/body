{
  FileOutputStream out;
  PrintStream p;
  try {
    out=new FileOutputStream(outputdir.getAbsoluteFile() + File.separator + "index.html");
    p=new PrintStream(out);
    p.println("<html> <body>");
    for (Iterator iter=generatedFiles.iterator(); iter.hasNext(); ) {
      String file=(String)iter.next();
      p.println("<img src=\"" + file + ".gif\" alt=\""+ file+ "\" title=\""+ file+ "\""+ "\"/>");
      p.println("<br/>");
    }
    p.println("</body></html> ");
    p.close();
  }
 catch (  Exception e) {
    System.err.println("Error writing to file");
  }
}
