{
  log("Processing : " + srcFiles[j]);
  MuleGrapher grapher=new MuleGrapher(fs.getDir(getProject()) + File.separator + srcFiles[j]);
  try {
    grapher.generateGraph(outputdir.getAbsolutePath(),srcFiles[j]);
    generatedFiles.add(srcFiles[j]);
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
