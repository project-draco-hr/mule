{
  super.prepareBeanFactory(beanFactory);
  registerEditors(beanFactory);
  addBeanPostProcessors(beanFactory,new MuleContextPostProcessor(muleContext),new GlobalNamePostProcessor(),new PostRegistrationActionsPostProcessor(this,(MuleRegistryHelper)muleContext.getRegistry()),new DiscardedOptionalBeanPostProcessor(optionalObjectsController,(DefaultListableBeanFactory)beanFactory),new LifecycleStatePostProcessor(muleContext.getLifecycleManager().getState()));
  beanFactory.registerSingleton(OBJECT_MULE_CONTEXT,muleContext);
}
