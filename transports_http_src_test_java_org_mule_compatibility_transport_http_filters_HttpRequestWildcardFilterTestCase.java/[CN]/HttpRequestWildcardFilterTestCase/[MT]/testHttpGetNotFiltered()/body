{
  Map<String,Serializable> props=new HashMap<>();
  props.put(HttpConstants.METHOD_GET,"true");
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send(((InboundEndpoint)((Flow)muleContext.getRegistry().lookupObject("httpIn")).getMessageSource()).getAddress() + "/" + "mulerulez",TEST_HTTP_MESSAGE,props).getRight();
  assertEquals(TEST_HTTP_MESSAGE,getPayloadAsString(result));
}
