{
  SimpleService simpleService=new SimpleServiceBuilder().name("test-simple-service-full").inboundAddress("test://foo").inboundTransformers(new StringAppendTransformer("bar")).inboundResponseTransformers(new ObjectToByteArray(),new GZipCompressTransformer()).component(EchoComponent.class).type(Type.AUTO).exceptionStrategy(new DefaultServiceExceptionStrategy()).build(muleContext);
  assertEquals("test-simple-service-full",simpleService.getName());
  assertEquals(EchoComponent.class,((AbstractJavaComponent)simpleService.getComponent()).getObjectType());
}
