{
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics before destroying [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
  accessTokenPool.invalidateObject(userId,connector);
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics after destroying [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
}
