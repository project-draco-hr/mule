{
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics before acquiring [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
  OAuth2Adapter object=((OAuth2Adapter)accessTokenPool.borrowObject(userId));
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics after acquiring [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
  return object;
}
