{
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics before acquiring [key %s] [active=%d] [idle=%d]",accessTokenId,accessTokenPool.getNumActive(accessTokenId),accessTokenPool.getNumIdle(accessTokenId)));
  }
  OAuth2Adapter object=accessTokenPool.borrowObject(accessTokenId);
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics after acquiring [key %s] [active=%d] [idle=%d]",accessTokenId,accessTokenPool.getNumActive(accessTokenId),accessTokenPool.getNumIdle(accessTokenId)));
  }
  return object;
}
