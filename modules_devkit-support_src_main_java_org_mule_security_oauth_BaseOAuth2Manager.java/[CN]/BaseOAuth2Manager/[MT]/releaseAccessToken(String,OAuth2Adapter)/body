{
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics before releasing [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
  accessTokenPool.returnObject(userId,connector);
  if (getLogger().isDebugEnabled()) {
    getLogger().debug(String.format("Pool Statistics after releasing [key %s] [active=%d] [idle=%d]",userId,accessTokenPool.getNumActive(userId),accessTokenPool.getNumIdle(userId)));
  }
}
