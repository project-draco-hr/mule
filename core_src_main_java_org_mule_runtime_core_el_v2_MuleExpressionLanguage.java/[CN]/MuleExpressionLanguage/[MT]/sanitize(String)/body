{
  String sanitizedExpression=expression.startsWith(DEFAULT_EXPRESSION_PREFIX) ? expression.substring(DEFAULT_EXPRESSION_PREFIX.length(),expression.length() - DEFAULT_EXPRESSION_POSTFIX.length()) : expression;
  if (sanitizedExpression.startsWith(DW_PREFIX)) {
    sanitizedExpression=sanitizedExpression.substring(DW_PREFIX.length());
  }
  return sanitizedExpression;
}
