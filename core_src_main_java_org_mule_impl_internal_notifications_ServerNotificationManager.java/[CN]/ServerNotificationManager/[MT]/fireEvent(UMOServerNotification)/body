{
  if (disposed) {
    return;
  }
  if (notification instanceof BlockingServerEvent) {
    this.notifyListeners(notification);
  }
 else {
    try {
      eventQueue.put(notification);
    }
 catch (    InterruptedException e) {
      if (!disposed) {
        logger.error("Failed to queue notification: " + notification,e);
      }
    }
  }
}
