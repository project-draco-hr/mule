{
  JdbcConnector jdbcConnector=(JdbcConnector)endpoint.getConnector();
  String statement=jdbcConnector.getStatement(endpoint);
  Object payload=event.getMessage().getPayload();
  SqlStatementStrategy strategy=jdbcConnector.getSqlStatementStrategyFactory().create(statement,payload);
  return strategy.executeStatement(jdbcConnector,endpoint,event,event.getTimeout(),(Connection)connector.getTransactionalResource(endpoint));
}
