{
  Class clazz=findLoadedClass(name);
  if (clazz == null) {
    if (this.parentFirst) {
      try {
        clazz=getParent().loadClass(name);
      }
 catch (      ClassNotFoundException cnfe) {
        clazz=findClass(name);
      }
    }
 else {
      try {
        clazz=findClass(name);
      }
 catch (      ClassNotFoundException e) {
        clazz=getParent().loadClass(name);
      }
    }
  }
  if (resolve) {
    resolveClass(clazz);
  }
  return clazz;
}
