{
  SystemStreamConnector c=(SystemStreamConnector)managementContext.getRegistry().lookupConnector("noBundleConnector");
  assertNotNull(c);
  assertEquals(1234,c.getMessageDelayTime());
  assertEquals("abc",c.getOutputMessage());
  assertEquals("bcd",c.getPromptMessage());
  assertTrue(c.isConnected());
  assertTrue(c.isStarted());
}
