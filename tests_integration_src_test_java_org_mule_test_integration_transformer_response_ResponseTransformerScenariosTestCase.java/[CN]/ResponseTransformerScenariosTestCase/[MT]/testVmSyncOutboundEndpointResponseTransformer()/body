{
  MuleClient client=muleContext.getClient();
  MuleMessage message=client.send("vm://syncOutboundEndpointResponseTransformer","request",null);
  assertNotNull(message);
  assertEquals("request" + VM_OUTBOUND + VM_INBOUND+ VM_OUT_IN_RESP+ CUSTOM_RESPONSE+ VM_RESPONSE,message.getPayloadAsString());
}
