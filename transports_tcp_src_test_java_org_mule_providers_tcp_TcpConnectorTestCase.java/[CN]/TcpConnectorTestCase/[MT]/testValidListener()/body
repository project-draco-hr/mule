{
  UMOComponent component=getTestComponent("orange",Orange.class);
  UMOEndpoint endpoint=(UMOEndpoint)muleContext.getRegistry().lookupEndpointFactory().getInboundEndpoint(getTestEndpointURI());
  UMOConnector connector=getConnector();
  try {
    endpoint.setEndpointURI(null);
    fail("endpointUri cannot be null");
  }
 catch (  Exception e) {
  }
  endpoint.setEndpointURI(new MuleEndpointURI(getTestEndpointURI()));
  connector.registerListener(component,endpoint);
}
