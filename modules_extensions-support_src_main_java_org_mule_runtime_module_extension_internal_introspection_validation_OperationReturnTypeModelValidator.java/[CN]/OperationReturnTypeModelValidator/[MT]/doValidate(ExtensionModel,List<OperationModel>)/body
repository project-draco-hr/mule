{
  for (  OperationModel operationModel : operations) {
    if (operationModel.getOutput() == null || operationModel.getOutput().getType() == null) {
      throw missingReturnTypeException(extensionModel,operationModel);
    }
    MetadataType returnType=operationModel.getOutput().getType();
    if (returnType.getMetadataFormat().equals(JAVA) && MuleEvent.class.isAssignableFrom(getType(returnType))) {
      throw new IllegalOperationModelDefinitionException(String.format("Operation '%s' in Extension '%s' specifies '%s' as a return type. Operations are " + "not allowed to return objects of that type",operationModel.getName(),extensionModel.getName(),MuleEvent.class.getName()));
    }
  }
}
