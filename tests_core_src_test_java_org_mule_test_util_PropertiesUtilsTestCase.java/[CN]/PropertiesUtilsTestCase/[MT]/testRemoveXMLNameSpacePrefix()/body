{
  String temp="j:namespace";
  String result=PropertiesUtils.removeXmlNamespacePrefix(temp);
  assertEquals("namespace",result);
  temp="this-namespace";
  result=PropertiesUtils.removeNamespacePrefix(temp);
  assertEquals("this-namespace",result);
  temp="namespace";
  result=PropertiesUtils.removeNamespacePrefix(temp);
  assertEquals("namespace",result);
}
