{
  Flow flow=MuleTestUtils.getTestFlow(muleContext);
  flow.setProcessingStrategy(new NonBlockingProcessingStrategy());
  return Event.builder(DefaultEventContext.create(flow,TEST_CONNECTOR)).message(InternalMessage.builder().payload(TEST_MESSAGE).build()).exchangePattern(REQUEST_RESPONSE).replyToHandler(new SensingNullReplyToHandler()).flow(flow).build();
}
