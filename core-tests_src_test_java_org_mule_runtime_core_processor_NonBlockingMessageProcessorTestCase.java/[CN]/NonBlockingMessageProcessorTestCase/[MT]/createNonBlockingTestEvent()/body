{
  Flow flow=MuleTestUtils.getTestFlow(muleContext);
  flow.setProcessingStrategy(new NonBlockingProcessingStrategy());
  return new DefaultMuleEvent(DefaultMessageContext.create(flow,TEST_CONNECTOR),MuleMessage.builder().payload(TEST_MESSAGE).build(),MessageExchangePattern.REQUEST_RESPONSE,new SensingNullReplyToHandler(),flow);
