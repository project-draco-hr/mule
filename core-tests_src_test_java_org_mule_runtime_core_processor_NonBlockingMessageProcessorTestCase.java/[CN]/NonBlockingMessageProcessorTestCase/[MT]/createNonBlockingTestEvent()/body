{
  Flow flow=MuleTestUtils.getTestFlow(muleContext);
  flow.setProcessingStrategy(new NonBlockingProcessingStrategy());
  return MuleEvent.builder(DefaultMessageContext.create(flow,TEST_CONNECTOR)).message(MuleMessage.builder().payload(TEST_MESSAGE).build()).exchangePattern(REQUEST_RESPONSE).replyToHandler(new SensingNullReplyToHandler()).flow(flow).build();
}
