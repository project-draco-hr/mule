{
  try {
    String xml=(String)src;
    org.dom4j.Document dom4jDoc=DocumentHelper.parseText(xml);
    if (org.dom4j.Document.class.equals(getReturnClass())) {
      return dom4jDoc;
    }
 else {
      return new DOMWriter().write(dom4jDoc);
    }
  }
 catch (  Exception e) {
    throw new TransformerException(this,e);
  }
}
