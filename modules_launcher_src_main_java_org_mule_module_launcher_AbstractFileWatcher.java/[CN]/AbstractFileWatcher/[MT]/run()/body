{
  long lastTimeStamp=timeStamp;
  File latestFile=null;
  for (  File file : files) {
    long timestamp=file.lastModified();
    if (timestamp > lastTimeStamp) {
      lastTimeStamp=timeStamp;
      latestFile=file;
    }
  }
  if (latestFile != null) {
    this.timeStamp=lastTimeStamp;
    onChange(latestFile);
  }
}
