{
  String realmHeader="Basic realm=";
  if (realm != null) {
    realmHeader+="\"" + realm + "\"";
  }
  MuleMessage msg=event.getMessage();
  msg.setProperty(HttpConstants.HEADER_WWW_AUTHENTICATE,realmHeader,PropertyScope.OUTBOUND);
  msg.setIntProperty(HttpConnector.HTTP_STATUS_PROPERTY,HttpConstants.SC_UNAUTHORIZED);
}
