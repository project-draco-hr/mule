{
  Object temp;
  try {
    temp=connector.lookupFromJndi(name);
  }
 catch (  NamingException e) {
    String message=MessageFormat.format("Failed to look up destination {0}. Reason: {1}",new Object[]{name,e.getMessage()});
    throw new JMSException(message);
  }
  if (temp != null) {
    if (temp instanceof Destination) {
      return (Destination)temp;
    }
  }
  return null;
}
