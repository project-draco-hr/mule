{
  HttpResponse response=new HttpResponse();
  response.setStatusLine(version,statusCode);
  response.setBody(description);
  DefaultMuleEvent event=new DefaultMuleEvent(new DefaultMuleMessage(response,connector.getMuleContext()),(InboundEndpoint)endpoint,new DefaultMuleSession(flowConstruct));
  RequestContext.setEvent(event);
  return transformResponse(response,event);
}
