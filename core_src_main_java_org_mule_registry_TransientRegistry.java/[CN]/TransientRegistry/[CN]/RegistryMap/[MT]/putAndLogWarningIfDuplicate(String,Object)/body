{
  Lock writeLock=registryLock.writeLock();
  try {
    writeLock.lock();
    if (registry.containsKey(key)) {
      logger.warn("TransientRegistry already contains an object named '" + key + "'.  The previous object will be overwritten.");
    }
    registry.put(key,object);
  }
  finally {
    writeLock.unlock();
  }
}
