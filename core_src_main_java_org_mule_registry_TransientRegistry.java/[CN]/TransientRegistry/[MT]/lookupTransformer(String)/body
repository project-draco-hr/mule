{
  Transformer transformer=super.lookupTransformer(name);
  if (transformer != null) {
    try {
      if (transformer.getEndpoint() != null) {
        throw new IllegalStateException("Endpoint cannot be set");
      }
      transformer=(Transformer)BeanUtils.cloneBean(transformer);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  return transformer;
}
