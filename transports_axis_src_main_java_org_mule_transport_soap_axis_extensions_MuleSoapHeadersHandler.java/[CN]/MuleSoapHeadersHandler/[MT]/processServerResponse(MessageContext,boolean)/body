{
  SOAPMessage msg=msgContext.getMessage();
  if (msg == null) {
    return;
  }
  MuleSoapHeaders headers=(MuleSoapHeaders)msgContext.getProperty(MuleSoapHeaders.ENV_REQUEST_HEADERS);
  if (headers == null) {
    return;
  }
 else {
    headers.addHeaders(msgContext.getMessage().getSOAPPart().getEnvelope());
  }
}
