{
  MuleEvent event=null;
  try {
    event=routeMessage(message);
    return event == null ? null : event.getMessage();
  }
 catch (  Exception e) {
    if (event == null) {
      event=createMuleEvent(message,null);
    }
    return flowConstruct.getExceptionListener().handleException(e,event).getMessage();
  }
}
