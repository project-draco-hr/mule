{
  try {
    execute();
    IOUtils.closeQuietly(getInputStream().getOut());
    while (!isClosed) {
      try {
        this.wait();
      }
 catch (      InterruptedException e) {
      }
    }
  }
 catch (  Exception e) {
    logger.error(e.getMessage(),e);
    try {
      IOUtils.write(e.getMessage().toCharArray(),getInputStream().getOut());
    }
 catch (    IOException exp) {
    }
  }
}
