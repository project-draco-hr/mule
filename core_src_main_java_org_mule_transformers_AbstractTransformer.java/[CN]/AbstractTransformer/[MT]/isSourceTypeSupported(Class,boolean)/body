{
  if (sourceTypes.isEmpty()) {
    return !exactMatch;
  }
  Class anotherClass;
  for (Iterator i=getSourceTypeClassesIterator(); i.hasNext(); ) {
    anotherClass=(Class)i.next();
    if (exactMatch) {
      if (anotherClass.equals(aClass)) {
        return true;
      }
    }
 else     if (anotherClass.isAssignableFrom(aClass)) {
      return true;
    }
  }
  return false;
}
