{
  List list=new ArrayList(threads_.values());
  Thread thread;
  for (int i=0; i < list.size(); i++) {
    thread=(Thread)list.get(i);
    if (thread instanceof Disposable) {
      try {
        ((Disposable)thread).dispose();
      }
 catch (      UMOException e) {
      }
    }
 else {
      thread.interrupt();
    }
  }
  super.shutdownNow();
}
