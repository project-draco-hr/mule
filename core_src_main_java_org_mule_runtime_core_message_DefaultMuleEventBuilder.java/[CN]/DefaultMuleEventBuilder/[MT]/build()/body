{
  if (originalEvent != null && !modified) {
    return originalEvent;
  }
 else {
    DefaultMuleEvent event=new DefaultMuleEvent(context,message,flowVariables,exchangePattern,flow,session,transacted,synchronous == null ? (resolveEventSynchronicity() && replyToHandler == null) : synchronous,nonBlocking || isFlowConstructNonBlockingProcessingStrategy(),replyToDestination,replyToHandler,flowCallStack,correlation,error,legacyCorrelationId,notificationsEnabled);
    return event;
  }
}
