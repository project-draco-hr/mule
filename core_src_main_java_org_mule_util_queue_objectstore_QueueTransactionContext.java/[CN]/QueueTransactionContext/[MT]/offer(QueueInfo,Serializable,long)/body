{
  readOnly=false;
  if (queue.canTakeFromStore()) {
    queue.writeToObjectStore(item);
    return true;
  }
  initializeAdded();
  List<Serializable> queueAdded=lookupAddedQueue(queue);
  if (queue.offer(null,queueAdded.size(),offerTimeout)) {
    queueAdded.add(item);
    return true;
  }
 else {
    return false;
  }
}
