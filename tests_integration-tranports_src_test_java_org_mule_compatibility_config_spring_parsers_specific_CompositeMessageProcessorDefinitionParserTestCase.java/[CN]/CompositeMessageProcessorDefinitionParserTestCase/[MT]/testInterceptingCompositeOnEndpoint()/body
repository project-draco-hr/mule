{
  EndpointBuilder endpointBuilder=lookupEndpointBuilder(muleContext.getRegistry(),"endpoint");
  InboundEndpoint endpoint=endpointBuilder.buildInboundEndpoint();
  assertEquals(2,endpoint.getMessageProcessors().size());
  MessageProcessor endpointProcessor=endpoint.getMessageProcessorsFactory().createInboundMessageProcessorChain(endpoint,getTestFlow(),new NullMessageProcessor());
  assertEquals("01231abc2",endpointProcessor.process(getTestEvent("0")).getMessageAsString(muleContext));
}
