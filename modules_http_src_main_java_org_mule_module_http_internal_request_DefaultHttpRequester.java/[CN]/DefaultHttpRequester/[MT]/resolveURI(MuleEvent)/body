{
  if (address != null) {
    return new URI(address.resolveStringValue(muleEvent));
  }
 else {
    return new URI(requestConfig.getScheme(),null,host.resolveStringValue(muleEvent),port.resolveIntegerValue(muleEvent),replaceUriParams(buildPath(basePath.resolveStringValue(muleEvent),path.resolveStringValue(muleEvent)),muleEvent),null,null);
  }
}
