{
  if (url != null) {
    return url.resolveStringValue(muleEvent);
  }
 else {
    String resolvedPath=replaceUriParams(buildPath(basePath.resolveStringValue(muleEvent),path.resolveStringValue(muleEvent)),muleEvent);
    resolvedPath=HttpParser.encodeSpaces(resolvedPath);
    return String.format("%s://%s:%s%s",requestConfig.getScheme(),host.resolveStringValue(muleEvent),port.resolveIntegerValue(muleEvent),resolvedPath);
  }
}
