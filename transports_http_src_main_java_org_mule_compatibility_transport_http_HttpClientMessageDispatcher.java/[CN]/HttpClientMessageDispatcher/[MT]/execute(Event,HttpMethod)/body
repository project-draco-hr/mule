{
  try {
    URI uri=endpoint.getEndpointURI().getUri();
    this.processCookies(event);
    this.processMuleSession(event,httpMethod);
    client.executeMethod(getHostConfig(uri),httpMethod);
    return httpMethod;
  }
 catch (  IOException e) {
    throw new DispatchException(getEndpoint(),e);
  }
catch (  Exception e) {
    throw new DispatchException(getEndpoint(),e);
  }
}
