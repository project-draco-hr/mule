{
  executeDdl(dataSource,"DROP PROCEDURE IF EXISTS multiplyInts;\n");
  final String sql="CREATE DEFINER=CURRENT_USER PROCEDURE multiplyInts(IN pInt1 INT, IN pInt2 INT, OUT pResult1 INT, IN pInt3 INT, OUT pResult2 INT)\n" + "BEGIN\n" + "SET pResult1 := pInt1 * pInt2;\n"+ "SET pResult2 := pInt1 * pInt2 * pInt3;\n"+ "END";
  createStoredProcedure(dataSource,sql);
}
