{
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Starting component");
    }
    doStart();
    if (logger.isDebugEnabled()) {
      logger.debug("Component started");
    }
  }
 catch (  JBIException e) {
    throw e;
  }
catch (  Exception e) {
    throw new JBIException("Error calling init",e);
  }
}
