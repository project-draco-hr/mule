{
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Initializing component");
    }
    this.context=context;
    doInit();
    if (logger.isDebugEnabled()) {
      logger.debug("Component initialized");
    }
  }
 catch (  JBIException e) {
    throw e;
  }
catch (  Exception e) {
    throw new JBIException("Error calling init",e);
  }
}
