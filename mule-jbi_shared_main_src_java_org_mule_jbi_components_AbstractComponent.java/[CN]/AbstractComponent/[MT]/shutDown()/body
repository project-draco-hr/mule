{
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Shutting down component");
    }
    doShutDown();
    this.context=null;
    if (logger.isDebugEnabled()) {
      logger.debug("Component shut down");
    }
  }
 catch (  JBIException e) {
    throw e;
  }
catch (  Exception e) {
    throw new JBIException("Error calling init",e);
  }
}
