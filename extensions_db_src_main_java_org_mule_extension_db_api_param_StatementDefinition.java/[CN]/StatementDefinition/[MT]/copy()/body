{
  StatementDefinition copy;
  try {
    copy=getClass().newInstance();
  }
 catch (  Exception e) {
    throw new MuleRuntimeException(createStaticMessage("Could not create instance of " + getClass().getName()),e);
  }
  copy.sql=sql;
  copy.inputParameters=new LinkedList<>(inputParameters);
  return (T)copy;
}
