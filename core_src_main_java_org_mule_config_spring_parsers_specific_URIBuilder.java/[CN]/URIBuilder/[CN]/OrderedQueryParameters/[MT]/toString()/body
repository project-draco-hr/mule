{
  StringBuffer buffer=new StringBuffer();
  Iterator keys=orderedKeys.iterator();
  boolean first=true;
  while (keys.hasNext()) {
    if (first) {
      buffer.append(QUERY);
      first=false;
    }
 else {
      buffer.append(AND);
    }
    String key=(String)keys.next();
    buffer.append(key);
    String value=(String)values.get(key);
    if (null != value) {
      buffer.append(EQUALS);
      buffer.append(value);
    }
  }
  return buffer.toString();
}
