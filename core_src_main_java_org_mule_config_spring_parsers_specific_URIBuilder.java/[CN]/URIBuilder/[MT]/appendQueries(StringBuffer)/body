{
  if (null != queryMap) {
    boolean first=buffer.indexOf(QUERY) == -1;
    Iterator keys=new TreeSet(queryMap.keySet()).iterator();
    while (keys.hasNext()) {
      if (first) {
        buffer.append(QUERY);
        first=false;
      }
 else {
        buffer.append(AND);
      }
      String key=(String)keys.next();
      buffer.append(key);
      buffer.append(EQUALS);
      buffer.append((String)queryMap.get(key));
    }
  }
}
