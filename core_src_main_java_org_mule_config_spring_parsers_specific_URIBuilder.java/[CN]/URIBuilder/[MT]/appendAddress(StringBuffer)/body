{
  if (null != address) {
    int index=address.indexOf(QUERY);
    if (index > -1) {
      buffer.append(address.substring(0,index));
      return parseQueries(address.substring(index + 1));
    }
 else {
      buffer.append(address);
      return Collections.EMPTY_MAP;
    }
  }
 else {
    constructAddress(buffer);
    return Collections.EMPTY_MAP;
  }
}
