{
  managementContext.getRegistry().registerTransformer(new NoActionTransformer(),managementContext);
  UMOEndpoint endpoint=managementContext.getRegistry().getOrCreateEndpointForUri("test:///tmp?responseTransformers=NoActionTransformer",UMOEndpoint.ENDPOINT_TYPE_SENDER,managementContext);
  assertEquals("NoActionTransformer",endpoint.getResponseTransformer().getName());
}
