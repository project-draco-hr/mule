{
  cachedMessageProcessors=CacheBuilder.newBuilder().maximumSize(CACHE_SIZE).expireAfterWrite(EXPIRATION_TIME_IN_MINUTES,TimeUnit.MINUTES).build(new CacheLoader<HttpRequestCacheKey,MessageProcessor>(){
    public MessageProcessor load(    HttpRequestCacheKey cacheKey) throws MuleException {
      return buildMessageProcessor(cacheKey);
    }
  }
);
}
