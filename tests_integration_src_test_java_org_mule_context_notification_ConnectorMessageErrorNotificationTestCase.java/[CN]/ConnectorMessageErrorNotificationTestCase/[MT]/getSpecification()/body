{
  return new Node().parallel(new Node(ConnectorMessageNotification.class,MESSAGE_REQUEST_BEGIN,MULE_CLIENT_ID)).parallel(new Node(ConnectorMessageNotification.class,MESSAGE_REQUEST_END,MULE_CLIENT_ID)).parallel(new Node(ConnectorMessageNotification.class,MESSAGE_RECEIVED,FLOW_ID)).parallel(new Node(ConnectorMessageNotification.class,MESSAGE_ERROR_RESPONSE,FLOW_ID));
}
