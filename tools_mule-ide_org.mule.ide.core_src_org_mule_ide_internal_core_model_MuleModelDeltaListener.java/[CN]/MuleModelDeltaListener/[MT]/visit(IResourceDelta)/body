{
  IResource resource=delta.getResource();
switch (resource.getType()) {
case IResource.FILE:
{
switch (delta.getKind()) {
case IResourceDelta.CHANGED:
        if (resource.getFullPath().equals(getConfigFilePath())) {
          if ((delta.getFlags() & IResourceDelta.MARKERS) == 0) {
            configFileChanged();
          }
        }
    }
  }
}
return true;
}
