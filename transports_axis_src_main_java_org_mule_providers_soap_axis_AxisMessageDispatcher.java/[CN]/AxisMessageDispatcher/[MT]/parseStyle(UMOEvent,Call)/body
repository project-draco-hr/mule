{
  String style=event.getMessage().getStringProperty(AxisConnector.STYLE,null);
  if (style != null) {
    Style s=Style.getStyle(style);
    if (s == null) {
      throw new IllegalArgumentException(CoreMessages.valueIsInvalidFor(style,AxisConnector.STYLE).toString());
    }
 else {
      call.setOperationStyle(s);
    }
  }
}
