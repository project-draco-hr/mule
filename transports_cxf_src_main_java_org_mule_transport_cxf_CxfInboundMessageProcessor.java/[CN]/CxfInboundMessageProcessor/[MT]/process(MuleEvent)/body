{
  String requestPath=parseHttpRequestProperty(event.getMessage().getInboundProperty(HttpConnector.HTTP_REQUEST_PROPERTY,StringUtils.EMPTY));
  try {
    if (requestPath.indexOf('?') > -1) {
      return generateWSDLOrXSD(event,requestPath);
    }
 else {
      return sendToDestination(event);
    }
  }
 catch (  IOException e) {
    throw new DefaultMuleException(e);
  }
}
