{
  Component c=muleContext.getRegistry().lookupComponent("stoppedComponent");
  assertFalse(c.isStarted());
  try {
    c.dispatchEvent(getTestEvent("hello",c));
    fail();
  }
 catch (  ComponentException e) {
  }
  try {
    c.sendEvent(getTestEvent("hello",c));
    fail();
  }
 catch (  ComponentException e) {
  }
}
