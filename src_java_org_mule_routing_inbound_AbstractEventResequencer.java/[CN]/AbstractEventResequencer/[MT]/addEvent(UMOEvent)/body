{
  String cId=event.getMessage().getCorrelationId();
  if (cId == null) {
    cId=NO_CORRELATION_ID;
  }
  EventGroup eg=(EventGroup)eventGroups.get(cId);
  if (eg == null) {
    eg=new EventGroup(cId);
    eg.addEvent(event);
    eventGroups.put(eg.getGroupId(),eg);
  }
 else {
    eg.addEvent(event);
  }
  return eg;
}
