{
  if (props != null) {
    try {
      for (Iterator iterator=props.entrySet().iterator(); iterator.hasNext(); ) {
        Map.Entry e=(Map.Entry)iterator.next();
        registry.registerObject((String)e.getKey(),e.getValue(),MuleProperties.OBJECT_MULE_APPLICATION_PROPERTIES,managementContext);
      }
    }
 catch (    RegistrationException e) {
      logger.error(e);
    }
  }
}
