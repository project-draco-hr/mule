{
  try {
    Thread.sleep(STARTUP_DELAY);
    while (!connector.isDisposed() && !disposing.get()) {
      if (connector.isStarted()) {
        poll();
      }
      Thread.sleep(frequency);
    }
  }
 catch (  InterruptedException e) {
  }
catch (  Exception e) {
    connector.handleException(e);
  }
}
