{
  configureWireMockToExpectTokenPathRequestForClientCredentialsGrantTypeWithMapResponse(NEW_ACCESS_TOKEN);
  wireMockRule.stubFor(post(urlEqualTo(RESOURCE_PATH)).withHeader(HttpHeaders.Names.AUTHORIZATION,containing(ACCESS_TOKEN)).willReturn(aResponse().withStatus(500).withHeader(HttpHeaders.Names.WWW_AUTHENTICATE,"Basic realm=\"myRealm\"")));
  wireMockRule.stubFor(post(urlEqualTo(RESOURCE_PATH)).withHeader(HttpHeaders.Names.AUTHORIZATION,containing(NEW_ACCESS_TOKEN)).willReturn(aResponse().withBody(TEST_MESSAGE).withStatus(200)));
  Flow testFlow=(Flow)getFlowConstruct("testFlow");
  testFlow.process(getTestEvent(TEST_MESSAGE));
  verifyRequestDoneToTokenUrlForClientCredentials();
  wireMockRule.verify(postRequestedFor(urlEqualTo(RESOURCE_PATH)).withHeader(HttpHeaders.Names.AUTHORIZATION,equalTo(buildAuthorizationHeaderContent(NEW_ACCESS_TOKEN))));
}
