{
  flowRunner("requestFlow").withPayload(TEST_MESSAGE).withOutboundAttachment("attachment1","Contents 1",TEXT).withOutboundAttachment("attachment2","Contents 2",HTML).withOutboundProperty("Content-Type","multipart/form-data2").run();
  assertThat(requestContentType,startsWith("multipart/form-data2; boundary="));
  assertThat(parts.size(),equalTo(2));
  assertPart("attachment1",TEXT,"Contents 1");
  assertPart("attachment2",HTML,"Contents 2");
}
