{
  return new IdentifiableDynamicRouteResolver(){
    @Override public String getRouteIdentifier(    Event event) throws MessagingException {
      return event.getVariable(ID_PROPERTY_NAME);
    }
    @Override public List<Processor> resolveRoutes(    Event event) throws MessagingException {
      return getMessageProcessorsList();
    }
  }
;
}
