{
  return new IdentifiableDynamicRouteResolver(){
    @Override public String getRouteIdentifier(    MuleEvent event) throws MessagingException {
      return event.getFlowVariable(ID_PROPERTY_NAME);
    }
    @Override public List<MessageProcessor> resolveRoutes(    MuleEvent event) throws MessagingException {
      return getMessageProcessorsList();
    }
  }
;
}
