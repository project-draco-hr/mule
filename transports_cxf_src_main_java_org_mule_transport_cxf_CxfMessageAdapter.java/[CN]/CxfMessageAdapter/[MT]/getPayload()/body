{
  List<Object> objs=CastUtils.cast(payload.getContent(List.class));
  if (objs == null) {
    Object o=payload.getContent(Object.class);
    if (o != null) {
      return o;
    }
 else {
      return new Object[0];
    }
  }
  if (objs.size() == 1 && objs.get(0) != null) {
    return objs.get(0);
  }
 else {
    return objs.toArray();
  }
}
