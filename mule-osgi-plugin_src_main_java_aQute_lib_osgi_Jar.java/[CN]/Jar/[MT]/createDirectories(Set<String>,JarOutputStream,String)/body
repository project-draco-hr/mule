{
  int index=name.lastIndexOf('/');
  if (index > 0) {
    String path=name.substring(0,index);
    if (directories.contains(path))     return;
    createDirectories(directories,zip,path);
    ZipEntry ze=new ZipEntry(path + '/');
    zip.putNextEntry(ze);
    zip.closeEntry();
    directories.add(path);
  }
}
