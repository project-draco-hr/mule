{
  int index=1;
  Node parentNode=element.getParentNode();
  NodeList childNodes=parentNode.getChildNodes();
  int itemIndex=0;
  while (itemIndex < childNodes.getLength()) {
    Node item=childNodes.item(itemIndex++);
    if (item == element) {
      break;
    }
    if (item instanceof Element) {
      index++;
    }
  }
  return index;
}
