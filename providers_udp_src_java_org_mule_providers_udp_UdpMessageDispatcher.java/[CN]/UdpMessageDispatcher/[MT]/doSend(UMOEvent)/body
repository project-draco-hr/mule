{
  doDispatch(event);
  if (event.getEndpoint().isRemoteSync()) {
    DatagramPacket result=receive(socket,event.getTimeout());
    if (result == null) {
      return null;
    }
    UMOMessage message=new MuleMessage(connector.getMessageAdapter(result));
    return message;
  }
 else {
    return event.getMessage();
  }
}
