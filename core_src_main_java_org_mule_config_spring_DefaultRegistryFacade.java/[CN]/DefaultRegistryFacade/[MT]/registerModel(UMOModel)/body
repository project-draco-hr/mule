{
  if (models.get(model.getName()) != null) {
    throw new MuleException(new Message(Messages.CONTAINER_X_ALREADY_REGISTERED,"model:" + model.getName()));
  }
  models.put(model.getName(),model);
  if (initialised.get()) {
    model.initialise(managementContext);
  }
  if (started.get()) {
    model.start();
  }
}
