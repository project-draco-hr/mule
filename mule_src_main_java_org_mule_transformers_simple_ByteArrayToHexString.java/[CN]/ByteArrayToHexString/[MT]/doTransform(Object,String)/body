{
  byte[] bytes=(byte[])src;
  if (src == null || bytes.length == 0) {
    return "";
  }
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < bytes.length; i++) {
    int val=bytes[i] & 0x0ff;
    if (val < 16)     sb.append("0");
    sb.append(Integer.toHexString(val));
  }
  return sb.toString();
}
