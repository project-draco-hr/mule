{
  checkPhase(phase);
  int current=getPhaseIndex(currentPhase);
  int end=getPhaseIndex(phase);
  LifecyclePhase li;
  if (end < current) {
    li=getPhaseForIndex(end);
    invokePhase(li);
    return;
  }
  current++;
  while (current <= end) {
    li=getPhaseForIndex(current);
    invokePhase(li);
    current++;
  }
}
