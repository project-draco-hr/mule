{
  super(protocol,new ChildEndpointDefinitionParser(endpoint),properties);
  registerPreProcessor(new CheckExclusiveAttributes(new String[][]{new String[]{AbstractMuleBeanDefinitionParser.ATTRIBUTE_REF},LazyEndpointURI.ATTRIBUTES}));
  registerPreProcessor(new CheckExclusiveAttributes(new String[][]{new String[]{AbstractMuleBeanDefinitionParser.ATTRIBUTE_REF},properties}));
}
