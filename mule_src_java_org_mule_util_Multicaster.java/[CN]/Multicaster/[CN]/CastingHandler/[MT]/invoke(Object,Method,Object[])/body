{
  List results=new ArrayList();
  Object item=null;
  Object result;
  for (Iterator iterator=objects.iterator(); iterator.hasNext(); ) {
    try {
      item=iterator.next();
      result=method.invoke(item,args);
      if (listener != null) {
        listener.afterExecute(item,method,args);
      }
      if (result != null) {
        results.add(result);
      }
    }
 catch (    Throwable t) {
      if (listener != null) {
        t=listener.onException(item,method,args,t);
        if (t != null) {
          throw t;
        }
      }
    }
  }
  return results;
}
