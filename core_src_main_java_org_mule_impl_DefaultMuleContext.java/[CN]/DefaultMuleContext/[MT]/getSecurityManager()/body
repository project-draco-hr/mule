{
  UMOSecurityManager securityManager=(UMOSecurityManager)getRegistry().lookupObject(MuleProperties.OBJECT_SECURITY_MANAGER);
  if (securityManager == null) {
    Collection temp=getRegistry().lookupObjects(UMOSecurityManager.class);
    if (temp.size() > 0) {
      securityManager=((UMOSecurityManager)temp.iterator().next());
    }
  }
  return securityManager;
}
