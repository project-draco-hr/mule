{
  File[] files=storeDirectory.listFiles();
  if (files == null) {
    return new ArrayList<String>();
  }
  Arrays.sort(files);
  List<String> partitions=new ArrayList<String>();
  for (int i=0; i < files.length; i++) {
    if (files[i].isDirectory()) {
      partitions.add(files[i].getName());
    }
  }
  return partitions;
}
