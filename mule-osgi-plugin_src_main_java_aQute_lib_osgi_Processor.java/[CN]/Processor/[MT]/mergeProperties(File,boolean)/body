{
  if (file.isFile()) {
    try {
      Properties properties=loadProperties(file);
      mergeProperties(properties,override);
    }
 catch (    Exception e) {
      error("Error loading properties file: " + file);
    }
  }
 else {
    if (!file.exists())     error("Properties file does not exist: " + file);
 else     error("Properties file must a file, not a directory: " + file);
  }
}
