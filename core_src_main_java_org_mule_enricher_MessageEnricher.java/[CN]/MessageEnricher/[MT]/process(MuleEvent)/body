{
  ExpressionManager expressionManager=event.getMuleContext().getExpressionManager();
  MuleMessage enrichmentMessage=enrichmentProcessor.process(RequestContext.cloneAndUpdateEventEndpoint(event,enrichmentProcessor)).getMessage();
  if (enrichmentMessage != null) {
    for (    EnrichExpressionPair pair : enrichExpressionPairs) {
      enrich(event.getMessage(),enrichmentMessage,pair.getSource(),pair.getTarget(),expressionManager);
    }
  }
  return event;
}
