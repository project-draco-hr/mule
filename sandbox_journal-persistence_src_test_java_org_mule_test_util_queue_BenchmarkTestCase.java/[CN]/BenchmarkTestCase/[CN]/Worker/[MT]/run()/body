{
  Random rnd=new Random();
  try {
    QueueSession s=mgr.getQueueSession();
    Queue q=s.getQueue(queue);
    for (int i=0; i < OUTERLOOP; i++) {
      for (int j=0; j < INNERLOOP; j++) {
        byte[] o=new byte[(rnd.nextInt(16) + 1) * OBJSIZE];
        q.put(o);
      }
      while (q.size() > 0) {
        q.take();
      }
    }
  }
 catch (  InterruptedException e) {
    e.printStackTrace();
  }
}
