{
  super.addAlias("endpoint","endpointBuilder");
  super.registerPreProcessor(new CheckExclusiveAttributes(new String[][]{new String[]{ADDRESS_ATTRIBUTE},new String[]{ENDPOINT_REF_ATTRIBUTE}}));
  super.registerPreProcessor(new CheckExclusiveAttributes(new String[][]{new String[]{COMPONENT_CLASS_ATTRIBUTE},new String[]{COMPONENT_REF_ATTRIBUTE}}));
  super.registerPreProcessor(new CheckExclusiveAttributesAndChildren(new String[]{ENDPOINT_REF_ATTRIBUTE,ADDRESS_ATTRIBUTE,TRANSFORMER_REFS_ATTRIBUTE,RESPONSE_TRANSFORMER_REFS_ATTRIBUTE},new String[]{INBOUND_ENDPOINT_CHILD}));
  super.registerPreProcessor(new CheckExclusiveAttributesAndChildren(new String[]{ABSTRACT_ATTRIBUTE},new String[]{COMPONENT_CHILD_TYPE}));
}
