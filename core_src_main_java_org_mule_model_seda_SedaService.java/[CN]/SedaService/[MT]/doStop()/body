{
  if (muleContext.getQueueManager().getQueueSession().getQueue(name).size() > 0) {
    try {
      stopping.whenFalse(null);
    }
 catch (    InterruptedException e) {
    }
  }
  workManager.dispose();
  if (componentFactory instanceof Stoppable) {
    ((Stoppable)componentFactory).stop();
  }
}
