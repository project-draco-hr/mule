{
  if (logger.isDebugEnabled()) {
    logger.debug(MessageFormat.format("Service: {0} has received asynchronous event on: {1}",name,event.getEndpoint().getEndpointURI()));
  }
  try {
    if (stats.isEnabled()) {
      enqueue(event);
synchronized (queueStatsGuard) {
        stats.incQueuedEvent();
      }
    }
 else {
      enqueue(event);
    }
  }
 catch (  Exception e) {
    FailedToQueueEventException e1=new FailedToQueueEventException(CoreMessages.interruptedQueuingEventFor(this.getName()),event.getMessage(),this,e);
    handleException(e1);
  }
  if (logger.isTraceEnabled()) {
    logger.trace("MuleEvent added to queue for: " + name);
  }
}
