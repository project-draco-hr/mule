{
  Connector conn=getConnector();
  initExchangePatternFromConnectorDefault(conn);
  if (conn.getOutboundExchangePatterns().contains(messageExchangePattern) == false) {
    throw new EndpointException(CoreMessages.exchangePatternForEndpointNotSupported(messageExchangePattern,"outbound",uriBuilder.getEndpoint()));
  }
}
