{
  if (mimeType == null) {
    this.mimeType=null;
  }
 else {
    MimeType mt=null;
    try {
      mt=new MimeType(mimeType);
    }
 catch (    MimeTypeParseException e) {
      throw new IllegalArgumentException(e.getMessage(),e);
    }
    this.mimeType=mt.getPrimaryType() + "/" + mt.getSubType();
  }
}
