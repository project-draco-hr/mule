{
  Connector conn=getConnector();
  initExchangePatternFromConnectorDefault(conn);
  if (conn.getInboundExchangePatterns().contains(messageExchangePattern) == false) {
    throw new EndpointException(CoreMessages.exchangePatternForEndpointNotSupported(messageExchangePattern,"inbound",uriBuilder.getEndpoint()));
  }
}
