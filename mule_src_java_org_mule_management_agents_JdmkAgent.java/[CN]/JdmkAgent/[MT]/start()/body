{
  try {
    mBeanServer.invoke(adaptorName,"start",null,null);
  }
 catch (  InstanceNotFoundException e) {
    throw new JmxManagementException("failed to start agent",e,adaptorName);
  }
catch (  MBeanException e) {
    throw new JmxManagementException("failed to start agent",e,adaptorName);
  }
catch (  ReflectionException e) {
  }
}
