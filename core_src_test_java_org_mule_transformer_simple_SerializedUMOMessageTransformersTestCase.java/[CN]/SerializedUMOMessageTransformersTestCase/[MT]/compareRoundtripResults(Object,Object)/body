{
  if (src == null && result == null) {
    return true;
  }
  if (src == null || result == null) {
    return false;
  }
  if (src instanceof MuleMessage && result instanceof MuleMessage) {
    return ((MuleMessage)src).getPayload().equals(((MuleMessage)result).getPayload()) && ((MuleMessage)src).getProperty("object").equals(((MuleMessage)result).getProperty("object")) && ((MuleMessage)src).getProperty("string").equals(((MuleMessage)result).getProperty("string"))&& ((MuleMessage)src).getIntProperty("number",-1) == ((MuleMessage)result).getIntProperty("number",-2);
  }
 else {
    return false;
  }
}
