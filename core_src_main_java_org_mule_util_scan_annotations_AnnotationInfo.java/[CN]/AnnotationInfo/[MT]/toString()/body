{
  StringBuilder sb=new StringBuilder(params.size() * 20);
  sb.append(className).append('(');
  for (int i=0; i < params.size(); i++) {
    NameValue param=params.get(i);
    sb.append(param.name).append('=').append(param.value);
    if (i < params.size() - 1) {
      sb.append(',');
    }
 else {
      sb.append(')');
    }
  }
  return sb.toString();
}
