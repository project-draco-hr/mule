{
  cachedMessageProcessors=CacheBuilder.newBuilder().maximumSize(CACHE_SIZE).expireAfterWrite(EXPIRATION_TIME_IN_MINUTES,TimeUnit.MINUTES).build(new CacheLoader<RequestCacheKey,MessageProcessor>(){
    @Override public MessageProcessor load(    RequestCacheKey cacheKey) throws MuleException {
      return buildMessageProcessor(cacheKey);
    }
  }
);
}
