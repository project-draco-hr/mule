{
  MuleClient client=muleContext.getClient();
  MuleMessage message=client.send("vm://chainedRouterOutboundEndpointResponseTransformer","request",null).getRight();
  assertThat(message,notNullValue());
  assertThat(getPayloadAsString(message),is(equalTo("request" + VM_OUTBOUND + VM_INBOUND+ VM_OUT_IN_RESP+ VM_OUT_IN_RESP+ CUSTOM_RESPONSE+ CUSTOM_RESPONSE+ VM_RESPONSE)));
}
