{
  ConnectionHandler<?,?> connectionHandler;
  try {
    connectionHandler=ClassUtils.instanciateClass(connectorAnnotation.value());
  }
 catch (  Exception e) {
    throw new MuleRuntimeException(createStaticMessage(format("Could not instantiate ConnectionHandler of type '%s' for configuration of type '%s'",connectorAnnotation.value().getName(),extractExtensionType(declaration).getName())),e);
  }
  return connectionHandler;
}
