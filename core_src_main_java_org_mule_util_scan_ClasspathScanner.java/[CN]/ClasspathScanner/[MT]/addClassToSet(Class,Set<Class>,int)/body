{
  if (c != null) {
synchronized (set) {
      if (c.isInterface()) {
        if (hasFlag(flags,INCLUDE_INTERFACE))         set.add(c);
      }
 else       if (Modifier.isAbstract(c.getModifiers())) {
        if (hasFlag(flags,INCLUDE_ABSTRACT))         set.add(c);
      }
 else {
        set.add(c);
      }
    }
  }
}
