{
  UMOTransformer trans=(UMOTransformer)transformers.get(name);
  if (trans != null) {
    try {
      return (UMOTransformer)trans.clone();
    }
 catch (    Exception e) {
      throw new MuleRuntimeException(new Message(Messages.FAILED_TO_CLONE_X,"Transformer: " + trans.getName()));
    }
  }
  return null;
}
