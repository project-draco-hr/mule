{
  UMOTransformer trans=(UMOTransformer)transformers.get(name);
  if (trans != null) {
    try {
      return (UMOTransformer)trans.clone();
    }
 catch (    Exception e) {
      throw new NoSuchElementException("Failed to clone global transformer + " + trans.getName() + ": "+ e.getMessage());
    }
  }
  return null;
}
