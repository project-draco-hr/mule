{
  started.set(false);
  stopping.set(true);
  fireSystemEvent(new ManagerEvent(this,ManagerEvent.MANAGER_STOPPING));
  stopConnectors();
  stopAgents();
  if (queueManager != null) {
    queueManager.stop();
  }
  logger.debug("Stopping model...");
  if (model != null) {
    model.stop();
  }
  stopping.set(false);
  fireSystemEvent(new ManagerEvent(this,ManagerEvent.MANAGER_STOPPED));
}
