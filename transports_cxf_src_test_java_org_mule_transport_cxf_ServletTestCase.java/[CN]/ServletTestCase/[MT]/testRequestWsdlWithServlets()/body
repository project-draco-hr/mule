{
  String request="<soap:Envelope xmlns:soap=\"http://schemas.xmlsoap.org/soap/envelope/\" " + "xmlns:xsd=\"http://www.w3.org/2001/XMLSchema\" " + "xmlns:xsi=\"http://www.w3.org/2001/XMLSchema-instance\">"+ "<soap:Body>"+ "<ns1:echo xmlns:ns1=\"http://testmodels.cxf.transport.mule.org/\">"+ "<text>Test String</text>"+ "</ns1:echo>"+ "</soap:Body>"+ "</soap:Envelope>";
  MuleClient client=new MuleClient();
  MuleMessage result=client.send("http://localhost:" + HTTP_PORT + "/services/mycomponent",request,null);
  String res=result.getPayloadAsString();
  System.out.println(res);
  assertTrue(res.indexOf("Test String") != -1);
}
