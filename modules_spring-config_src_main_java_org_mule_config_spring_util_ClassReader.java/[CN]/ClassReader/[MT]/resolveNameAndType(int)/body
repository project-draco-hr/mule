{
  int oldPos=pos;
  try {
    NameAndType nt=(NameAndType)cpool[i];
    if (nt == null) {
      pos=cpoolIndex[i];
      String name=resolveUtf8(readShort());
      String type=resolveUtf8(readShort());
      cpool[i]=new NameAndType(name,type);
      nt=new NameAndType(name,type);
    }
    return nt;
  }
  finally {
    pos=oldPos;
  }
}
