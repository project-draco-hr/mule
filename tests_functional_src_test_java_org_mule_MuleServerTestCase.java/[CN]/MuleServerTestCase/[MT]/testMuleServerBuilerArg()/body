{
  MuleServer muleServer=new MuleServer(new String[]{"-builder","org.mule.config.spring.SpringXmlConfigurationBuilder"});
  assertEquals(ClassUtils.getResource("mule-config.xml",MuleServer.class).toString(),muleServer.getConfigurationResources());
  assertEquals("org.mule.config.spring.SpringXmlConfigurationBuilder",muleServer.getConfigBuilderClassName());
  muleServer.initialize();
}
