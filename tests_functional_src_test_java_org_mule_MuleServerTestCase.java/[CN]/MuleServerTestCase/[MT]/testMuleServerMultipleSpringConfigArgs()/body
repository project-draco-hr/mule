{
  MuleServer muleServer=new MuleServer(new String[]{"-config","mule-config.xml,org/mule/test/spring/config1/test-xml-mule2-config.xml"});
  assertEquals("mule-config.xml,org/mule/test/spring/config1/test-xml-mule2-config.xml",muleServer.getConfigurationResources());
  assertEquals(MuleServer.CLASSNAME_DEV_MODE_CONFIG_BUILDER,MuleServer.getConfigBuilderClassName());
  muleServer.initialize();
}
