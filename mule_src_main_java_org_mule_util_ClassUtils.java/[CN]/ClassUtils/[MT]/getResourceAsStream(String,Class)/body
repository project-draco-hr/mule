{
  InputStream inputStream=(InputStream)AccessController.doPrivileged(new PrivilegedAction(){
    public Object run(){
      URL url=getResource(resourceName,callingClass);
      try {
        return (url != null) ? url.openStream() : null;
      }
 catch (      IOException e) {
        return null;
      }
    }
  }
);
  return inputStream;
}
