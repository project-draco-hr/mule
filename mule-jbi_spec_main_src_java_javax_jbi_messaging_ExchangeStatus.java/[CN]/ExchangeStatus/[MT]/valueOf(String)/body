{
  ExchangeStatus instance;
  if (status.equals(DONE.toString())) {
    instance=DONE;
  }
 else   if (status.equals(ERROR.toString())) {
    instance=ERROR;
  }
 else   if (status.equals(ACTIVE.toString())) {
    instance=ACTIVE;
  }
 else {
    throw new java.lang.IllegalArgumentException(status);
  }
  return (instance);
}
