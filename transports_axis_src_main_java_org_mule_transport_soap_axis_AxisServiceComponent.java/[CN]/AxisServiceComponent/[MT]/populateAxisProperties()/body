{
  Map<String,Object> invocationProperties=new HashMap<String,Object>();
  Set<String> propertyNames=RequestContext.getEvent().getMessage().getPropertyNames(PropertyScope.INVOCATION);
  for (  String propName : propertyNames) {
    invocationProperties.put(propName,RequestContext.getEvent().getMessage().getInvocationProperty(propName));
  }
  AxisServiceProxy.setProperties(invocationProperties);
}
