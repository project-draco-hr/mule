{
  Object resultValue=intercepted.execute(executor,context);
  if (!(resultValue instanceof InterceptingCallback)) {
    throw new IllegalStateException(format("%s '%s' was expected to return a '%s' but a '%s' was found instead",getComponentModelTypeName(context.getComponentModel()),context.getComponentModel().getName(),InterceptingCallback.class.getSimpleName(),resultValue));
  }
  context.setVariable(INTERCEPTING_CALLBACK_PARAM,resultValue);
  return ((InterceptingCallback)resultValue).getResult();
}
