{
  if (callbackMethod != null) {
    try {
      callbackMethod.releaseConnection();
    }
 catch (    Exception e) {
      handleExceptionWhileTearingDown(e);
    }
  }
  if (callback != null) {
    try {
      callback.stop();
    }
 catch (    Exception e) {
      handleExceptionWhileTearingDown(e);
    }
  }
}
