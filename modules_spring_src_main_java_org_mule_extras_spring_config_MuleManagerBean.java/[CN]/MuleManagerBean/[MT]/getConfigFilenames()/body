{
  String[] result=new String[configResources.length];
  for (int i=0; i < result.length; i++) {
    try {
      result[i]=configResources[i].getURL().getPath();
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
  return StringUtils.arrayToCommaDelimitedString(result);
}
