{
  InboundEndpoint endpoint=muleContext.getRegistry().lookupEndpointFactory().getInboundEndpoint("jms://out");
  JmsConnector jmsConnector=(JmsConnector)muleContext.getRegistry().lookupConnector("jmsConnector");
  JmsSupport jmsSupport=jmsConnector.getJmsSupport();
  MessageConsumer consumer=jmsSupport.createConsumer(jmsConnector.getSession(endpoint),jmsSupport.createDestination(jmsConnector.getSession(endpoint),endpoint),false,endpoint);
  return consumer;
}
