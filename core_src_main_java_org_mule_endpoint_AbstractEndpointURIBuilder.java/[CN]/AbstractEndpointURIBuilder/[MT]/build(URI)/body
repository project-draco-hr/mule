{
  Properties props=getPropertiesForURI(uri);
  String replaceAddress=null;
  if (address != null) {
    replaceAddress=address;
    setEndpoint(uri,props);
    address=replaceAddress;
  }
 else {
    setEndpoint(uri,props);
  }
  EndpointURI ep=new MuleEndpointURI(address,endpointName,connectorName,transformers,responseTransformers,props,uri,userInfo);
  address=null;
  endpointName=null;
  connectorName=null;
  transformers=null;
  responseTransformers=null;
  return ep;
}
