{
  AxisFault e=(AxisFault)t;
  Map<?,?> props=getInfo(e);
  StringBuilder msg=new StringBuilder(64);
  msg.append("(");
  for (  Map.Entry<?,?> entry : props.entrySet()) {
    msg.append(entry.getKey()).append(": ").append(entry.getValue()).append(", ");
  }
  msg.append(")");
  return e.getMessage() + msg.toString();
}
