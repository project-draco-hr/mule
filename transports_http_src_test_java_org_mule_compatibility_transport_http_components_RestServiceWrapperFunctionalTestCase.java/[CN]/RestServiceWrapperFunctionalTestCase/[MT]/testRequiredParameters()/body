{
  Map<String,Serializable> props=new HashMap<>();
  props.put("baz-header","baz");
  props.put("bar-optional-header","bar");
  MuleMessage result=muleContext.getClient().send("restServiceEndpoint3",MuleMessage.builder().nullPayload().outboundProperties(props).build()).getRight();
  assertEquals("foo=boo&faz=baz&far=bar",getPayloadAsString(result));
}
