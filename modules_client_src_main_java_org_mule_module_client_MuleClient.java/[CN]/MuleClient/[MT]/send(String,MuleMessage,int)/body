{
  OutboundEndpoint endpoint=getOutboundEndpoint(url,MessageExchangePattern.REQUEST_RESPONSE,timeout);
  MuleEvent event=getEvent(message,new NullInboundEndpoint(MessageExchangePattern.REQUEST_RESPONSE,muleContext));
  event.setTimeout(timeout);
  MuleEvent response=endpoint.process(event);
  if (response != null) {
    return response.getMessage();
  }
 else {
    return new DefaultMuleMessage(NullPayload.getInstance(),muleContext);
  }
}
