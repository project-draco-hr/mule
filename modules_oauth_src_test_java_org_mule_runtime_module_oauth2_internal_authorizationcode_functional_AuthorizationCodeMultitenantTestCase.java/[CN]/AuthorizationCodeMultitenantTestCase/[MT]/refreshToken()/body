{
  executeForUserWithAccessToken(USER_ID_JOHN,JOHN_ACCESS_TOKEN,NO_STATE);
  WireMock.reset();
  executeForUserWithAccessToken(USER_ID_TONY,TONY_ACCESS_TOKEN,NO_STATE);
  OAuthContextFunctionAsserter.createFrom(muleContext.getExpressionLanguage(),MULTITENANT_CONFIG,USER_ID_JOHN).assertAccessTokenIs(JOHN_ACCESS_TOKEN).assertState(null);
  OAuthContextFunctionAsserter.createFrom(muleContext.getExpressionLanguage(),MULTITENANT_CONFIG,USER_ID_TONY).assertAccessTokenIs(TONY_ACCESS_TOKEN).assertState(null);
}
