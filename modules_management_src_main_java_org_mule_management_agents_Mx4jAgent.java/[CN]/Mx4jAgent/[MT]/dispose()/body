{
  try {
    stop();
  }
 catch (  Exception e) {
    logger.warn("Failed to stop Mx4jAgent: " + e.getMessage());
  }
 finally {
    try {
      mBeanServer.unregisterMBean(adaptorName);
    }
 catch (    Exception e) {
      logger.error("Couldn't unregister MBean: " + adaptorName.getCanonicalName(),e);
    }
  }
}
