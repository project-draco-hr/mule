{
  super.doSetUp();
  tempDir=new File(managementContext.getRegistry().getConfiguration().getWorkingDirectory(),tempDirName);
  if (!tempDir.exists()) {
    tempDir.mkdirs();
  }
  validMessage=File.createTempFile("hello",".txt",tempDir);
  assertNotNull(validMessage);
  connector=getConnector();
}
