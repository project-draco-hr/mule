{
  ClassLoader classLoader=(ClassLoader)pluginClassLoader.getClass().getMethod("getClassLoader").invoke(pluginClassLoader);
  final URL localResource=(URL)classLoader.getClass().getMethod("findResource",String.class).invoke(classLoader,BOOTSTRAP_PROPERTIES);
  BootstrapService pluginBootstrapService=null;
  if (localResource != null) {
    final Properties properties=PropertiesUtils.loadProperties(localResource);
    pluginBootstrapService=new PropertiesBootstrapService(classLoader,properties);
  }
  return pluginBootstrapService;
}
