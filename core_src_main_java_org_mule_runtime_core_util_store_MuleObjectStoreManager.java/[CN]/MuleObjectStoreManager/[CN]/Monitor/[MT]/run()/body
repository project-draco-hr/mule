{
  if (muleContext.isPrimaryPollingInstance()) {
    try {
      store.expire(entryTTL,maxEntries,partitionName);
    }
 catch (    Exception e) {
      MuleObjectStoreManager.logger.warn("Running expirty on partition " + partitionName + " of "+ store+ " threw "+ e+ ":"+ e.getMessage());
    }
  }
}
