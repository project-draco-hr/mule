{
  try {
    if (isStarted()) {
      stop();
    }
    if (isConnected()) {
      disconnect();
    }
  }
 catch (  Exception e) {
    logger.warn(e.getMessage(),e);
  }
  try {
    lifecycleManager.fireDisposePhase(new LifecycleCallback<O>(){
      @Override public void onTransition(      String phaseName,      O object) throws MuleException {
        doDispose();
      }
    }
);
  }
 catch (  MuleException e) {
    logger.warn(e.getMessage(),e);
  }
}
