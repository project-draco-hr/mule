{
  MuleClient client=new MuleClient(muleContext);
  MuleMessage msg=client.send(((InboundEndpoint)client.getMuleContext().getRegistry().lookupObject("httpInbound")).getAddress(),new DefaultMuleMessage(request,muleContext));
  assertNotNull(msg);
  assertNotNull(msg.getPayload());
  assertEquals(response,msg.getPayloadAsString());
}
