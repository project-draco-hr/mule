{
  MuleClient client=muleContext.getClient();
  MuleMessage msg=client.send(((InboundEndpoint)muleContext.getRegistry().lookupObject("httpInbound")).getAddress(),new DefaultMuleMessage(request,muleContext));
  assertNotNull(msg);
  assertNotNull(msg.getPayload());
  assertEquals(response,msg.getPayloadAsString());
}
