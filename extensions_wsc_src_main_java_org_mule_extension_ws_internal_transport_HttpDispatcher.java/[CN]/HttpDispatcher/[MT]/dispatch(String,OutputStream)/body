{
  OkHttpClient client=new OkHttpClient();
  MediaType mediaType=MediaType.parse("text/xml");
  RequestBody body=RequestBody.create(mediaType,os.toString());
  Request request=new Request.Builder().url(address).post(body).addHeader("content-type","text/xml").addHeader("cache-control","no-cache").build();
  try {
    Response response=client.newCall(request).execute();
    return IOUtils.toString(response.body().byteStream());
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
