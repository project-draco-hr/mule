{
  super.setUp();
  if (manager == null) {
    manager=new MuleXmlConfigurationBuilder().configure(new ReaderResource[]{new ReaderResource("Hard-coded server configuration",new StringReader(getServerConfiguration()))});
    manager.start();
    connector=((OracleJmsConnector)manager.lookupConnector("TestConnector"));
    session=(AQjmsSession)connector.getSession(false,false);
  }
}
