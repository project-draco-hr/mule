{
  parseConfigRef(element,builder);
  builder.addConstructorArgValue(extensionModel).addConstructorArgValue(operationModel).addConstructorArgValue(toElementDescriptorBeanDefinition(element)).addConstructorArgValue(parseNestedOperations(element,parserContext)).addConstructorArgReference(OBJECT_MULE_CONTEXT);
  attachProcessorDefinition(parserContext,builder.getBeanDefinition());
}
