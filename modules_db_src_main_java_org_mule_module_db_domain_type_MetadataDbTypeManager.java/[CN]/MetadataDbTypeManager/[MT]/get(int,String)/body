{
  if (!initialised) {
synchronized (lock) {
      if (!initialised) {
        initialise();
        initialised=true;
      }
    }
  }
  String typeKey=name + id;
  if (typesById.containsKey(typeKey)) {
    return typesById.get(typeKey);
  }
 else   if (id == Types.OTHER) {
    return UnknownDbType.getInstance();
  }
 else {
    throw new UnknownDbTypeException(id,name);
  }
}
