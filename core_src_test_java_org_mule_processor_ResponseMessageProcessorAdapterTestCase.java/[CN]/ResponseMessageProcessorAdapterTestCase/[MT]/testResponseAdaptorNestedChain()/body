{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(new StringAppendTransformer("1"),new ResponseMessageProcessorAdapter(new DefaultMessageProcessorChainBuilder().chain(new StringAppendTransformer("a"),new StringAppendTransformer("b")).build()),new StringAppendTransformer("2"));
  assertEquals("012ab",builder.build().process(getTestEvent("0")).getMessageAsString());
}
