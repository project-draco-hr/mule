{
switch (eDerivedStructuralFeatureID(eFeature)) {
case SchemaPackage.CONTAINER_CONTEXT_TYPE__MIXED:
    getMixed().clear();
  getMixed().addAll((Collection)newValue);
return;
case SchemaPackage.CONTAINER_CONTEXT_TYPE__PROPERTIES:
setProperties((PropertiesType)newValue);
return;
case SchemaPackage.CONTAINER_CONTEXT_TYPE__CLASS_NAME:
setClassName((String)newValue);
return;
case SchemaPackage.CONTAINER_CONTEXT_TYPE__NAME:
setName((String)newValue);
return;
}
eDynamicSet(eFeature,newValue);
}
