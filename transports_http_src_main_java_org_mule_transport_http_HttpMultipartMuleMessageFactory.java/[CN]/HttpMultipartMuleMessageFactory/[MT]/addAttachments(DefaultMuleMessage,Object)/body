{
  if (parts != null) {
    try {
      for (      Part part : parts) {
        if (!part.getName().equals("payload")) {
          message.addInboundAttachment(part.getName(),new DataHandler(new PartDataSource(part)));
        }
      }
    }
  finally {
      parts.clear();
      parts=null;
    }
  }
}
