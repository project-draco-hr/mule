{
  if (licenses == null || licenses.size() == 0)   return null;
  StringBuffer sb=new StringBuffer();
  String del="";
  for (Iterator i=licenses.iterator(); i.hasNext(); ) {
    License l=(License)i.next();
    String url=l.getUrl();
    if (url == null)     continue;
    sb.append(del);
    sb.append(url);
    del=", ";
  }
  if (sb.length() == 0)   return null;
  return sb;
}
