{
  if (null != element && !element.hasAttribute(ATTRIBUTE_ID)) {
    element.setAttribute(ATTRIBUTE_ID,ID_PREFIX + counter.incrementAndGet());
  }
  if (null != element && element.hasAttribute(INHERIT) && Boolean.valueOf(element.getAttribute(INHERIT)).booleanValue()) {
    return named;
  }
 else {
    return orphan;
  }
}
