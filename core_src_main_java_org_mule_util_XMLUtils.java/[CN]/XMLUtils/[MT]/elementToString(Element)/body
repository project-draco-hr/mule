{
  StringBuilder buf=new StringBuilder();
  buf.append(e.getTagName()).append("{");
  for (int i=0; i < e.getAttributes().getLength(); i++) {
    if (i > 0) {
      buf.append(", ");
    }
    Node n=e.getAttributes().item(i);
    buf.append(attributeName((Attr)n)).append("=").append(n.getNodeValue());
  }
  buf.append("}");
  return buf.toString();
}
