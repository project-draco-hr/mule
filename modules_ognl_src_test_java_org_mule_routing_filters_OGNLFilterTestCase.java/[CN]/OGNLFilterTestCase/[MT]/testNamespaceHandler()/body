{
  String expression;
  if (variant.equals(ConfigVariant.SERVICE)) {
    expression=((OGNLFilter)((FilteringOutboundRouter)((OutboundRouterCollection)muleContext.getRegistry().lookupService(SERVICE_NAME).getOutboundMessageProcessor()).getRoutes().get(0)).getFilter()).getExpression();
  }
 else {
    expression=((OGNLFilter)((MessageFilter)((Flow)muleContext.getRegistry().lookupFlowConstruct(SERVICE_NAME)).getMessageProcessors().get(0)).getFilter()).getExpression();
  }
  assertEquals(expression,OGNL_EXSPRESSION);
}
