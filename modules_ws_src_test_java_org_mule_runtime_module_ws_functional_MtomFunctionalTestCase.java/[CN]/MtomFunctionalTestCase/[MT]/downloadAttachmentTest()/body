{
  String request=String.format("<ns:downloadAttachment xmlns:ns=\"http://consumer.ws.module.runtime.mule.org/\">" + "<fileName>%s</fileName></ns:downloadAttachment>",TEST_FILE_ATTACHMENT);
  Event event=flowRunner("clientDownloadAttachment").withPayload(request).run();
  assertAttachmentInResponse(event.getMessage(),TEST_FILE_ATTACHMENT);
}
