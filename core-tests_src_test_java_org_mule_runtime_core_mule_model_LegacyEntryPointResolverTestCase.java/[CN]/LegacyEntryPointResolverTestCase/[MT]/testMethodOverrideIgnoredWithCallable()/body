{
  EntryPointResolverSet resolver=new LegacyEntryPointResolverSet();
  setCurrentEvent(getTestEvent(new FruitLover("Yummy!")));
  MuleEvent event=getCurrentEvent();
  event.setMessage(MuleMessage.builder(event.getMessage()).addOutboundProperty(METHOD_PROPERTY_NAME,INVALID_METHOD_NAME).build());
  Apple apple=new Apple();
  apple.setAppleCleaner(new FruitCleaner(){
    @Override public void wash(    Fruit fruit){
    }
    @Override public void polish(    Fruit fruit){
    }
  }
);
  apple.setMuleContext(muleContext);
  resolver.invoke(apple,new DefaultMuleEventContext(event));
}
