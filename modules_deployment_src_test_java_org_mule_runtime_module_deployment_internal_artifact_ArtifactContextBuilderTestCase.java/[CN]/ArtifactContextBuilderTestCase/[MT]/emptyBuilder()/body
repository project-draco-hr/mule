{
  MuleContext muleContext=new ArtifactContextBuilder().setExecutionClassloader(currentThread().getContextClassLoader()).build().getMuleContext();
  assertThat(muleContext,notNullValue());
  assertThat(muleContext.isInitialised(),is(true));
  muleContext.start();
  assertThat(muleContext.isStarted(),is(true));
}
