{
  parserDelegate.parseConfigRef(element,builder);
  builder.addConstructorArgValue(extensionModel).addConstructorArgValue(operationModel).addConstructorArgValue(parserDelegate.toElementDescriptorBeanDefinition(element)).addConstructorArgValue(parseNestedOperations(element,parserContext)).addConstructorArgReference(OBJECT_MULE_CONTEXT);
  attachProcessorDefinition(parserContext,builder.getBeanDefinition());
}
