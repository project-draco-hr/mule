{
  verifyAllNotifications(spec,ModelNotification.class,ModelNotification.MODEL_INITIALISING,ModelNotification.MODEL_INITIALISED);
  verifyAllNotifications(spec,ModelNotification.class,ModelNotification.MODEL_STARTING,ModelNotification.MODEL_DISPOSED);
  verifyAllNotifications(spec,ServiceNotification.class,ServiceNotification.SERVICE_INITIALISED,ServiceNotification.SERVICE_STOPPING);
  verifyAllNotifications(spec,ManagerNotification.class,ManagerNotification.MANAGER_STARTING,ManagerNotification.MANAGER_STOPPED_MODELS);
  assertExpectedNotifications(spec);
}
