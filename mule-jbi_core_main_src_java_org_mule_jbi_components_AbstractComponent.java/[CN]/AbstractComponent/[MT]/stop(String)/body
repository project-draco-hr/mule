{
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Stopping service unit");
    }
    doStop(serviceUnitName);
    if (logger.isDebugEnabled()) {
      logger.debug("Service unit stopped");
    }
  }
 catch (  DeploymentException e) {
    throw e;
  }
catch (  Exception e) {
    throw new DeploymentException("Error stopping service unit",e);
  }
}
