{
  MetadataResult<MetadataType> outputMetadataResult=getOutputMetadata(context,key);
  OutputMetadataDescriptor descriptor=MetadataDescriptorBuilder.outputDescriptor().withReturnType(outputMetadataResult.get()).withAttributesType(operationModel.getAttributesType()).build();
  return outputMetadataResult.isSuccess() ? success(descriptor) : failure(descriptor,outputMetadataResult);
}
