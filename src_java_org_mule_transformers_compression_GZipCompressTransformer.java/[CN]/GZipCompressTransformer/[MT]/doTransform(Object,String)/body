{
  try {
    byte[] data=null;
    if (src instanceof byte[]) {
      data=(byte[])src;
    }
 else {
      data=SerializationUtils.serialize((Serializable)src);
    }
    return this.getStrategy().compressByteArray(data);
  }
 catch (  IOException ioex) {
    throw new TransformerException(this,ioex);
  }
}
