{
  int event;
  if (mode == MODE_TRACKING) {
    event=super.next();
    capture(event);
  }
 else   if (mode == MODE_REPLAY) {
    if (replayIndex == events.size()) {
      mode=MODE_NORMAL;
      event=super.next();
    }
 else {
      event=getReplayEvent();
    }
  }
 else {
    event=super.next();
  }
  return event;
}
