{
  UMOEndpointURI url=new MuleEndpointURI("jms://XML_QUEUE" + "?" + OracleJmsConnector.PAYLOADFACTORY_PROPERTY + "=oracle.xdb.XMLTypeFactory"+ "&transformers=XMLMessageToString");
  assertEquals("oracle.xdb.XMLTypeFactory",url.getParams().getProperty(OracleJmsConnector.PAYLOADFACTORY_PROPERTY));
}
