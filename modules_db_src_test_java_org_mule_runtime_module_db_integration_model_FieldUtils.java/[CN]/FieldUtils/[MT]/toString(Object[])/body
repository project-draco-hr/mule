{
  if (value == null) {
    return "null";
  }
  int iMax=value.length - 1;
  if (iMax == -1) {
    return "[]";
  }
  StringBuilder builder=new StringBuilder();
  builder.append('[');
  for (int i=0; ; i++) {
    if (value[i] instanceof Object[]) {
      builder.append(toString((Object[])value[i]));
    }
 else {
      builder.append(String.valueOf(value[i]));
    }
    if (i == iMax) {
      return builder.append(']').toString();
    }
    builder.append(", ");
  }
}
