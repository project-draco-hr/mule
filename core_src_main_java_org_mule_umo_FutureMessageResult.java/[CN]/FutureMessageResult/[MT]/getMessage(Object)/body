{
  if (obj != null) {
    if (obj instanceof UMOMessage) {
      return (UMOMessage)obj;
    }
synchronized (this) {
      if (transformers != null) {
        obj=TransformerUtils.applyAllTransformersToObject(transformers,obj);
      }
    }
    return new MuleMessage(obj);
  }
 else {
    return null;
  }
}
