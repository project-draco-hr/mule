{
  UMOMessage result=null;
  if (obj != null) {
    if (obj instanceof UMOMessage) {
      result=(UMOMessage)obj;
    }
 else {
      result=new MuleMessage(obj);
    }
synchronized (this) {
      if (transformers != null) {
        result.applyTransformers(transformers);
      }
    }
  }
  return result;
}
