{
  UMOEndpoint endpoint;
  List endpoints=getIncomingEndpoints();
  for (Iterator it=endpoints.iterator(); it.hasNext(); ) {
    endpoint=(UMOEndpoint)it.next();
    try {
      endpoint.getConnector().registerListener(this,endpoint);
    }
 catch (    UMOException e) {
      throw e;
    }
catch (    Exception e) {
      throw new ModelException(CoreMessages.failedtoRegisterOnEndpoint(name,endpoint.getEndpointURI()),e);
    }
  }
}
