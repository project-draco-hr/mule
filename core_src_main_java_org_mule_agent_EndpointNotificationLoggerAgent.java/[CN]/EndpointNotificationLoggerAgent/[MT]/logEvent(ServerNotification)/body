{
  if (muleContext.isDisposing() || muleContext.isDisposed()) {
    logger.warn("MuleContext is disposing/disposed, no notification will be processed: " + e);
    return;
  }
  if (endpoint != null && !ignoredNotifications.contains(new Integer(e.getAction()))) {
    if ((e.getAction() == ConnectionNotification.CONNECTION_FAILED || e.getAction() == ConnectionNotification.CONNECTION_DISCONNECTED) && ((Connector)e.getSource()).equals(endpoint.getConnector())) {
      return;
    }
    try {
      MuleMessage msg=new DefaultMuleMessage(e,(Map)null);
      MuleEvent event=new DefaultMuleEvent(msg,endpoint,session,false);
      endpoint.dispatch(event);
    }
 catch (    Exception e1) {
      logger.error("Failed to dispatch event: " + e.toString() + " over endpoint: "+ endpoint+ ". Error is: "+ e1.getMessage(),e1);
    }
  }
}
