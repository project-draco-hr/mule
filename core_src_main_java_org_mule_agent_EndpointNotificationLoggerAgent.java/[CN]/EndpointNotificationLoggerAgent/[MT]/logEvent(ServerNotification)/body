{
  if (muleContext.isDisposing() || muleContext.isDisposed()) {
    logger.warn("MuleContext is disposing/disposed, no notification will be processed");
    return;
  }
  if (logEndpoint != null && !ignoredNotifications.contains(new Integer(e.getAction()))) {
    if ((e.getAction() == ConnectionNotification.CONNECTION_FAILED || e.getAction() == ConnectionNotification.CONNECTION_DISCONNECTED) && ((Connector)e.getSource()).equals(logEndpoint.getConnector())) {
      return;
    }
    try {
      MuleMessage msg=new DefaultMuleMessage(e.toString(),(Map)null);
      MuleEvent event=new DefaultMuleEvent(msg,logEndpoint,session,false);
      logEndpoint.dispatch(event);
    }
 catch (    Exception e1) {
      logger.error("Failed to dispatch event: " + e.toString() + " over endpoint: "+ logEndpoint+ ". Error is: "+ e1.getMessage(),e1);
    }
  }
}
