{
  final String[] tokens=new String[]{artifact.getGroupId(),artifact.getArtifactId(),artifact.getExtension(),artifact.getClassifier(),artifact.getBaseVersion()};
  final String[] patternTokens=pattern.split(MAVEN_COORDINATES_SEPARATOR);
  boolean matched=(patternTokens.length <= tokens.length);
  for (int i=0; matched && i < patternTokens.length; i++) {
    matched=matches(tokens[i],patternTokens[i]);
  }
  return matched;
}
