{
  ReflectionEntryPointResolver resolver=new ReflectionEntryPointResolver();
  final Event testEvent=Event.builder(DefaultEventContext.create(flowConstruct,TEST_CONNECTOR)).message(InternalMessage.of("Hello")).build();
  setCurrentEvent(testEvent);
  MuleEventContext eventContext=new DefaultMuleEventContext(flowConstruct,getCurrentEvent());
  InvocationResult result=resolver.invoke(new MultiplePayloadsTestObject(),eventContext,Event.builder(getCurrentEvent()));
  assertEquals(result.getState(),InvocationResult.State.FAILED);
}
