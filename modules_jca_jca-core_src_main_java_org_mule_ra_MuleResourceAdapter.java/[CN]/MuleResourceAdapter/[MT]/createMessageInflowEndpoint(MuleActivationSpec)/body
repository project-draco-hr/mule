{
  EndpointURI uri=new MuleEndpointURI(muleActivationSpec.getEndpoint());
  EndpointBuilder endpointBuilder=new EndpointURIEndpointBuilder(new URIBuilder(muleActivationSpec.getEndpoint()),muleContext);
  endpointBuilder.setSynchronous(false);
  return muleContext.getRegistry().lookupEndpointFactory().getInboundEndpoint(endpointBuilder);
}
