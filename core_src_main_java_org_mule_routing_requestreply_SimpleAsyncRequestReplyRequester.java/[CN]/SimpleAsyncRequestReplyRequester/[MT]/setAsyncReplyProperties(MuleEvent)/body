{
  event.getMessage().setReplyTo(getReplyTo());
  event.getMessage().setOutboundProperty(MuleProperties.MULE_REPLY_TO_REQUESTOR_PROPERTY,event.getFlowConstruct().getName());
  String correlation=event.getFlowConstruct().getMessageInfoMapping().getCorrelationId(event.getMessage());
  event.getMessage().setCorrelationId(correlation);
}
