{
  try {
    close(tempTopic);
  }
 catch (  JMSException e) {
    if (logger.isWarnEnabled()) {
      String topicName="";
      try {
        topicName=tempTopic.getTopicName();
      }
 catch (      JMSException innerEx) {
      }
      logger.warn("Failed to delete a temporary topic " + topicName + ": "+ e.getMessage());
    }
  }
}
