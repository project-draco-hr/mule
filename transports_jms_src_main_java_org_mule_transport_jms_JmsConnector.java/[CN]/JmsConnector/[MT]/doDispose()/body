{
  if (connection != null) {
    try {
      connection.close();
    }
 catch (    JMSException e) {
      logger.error("Jms connector failed to dispose properly: ",e);
    }
    connection=null;
  }
  if (jndiContext != null) {
    try {
      jndiContext.close();
    }
 catch (    NamingException ne) {
      logger.error("Jms connector failed to dispose properly: ",ne);
    }
 finally {
      jndiContext=null;
    }
  }
}
