{
  for (  VariableResolverFactory factory : resolverFactories) {
    if (factory.isResolveable(name)) {
      return factory.getVariableResolver(name);
    }
 else     if (factory instanceof AbstractVariableResolverFactory && ((AbstractVariableResolverFactory)factory).getAliases().containsKey(name)) {
      return new MuleVariableResolver(name,MVEL.eval(((AbstractVariableResolverFactory)factory).getAliases().get(name),this),null);
    }
  }
  if (variableResolvers.containsKey(name)) {
    return variableResolvers.get(name);
  }
 else   if (nextFactory != null) {
    return nextFactory.getVariableResolver(name);
  }
  throw new UnresolveablePropertyException("unable to resolve variable '" + name + "'");
}
