{
  String line=null;
  try {
    do {
      line=HttpParser.readLine(in,encoding);
    }
 while (line != null && line.length() == 0);
    if (line == null) {
      setKeepAlive(false);
      return null;
    }
    HttpRequest request=new HttpRequest(RequestLine.parseLine(line),HttpParser.parseHeaders(this.in,encoding),this.in);
    return request;
  }
 catch (  IOException e) {
    System.out.println(line);
    close();
    throw e;
  }
}
