{
  try {
    String line=null;
    do {
      line=HttpParser.readLine(in,encoding);
    }
 while (line != null && line.length() == 0);
    if (line == null) {
      setKeepAlive(false);
      return null;
    }
    HttpResponse response=new HttpResponse(new StatusLine(line),HttpParser.parseHeaders(this.in,encoding),this.in);
    return response;
  }
 catch (  IOException e) {
    close();
    throw e;
  }
}
