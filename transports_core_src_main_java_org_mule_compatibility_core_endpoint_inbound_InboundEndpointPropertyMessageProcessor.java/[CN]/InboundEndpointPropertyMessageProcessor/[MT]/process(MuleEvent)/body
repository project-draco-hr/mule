{
  String inboundEndpoint=endpoint.getName();
  if (StringUtils.isBlank(inboundEndpoint)) {
    inboundEndpoint=endpoint.getEndpointURI().getUri().toString();
  }
  return MuleEvent.builder(event).message(MuleMessage.builder(event.getMessage()).addInboundProperty(MULE_ORIGINATING_ENDPOINT_PROPERTY,inboundEndpoint).build()).build();
}
