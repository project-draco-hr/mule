{
  String inboundEndpoint=endpoint.getName();
  if (StringUtils.isBlank(inboundEndpoint)) {
    inboundEndpoint=endpoint.getEndpointURI().getUri().toString();
  }
  return Event.builder(event).message(InternalMessage.builder(event.getMessage()).addInboundProperty(MULE_ORIGINATING_ENDPOINT_PROPERTY,inboundEndpoint).build()).build();
}
