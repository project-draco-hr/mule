{
  try {
    if (localContext != null) {
      return ((TransactionalQueueManager.QueueTransactionContext)localContext).peek(queue);
    }
 else {
      Object id=queue.peek();
      if (id != null) {
        return queueManager.doLoad(queue,id);
      }
      return null;
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
