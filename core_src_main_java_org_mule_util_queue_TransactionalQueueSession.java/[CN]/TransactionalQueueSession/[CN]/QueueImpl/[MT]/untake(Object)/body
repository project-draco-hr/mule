{
  if (localContext != null) {
    ((TransactionalQueueManager.QueueTransactionContext)localContext).untake(queue,item);
  }
 else {
    try {
      Object id=queueManager.doStore(queue,item);
      queue.untake(id);
    }
 catch (    IOException e) {
      throw new RuntimeException(e);
    }
  }
}
