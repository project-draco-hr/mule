{
  StringBuilder buffer=new StringBuilder();
  boolean first=true;
  for (int i=0; i < names.size(); i++) {
    if (first) {
      buffer.append(QUERY);
      first=false;
    }
 else {
      buffer.append(AND);
    }
    buffer.append(names.get(i));
    String value=values.get(i);
    if (null != value) {
      buffer.append(EQUALS);
      buffer.append(value);
    }
  }
  return buffer.toString();
}
