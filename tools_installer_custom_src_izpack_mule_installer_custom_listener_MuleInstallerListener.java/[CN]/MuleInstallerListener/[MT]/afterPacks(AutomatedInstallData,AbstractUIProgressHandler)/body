{
  if (!idata.installSuccess) {
    List availablePacks=idata.availablePacks;
    for (int i=0; i < availablePacks.size(); i++) {
      String packagePath="/" + ((Pack)availablePacks.get(i)).name;
      File dir=new File(idata.getInstallPath() + packagePath);
      if (dir.exists()) {
        deleteDir(dir);
      }
    }
  }
}
