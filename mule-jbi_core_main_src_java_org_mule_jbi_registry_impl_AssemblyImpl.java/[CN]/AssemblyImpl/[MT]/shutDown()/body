{
  if (getCurrentState().equals(UNKNOWN)) {
    throw new JBIException("Illegal status: " + getCurrentState());
  }
  if (!getCurrentState().equals(SHUTDOWN)) {
    stop();
    Unit[] units=getUnits();
    for (int i=0; i < units.length; i++) {
      units[i].shutDown();
    }
    setCurrentState(SHUTDOWN);
  }
  return "";
}
