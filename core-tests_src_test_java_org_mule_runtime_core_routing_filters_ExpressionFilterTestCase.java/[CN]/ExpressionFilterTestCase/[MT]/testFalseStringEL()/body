{
  ExpressionFilter filter=new ExpressionFilter("payload");
  filter.setMuleContext(muleContext);
  filter.setNullReturnsTrue(false);
  assertFalse(filter.accept(MuleMessage.builder().payload("false").build()));
  assertFalse(filter.accept(MuleMessage.builder().payload("FALSE").build()));
  assertFalse(filter.accept(MuleMessage.builder().payload("faLSe").build()));
}
