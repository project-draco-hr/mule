{
  Object temp;
  if (isTransformFirst()) {
    temp=context.transformMessage();
  }
 else {
    temp=context.getMessage().getPayload();
  }
  if (temp instanceof Object[]) {
    return (Object[])temp;
  }
 else   if (temp instanceof NullPayload) {
    return ClassUtils.NO_ARGS;
  }
 else {
    return new Object[]{temp};
  }
}
