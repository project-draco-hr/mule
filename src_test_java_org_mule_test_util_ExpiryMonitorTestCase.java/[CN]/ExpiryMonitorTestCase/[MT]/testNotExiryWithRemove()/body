{
  ExpiryMonitor monitor=new ExpiryMonitor(100);
  Expirable e=new Expirable(){
    public void expired(){
      expired=true;
    }
  }
;
  monitor.addExpirable(800,e);
  Thread.sleep(500);
  assertTrue(!expired);
  Thread.sleep(200);
  monitor.removeExpirable(e);
  Thread.sleep(300);
  assertTrue(!expired);
  assertTrue(!monitor.isRegistered(e));
}
