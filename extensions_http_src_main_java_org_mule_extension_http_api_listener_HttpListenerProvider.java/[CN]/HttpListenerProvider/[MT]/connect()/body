{
  HttpServerConfiguration serverConfiguration=new HttpServerConfiguration.Builder().setHost(host).setPort(port).setTlsContextFactory(tlsContext).setUsePersistentConnections(usePersistentConnections).setConnectionIdleTimeout(connectionIdleTimeout).setOwnerName(configName).build();
  Server server=connectionManager.create(serverConfiguration);
  try {
    server.start();
  }
 catch (  IOException e) {
    throw new ConnectionException("Could not start server",e);
  }
  return server;
}
