{
  String[] all;
  if (first) {
    all=new String[configResources.length + 1];
    all[0]=defaultConfigResource;
    System.arraycopy(configResources,0,all,1,configResources.length);
  }
 else {
    all=configResources;
  }
  createSpringParentRegistry(muleContext,muleContext.getRegistry(),all);
}
