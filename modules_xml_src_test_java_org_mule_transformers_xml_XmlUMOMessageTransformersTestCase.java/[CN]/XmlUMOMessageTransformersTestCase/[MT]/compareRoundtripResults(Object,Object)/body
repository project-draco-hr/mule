{
  if (expected == null && result == null) {
    return true;
  }
  if (expected == null || result == null) {
    return false;
  }
  if (expected instanceof MuleMessage && result instanceof MuleMessage) {
    return ((MuleMessage)expected).getPayload().equals(((MuleMessage)result).getPayload()) && ((MuleMessage)expected).getProperty("object").equals(((MuleMessage)result).getProperty("object")) && ((MuleMessage)expected).getProperty("string").equals(((MuleMessage)result).getProperty("string"))&& ((MuleMessage)expected).getIntProperty("number",-1) == ((MuleMessage)result).getIntProperty("number",-2);
  }
 else {
    return false;
  }
}
