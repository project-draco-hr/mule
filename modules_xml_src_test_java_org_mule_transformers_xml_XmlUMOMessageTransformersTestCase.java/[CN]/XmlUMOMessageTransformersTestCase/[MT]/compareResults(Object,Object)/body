{
  if (!super.compareResults(expected,result)) {
    try {
      XMLAssert.assertXpathEvaluatesTo("3","count(//adapter/properties/scopedMap/entry/map/entry)",(String)result);
      String expectedValue=(SystemUtils.IS_JAVA_1_5 ? "object" : "number");
