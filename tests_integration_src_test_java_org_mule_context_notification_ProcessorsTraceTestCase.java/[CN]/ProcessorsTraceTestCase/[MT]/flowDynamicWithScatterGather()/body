{
  muleContext.getClient().send("vm://in-flowDynamicWithScatterGather",new DefaultMuleMessage("payload",muleContext));
  assertThat(ProcessorsTraceAsserter.processorsTraceToAssert,not(nullValue()));
  assertThat(ProcessorsTraceAsserter.processorsTraceToAssert,hasExecutedProcessors("/flowDynamicWithScatterGather/processors/0","/flowDynamicWithScatterGather/processors/0/0/0","/flowDynamicWithScatterGather/processors/0/1/0","/flow/processors/0"));
}
