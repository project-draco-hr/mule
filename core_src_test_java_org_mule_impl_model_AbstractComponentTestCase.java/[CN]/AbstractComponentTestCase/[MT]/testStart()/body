{
  try {
    component.start();
    fail("Exception expected: Cannot start an uninitialised component");
  }
 catch (  Exception e) {
  }
  component.initialise();
  component.start();
  try {
    component.initialise();
    fail("Exception expected: Cannot initialise an already initialised component");
  }
 catch (  Exception e) {
  }
}
