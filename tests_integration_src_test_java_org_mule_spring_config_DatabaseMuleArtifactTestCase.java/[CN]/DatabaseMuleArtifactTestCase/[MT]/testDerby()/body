{
  String config="<jdbc:connector name=\"jdbcConnector\" pollingFrequency=\"1000\" dataSource-ref=\"jdbcDataSource\" queryTimeout=\"3000\" xmlns:jdbc=\"http://www.mulesoft.org/schema/mule/jdbc\"/>";
  Document document=XMLUnit.buildControlDocument(config);
  String refDef="<spring:bean xmlns:spring=\"http://www.springframework.org/schema/beans\" class=\"org.apache.commons.dbcp.BasicDataSource\" destroy-method=\"close\" id=\"jdbcDataSource\" name=\"Bean\">" + "<spring:property name=\"driverClassName\" value=\"org.apache.derby.jdbc.EmbeddedDriver\"/>" + "<spring:property name=\"url\" value=\"jdbc:derby:muleEmbeddedDB;create=true\"/>"+ "</spring:bean>";
  XmlConfigurationCallback callback=new DatabaseConfigurationCallback(Collections.singletonMap("jdbcDataSource",refDef));
  doTest(document,callback,true);
  doTest(document,callback,true);
  doTest(document,callback,true);
}
