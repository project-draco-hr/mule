{
  String config="<jdbc:connector name=\"jdbcConnector\" pollingFrequency=\"1000\" dataSource-ref=\"mysqlDatasource\" queryTimeout=\"3000\" xmlns:jdbc=\"http://www.mulesoft.org/schema/mule/jdbc\"/>";
  Document document=XMLUnit.buildControlDocument(config);
  String refDef="<spring:bean xmlns:spring=\"http://www.springframework.org/schema/beans\" class=\"org.apache.commons.dbcp.BasicDataSource\" destroy-method=\"close\" id=\"mysqlDatasource\" name=\"Bean\">" + "<spring:property name=\"driverClassName\" value=\"com.mysql.jdbc.Driver\"/>" + "<spring:property name=\"url\" value=\"jdbc:mysql://localhost/test\"/>"+ "<spring:property name=\"username\" value=\"myUser\"/>"+ "<spring:property name=\"password\" value=\"secret\"/>"+ "</spring:bean>";
  XmlConfigurationCallback callback=new DatabaseConfigurationCallback(Collections.singletonMap("mysqlDatasource",refDef));
  doTest(document,callback,false);
  doTest(document,callback,false);
  doTest(document,callback,false);
}
