{
  if (disposed.get())   return;
  if (started.get())   stop();
  disposed.set(true);
  disposeConnectors();
  model.dispose();
  listeners.dispose();
  disposeAgents();
  transformers.clear();
  endpoints.clear();
  endpointIdentifiers.clear();
  fireSystemEvent(new ManagerEvent(this,ManagerEvent.MANAGER_DISPOSED));
  transformers=null;
  endpoints=null;
  endpointIdentifiers=null;
  initialised.set(false);
  listeners.clear();
  instance=null;
  System.out.println(getEndSplash());
}
