{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("inboundEndpoint",MuleMessage.builder().payload("some data").replyTo("jms://client-reply").build());
  assertNotNull(result);
  assertEquals("got it!",getPayloadAsString(result));
  final Object foo=result.getInboundProperty("foo");
  assertNotNull(foo);
  assertEquals("bar",foo);
}
