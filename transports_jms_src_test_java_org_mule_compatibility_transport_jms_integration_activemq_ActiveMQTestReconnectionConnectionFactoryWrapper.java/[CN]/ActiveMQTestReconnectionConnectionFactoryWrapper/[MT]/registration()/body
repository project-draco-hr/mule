{
  calledMethods.add(new Date());
  if (!isEnabled()) {
    if (connection.getExceptionListener() != null) {
      try {
        connection.getExceptionListener().onException(new JMSException("Disabled"));
      }
 catch (      Exception e) {
        throw new JMSException("Disabled");
      }
    }
 else {
      throw new JMSException("Disabled");
    }
  }
}
