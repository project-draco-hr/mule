{
  String configName="nonexisting-config";
  final MetadataResult<MetadataKeysContainer> metadataKeysResult=metadataManager.getMetadataKeys(new ConfigurationId(configName));
  assertFailure(metadataKeysResult,format("Configuration named [%s] doesn't exist",configName),COMPONENT_NOT_FOUND,InvalidComponentIdException.class.getName());
}
