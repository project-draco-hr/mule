{
  ComponentId componentId=new ConfigurationId("dynamic-config");
  final MetadataResult<MetadataKeysContainer> metadataKeysResult=metadataManager.getMetadataKeys(componentId);
  assertFailure(metadataKeysResult,format("Component [%s] is not a MetadataKeyProvider, no information available",componentId),FailureCode.UNKNOWN,InvalidComponentIdException.class.getName());
}
