{
  keySet=new TreeSet<String>();
  scopedMap=new TreeMap<PropertyScope,Map<String,Object>>(new PropertyScope.ScopeComparator());
  scopedMap.put(PropertyScope.INVOCATION,new CaseInsensitiveHashMap(6));
  scopedMap.put(PropertyScope.INBOUND,new CaseInsensitiveHashMap(6));
  scopedMap.put(PropertyScope.OUTBOUND,new CaseInsensitiveHashMap(6));
}
