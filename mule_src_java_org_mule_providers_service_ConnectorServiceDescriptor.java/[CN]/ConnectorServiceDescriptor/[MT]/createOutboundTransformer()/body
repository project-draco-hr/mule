{
  if (outboundTransformer != null) {
    return outboundTransformer;
  }
  if (getDefaultOutboundTransformer() != null) {
    logger.info("Loading default outbound transformer: " + getDefaultOutboundTransformer());
    try {
      outboundTransformer=(UMOTransformer)ClassUtils.instanciateClass(getDefaultOutboundTransformer(),ClassUtils.NO_ARGS);
      return outboundTransformer;
    }
 catch (    Exception e) {
      throw new ConnectorFactoryException(new Message(Messages.FAILED_LOAD_X_TRANSFORMER_X,"outbound",getDefaultOutboundTransformer()),e);
    }
  }
  return null;
}
