{
  fireNotification(event.getFlowConstruct(),event,messageProcessor,null,MessageProcessorNotification.MESSAGE_PROCESSOR_PRE_INVOKE);
  MuleEvent result=null;
  MessagingException exceptionThrown=null;
  try {
    result=next.execute(messageProcessor,event);
  }
 catch (  MessagingException e) {
    exceptionThrown=e;
    throw e;
  }
 finally {
    fireNotification(event.getFlowConstruct(),result != null ? result : event,messageProcessor,exceptionThrown,MessageProcessorNotification.MESSAGE_PROCESSOR_POST_INVOKE);
  }
  return result;
}
