{
  OutputStream out;
  String streamName=endpoint.getEndpointURI().getAddress();
  if (STREAM_SYSTEM_OUT.equalsIgnoreCase(streamName)) {
    out=System.out;
  }
 else   if (STREAM_SYSTEM_ERR.equalsIgnoreCase(streamName)) {
    out=System.err;
  }
 else {
    out=getOutputStream();
  }
  return out;
}
