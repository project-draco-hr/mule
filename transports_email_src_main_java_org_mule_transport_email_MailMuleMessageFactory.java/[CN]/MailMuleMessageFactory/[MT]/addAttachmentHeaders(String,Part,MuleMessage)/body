{
  Map<String,String> headers=new HashMap<String,String>();
  for (Enumeration<?> e=part.getAllHeaders(); e.hasMoreElements(); ) {
    Header h=(Header)e.nextElement();
    headers.put(h.getName(),h.getValue());
  }
  if (headers.size() > 0) {
    muleMessage.setProperty(name + AbstractMailConnector.ATTACHMENT_HEADERS_PROPERTY_POSTFIX,headers,PropertyScope.INBOUND);
    muleMessage.setOutboundProperty(name + AbstractMailConnector.ATTACHMENT_HEADERS_PROPERTY_POSTFIX,headers);
  }
}
