{
  index++;
  if (stats.isInbound()) {
    col[index++]="Inbound";
  }
 else {
    col[index++]="Outbound";
  }
  col[index++]=String.valueOf(stats.getTotalReceived());
  col[index++]=String.valueOf(stats.getTotalRouted());
  col[index++]=String.valueOf(stats.getNotRouted());
  col[index++]=String.valueOf(stats.getCaughtMessages());
  return ++index;
}
