{
  compositeSource.setListener(listener);
  compositeSource.addSource(source);
  source.triggerSource();
  assertNull(listener.event);
  source.start();
  try {
    source.triggerSource();
    fail("Exception expected");
  }
 catch (  Exception e) {
  }
  assertNull(listener.event);
  compositeSource.start();
  source.triggerSource();
  assertEquals(testEvent,listener.event);
}
