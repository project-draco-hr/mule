{
  if (receiver.started.get()) {
    running=true;
    try {
      receiver.connected.whenTrue(null);
      receiver.poll();
    }
 catch (    InterruptedException e) {
      receiver.stop();
    }
catch (    Exception e) {
      receiver.handleException(e);
    }
 finally {
      running=false;
    }
  }
}
