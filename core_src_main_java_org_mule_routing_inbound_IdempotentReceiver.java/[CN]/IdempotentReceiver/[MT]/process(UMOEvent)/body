{
  if (isMatch(event)) {
    try {
      checkComponentName(event.getComponent().getDescriptor().getName());
    }
 catch (    IllegalArgumentException e) {
      throw new RoutingException(event.getMessage(),event.getEndpoint(),e);
    }
    Object id=this.getIdForEvent(event);
    try {
      this.storeId(id);
      return new UMOEvent[]{event};
    }
 catch (    IOException e) {
      throw new RoutingException(CoreMessages.failedToWriteMessageToStore(id,idStore.getAbsolutePath()),event.getMessage(),event.getEndpoint(),e);
    }
  }
 else {
    return null;
  }
}
