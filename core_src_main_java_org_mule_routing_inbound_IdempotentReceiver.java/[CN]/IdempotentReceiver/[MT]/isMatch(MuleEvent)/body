{
  if (!super.isMatch(event)) {
    return false;
  }
 else {
    if (store == null || assignedComponentName == null) {
synchronized (this) {
        this.initialize(event);
      }
    }
    try {
      return !store.contains(this.getIdForEvent(event));
    }
 catch (    Exception ex) {
      throw new RoutingException(event.getMessage(),event.getEndpoint(),ex);
    }
  }
}
