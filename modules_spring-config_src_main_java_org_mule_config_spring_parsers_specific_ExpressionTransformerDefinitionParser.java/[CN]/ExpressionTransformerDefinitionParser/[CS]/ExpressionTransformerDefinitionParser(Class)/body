{
  super(MuleOrphanDefinitionParser.ROOT_ELEMENT,new ExpressionTransformerOrphanDefinitionParser(messageProcessor,false));
  otherwise(new ExpressionTransformerChildDefinitionParser("messageProcessor",messageProcessor));
  registerPreProcessor(new CheckRequiredAttributesWhenNoChildren(new String[][]{{"expression"}},"return-argument")).registerPreProcessor(new CheckExclusiveAttributesAndChildren(new String[]{"expression"},new String[]{"return-argument"})).addIgnored("expression");
}
