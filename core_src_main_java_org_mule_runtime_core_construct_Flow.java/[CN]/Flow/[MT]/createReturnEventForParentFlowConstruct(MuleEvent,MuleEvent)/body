{
  if (result != null && !(result instanceof VoidMuleEvent)) {
    Error error=result.getError();
    result=MuleEvent.builder(result).flow(original.getFlowConstruct()).replyToHandler(original.getReplyToHandler()).replyToDestination(original.getReplyToDestination()).synchronous(original.isSynchronous()).error(error).build();
  }
  resetRequestContextEvent(result);
  return result;
}
