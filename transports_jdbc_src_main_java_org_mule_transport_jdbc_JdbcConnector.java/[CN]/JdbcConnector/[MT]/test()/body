{
  if (isConnected()) {
    return new ConnectorTestResult(TestResult.Status.SUCCESS);
  }
  Connection con=null;
  try {
    con=dataSource.getConnection();
    return new ConnectorTestResult(TestResult.Status.SUCCESS);
  }
 catch (  Exception e) {
    return new ConnectorTestResult(TestResult.Status.FAILURE,e.toString());
  }
 finally {
    if (con != null) {
      try {
        con.close();
      }
 catch (      SQLException e) {
      }
    }
  }
}
