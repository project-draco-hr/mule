{
  String queryTimeoutAsString=(String)endpoint.getProperty("queryTimeout");
  Integer queryTimeout=-1;
  try {
    queryTimeout=Integer.valueOf(queryTimeoutAsString);
  }
 catch (  NumberFormatException e) {
  }
  if (queryTimeout >= 0) {
    ExtendedQueryRunner extendedQueryRunner=new ExtendedQueryRunner(queryTimeout);
    extendedQueryRunner.setDataSource(this.queryRunner.getDataSource());
    return extendedQueryRunner;
  }
 else {
    return queryRunner;
  }
}
