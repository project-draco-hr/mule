{
  return new Node().parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_SENT,CLIENT)).parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_RECEIVED,SERVICE_1)).parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_DISPATCHED,CLIENT)).parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_RECEIVED,SERVICE_2)).parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_DISPATCHED,SERVICE_2)).parallel(new Node(MessageNotification.class,MessageNotification.MESSAGE_REQUESTED,CLIENT));
