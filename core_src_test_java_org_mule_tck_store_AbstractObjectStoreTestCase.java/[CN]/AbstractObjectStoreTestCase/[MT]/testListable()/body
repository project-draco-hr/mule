{
  Apple apple=new Apple();
  store.store(1,apple);
  store.store(2,apple);
  store.store(3,apple);
  List keys=store.allKeys();
  assertNotNull(keys);
  assertEquals(3,keys.size());
  assertEquals(apple,store.remove(2));
  keys=store.allKeys();
  assertEquals(2,keys.size());
  store.store(4,new Orange());
  keys=store.allKeys();
  assertEquals(3,keys.size());
}
