{
  Apple apple=new Apple();
  apple.setBitten(true);
  assertNull(store.retrieve(1));
  store.store(1,apple);
  Object obj=store.retrieve(1);
  assertNotNull(obj);
  assertTrue(obj instanceof Apple);
  assertTrue(((Apple)obj).isBitten());
  assertEquals(apple,store.remove(1));
  assertNull(store.retrieve(1));
}
