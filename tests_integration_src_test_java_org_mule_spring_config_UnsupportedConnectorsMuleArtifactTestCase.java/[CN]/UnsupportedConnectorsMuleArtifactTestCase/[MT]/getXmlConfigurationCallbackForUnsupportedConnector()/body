{
  return new XmlConfigurationCallback(){
    @Override public Element getGlobalElement(    String s){
      return null;
    }
    @Override public String getSchemaLocation(    String s){
      if (s != null) {
        int connectorNameStart=s.lastIndexOf("/") + 1;
        s=s + "/current/mule-" + s.substring(connectorNameStart)+ ".xsd";
        return s;
      }
 else       return null;
    }
  }
;
}
