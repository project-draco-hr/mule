{
  MuleManager.getConfiguration().setSynchronous(true);
  MuleManager.getConfiguration().getPoolingProfile().setInitialisationPolicy(PoolingProfile.POOL_INITIALISE_ONE_COMPONENT);
  MuleManager.getInstance().setModel(new SedaModel());
  callbackCalled=false;
  ConnectionFactory cf=getConnectionFactory();
  cnn=getSenderConnection();
  cnn.start();
  connector=createConnector();
  connector.setConnectionFactory(cf);
  MuleManager.getInstance().registerConnector(connector);
  currentMsg=null;
  eventCount=0;
}
