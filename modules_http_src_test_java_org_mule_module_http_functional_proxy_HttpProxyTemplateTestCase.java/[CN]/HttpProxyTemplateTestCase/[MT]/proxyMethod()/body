{
  handlerExtender=new EchoRequestHandlerExtender(){
    @Override protected String selectRequestPartToReturn(    org.eclipse.jetty.server.Request baseRequest){
      return baseRequest.getMethod();
    }
  }
;
  assertRequestOk(getProxyUrl("test?parameterName=parameterValue"),"GET");
  Response response=Request.Post(getProxyUrl("test?parameterName=parameterValue")).bodyString("Some Text",ContentType.DEFAULT_TEXT).connectTimeout(1000).execute();
  HttpResponse httpResponse=response.returnResponse();
  assertThat(httpResponse.getStatusLine().getStatusCode(),is(200));
  assertThat(IOUtils.toString(httpResponse.getEntity().getContent()),is("POST"));
}
