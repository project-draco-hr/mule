{
  return new ThreadFactory(){
    public Thread newThread(    Runnable runnable){
      Thread thread=new Thread(runnable,toString());
      if (isServerSide()) {
        thread.setDaemon(true);
      }
 else {
        thread.setPriority(Thread.NORM_PRIORITY + 2);
      }
      return thread;
    }
  }
;
}
