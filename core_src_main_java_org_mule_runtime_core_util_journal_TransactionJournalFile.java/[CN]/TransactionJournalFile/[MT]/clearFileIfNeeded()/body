{
  if (this.entries.isEmpty()) {
    if (clearFileMinimumSizeInBytes != null) {
      if (fileLength() > clearFileMinimumSizeInBytes) {
        clear();
        journalOperations=0;
      }
    }
 else     if (journalOperations > MINIMUM_ENTRIES_TO_CLEAR_FILE) {
      clear();
      journalOperations=0;
    }
  }
}
