{
  addReference(prototype);
  StringTokenizer st=new StringTokenizer(prototype,"(;)",true);
  while (st.hasMoreTokens()) {
    if (st.nextToken().equals("(")) {
      String token=st.nextToken();
      while (!token.equals(")")) {
        addReference(token);
        token=st.nextToken();
      }
      token=st.nextToken();
      addReference(token);
    }
  }
}
