{
  flow.setProcessingStrategyFactory(new AsynchronousProcessingStrategyFactory());
  flow.initialise();
  flow.start();
  flow.stop();
  flow.start();
  Event response=directInboundMessageSource.process(eventBuilder().message(InternalMessage.of(TEST_PAYLOAD)).synchronous(false).build());
  assertThat(response,not(nullValue()));
}
