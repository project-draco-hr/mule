{
  try {
    if (listenerObjectName != null)     mBeanServer.unregisterMBean(listenerObjectName);
  }
 catch (  Exception e) {
    logger.warn(e.getMessage(),e);
  }
  try {
    mBeanServer.unregisterMBean(broadcasterObjectName);
  }
 catch (  Exception e) {
    logger.warn(e.getMessage(),e);
  }
  super.dispose();
}
