{
  StringBuffer schemaLocation=new StringBuffer();
  for (  String key : schemaLocations.keySet()) {
    schemaLocation.append(key + " " + schemaLocations.get(key)+ "\n");
  }
  rootElement.addAttribute(org.dom4j.QName.get("schemaLocation","xsi","http://www.w3.org/2001/XMLSchema-instance"),schemaLocation.toString().trim());
}
