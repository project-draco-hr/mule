{
  if (event.getAction() == ManagerNotification.MANAGER_STARTED) {
    for (Iterator itr=components.iterator(); itr.hasNext(); ) {
      Component c=(Component)itr.next();
      try {
        muleContext.getRegistry().registerComponent(c);
      }
 catch (      MuleException e) {
        handleException(e);
      }
    }
  }
}
