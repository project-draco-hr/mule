{
  DataSource defaultDataSource=getDefaultDataSource();
  Connection connection=defaultDataSource.getConnection();
  try {
    testDatabase.executeUpdate(connection,"DELETE FROM ALIEN");
    final MuleEvent responseEvent=flowRunner("insertXmlType").withPayload(TEST_MESSAGE).withInboundProperty("name",Alien.ET.getName()).run();
    return responseEvent;
  }
  finally {
    if (connection != null) {
      connection.close();
    }
  }
}
