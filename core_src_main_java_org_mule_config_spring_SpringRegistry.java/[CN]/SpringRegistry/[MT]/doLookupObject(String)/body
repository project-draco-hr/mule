{
  if (StringUtils.isBlank(key)) {
    logger.warn(MessageFactory.createStaticMessage("Detected a lookup attempt with an empty or null key"),new Throwable().fillInStackTrace());
    return null;
  }
  try {
    return applicationContext.getBean(key);
  }
 catch (  NoSuchBeanDefinitionException e) {
    logger.debug(e);
    return null;
  }
catch (  Exception e) {
    logger.error(e);
    return null;
  }
}
