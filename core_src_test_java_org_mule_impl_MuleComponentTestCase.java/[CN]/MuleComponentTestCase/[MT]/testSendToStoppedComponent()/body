{
  UMOComponent comp=getTestComponent("myComponent",EchoComponent.class);
  assertTrue(!comp.isStarted());
  try {
    comp.dispatchEvent(getTestEvent("hello"));
    fail();
  }
 catch (  ComponentException e) {
  }
  try {
    comp.sendEvent(getTestEvent("hello"));
    fail();
  }
 catch (  ComponentException e) {
  }
}
