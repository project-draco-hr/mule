{
  String name="JcaService#" + endpointFactory.hashCode();
  Service service=new JcaService(muleContext);
  service.setName(name);
  ((CompositeMessageSource)service.getMessageSource()).addSource(endpoint);
  service.setComponent(new JcaComponent(endpointFactory,model.getEntryPointResolverSet(),service,new DelegateWorkManager(bootstrapContext.getWorkManager())));
  service.setModel(model);
  muleContext.getRegistry().registerService(service);
  return service;
}
