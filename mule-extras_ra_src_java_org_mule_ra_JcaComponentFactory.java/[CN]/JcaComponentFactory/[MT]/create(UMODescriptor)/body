{
  if (descriptor instanceof MuleDescriptor) {
    return new JcaComponent((MuleDescriptor)descriptor);
  }
 else {
    throw new IllegalArgumentException(new Message(Messages.CANT_SET_PROP_X_ON_X_OF_TYPE_X,MuleDescriptor.class.getName(),MuleComponent.class.getName(),descriptor.getClass().getName()).getMessage());
  }
}
