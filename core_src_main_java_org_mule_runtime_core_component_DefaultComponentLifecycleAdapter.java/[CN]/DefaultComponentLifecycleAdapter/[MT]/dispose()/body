{
  try {
    if (isDisposable) {
      Object o=componentObject;
      if (o != null) {
        try {
          disposeMethod.invoke(o);
        }
 catch (        InvocationTargetException e) {
          throw e.getTargetException();
        }
      }
    }
    componentObject=null;
  }
 catch (  Throwable e) {
    logger.error("failed to dispose: " + flowConstruct.getName(),e);
  }
  disposed=true;
}
