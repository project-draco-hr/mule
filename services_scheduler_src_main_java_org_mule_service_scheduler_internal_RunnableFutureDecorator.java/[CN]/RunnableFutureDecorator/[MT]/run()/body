{
  long startTime=0;
  if (logger.isDebugEnabled()) {
    startTime=System.nanoTime();
    logger.debug("Starting task " + this.toString() + "...");
  }
  started=true;
  try {
    task.run();
  }
  finally {
    wrapUp();
    if (logger.isDebugEnabled()) {
      logger.debug("Task " + this.toString() + " finished after "+ (System.nanoTime() - startTime)+ " nanoseconds");
    }
  }
}
