{
  MuleClient client=muleContext.getClient();
  client.dispatch("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",new DefaultMuleMessage(prepareOneWayTestMessage(),prepareOneWayWithSoapActionTestProperties(),muleContext));
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
