{
  MuleClient client=new MuleClient(muleContext);
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",prepareOneWayTestMessage(),prepareOneWayWithSoapActionTestProperties());
  assertNotNull(result);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
