{
  if (variant.equals(ConfigVariant.FLOW)) {
    testComponentLifecycle("MuleSingletonService","[setProperty, setMuleContext, jsr250 initialise, jsr250 initialise, start, stop, jsr250 dispose]");
  }
 else {
