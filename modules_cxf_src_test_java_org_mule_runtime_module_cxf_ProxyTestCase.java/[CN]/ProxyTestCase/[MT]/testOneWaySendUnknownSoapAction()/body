{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",new DefaultMuleMessage(prepareOneWayTestMessage(),prepareOneWayWithSoapActionTestProperties()),HTTP_REQUEST_OPTIONS);
  assertNotNull(result);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
