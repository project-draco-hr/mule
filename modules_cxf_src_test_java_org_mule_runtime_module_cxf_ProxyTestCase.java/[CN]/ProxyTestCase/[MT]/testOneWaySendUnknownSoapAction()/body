{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",MuleMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWayWithSoapActionTestProperties()).build(),HTTP_REQUEST_OPTIONS).getRight();
  assertNotNull(result);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
