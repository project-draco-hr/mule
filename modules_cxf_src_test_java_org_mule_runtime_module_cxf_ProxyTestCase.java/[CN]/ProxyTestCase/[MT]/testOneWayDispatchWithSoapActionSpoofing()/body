{
  MuleClient client=muleContext.getClient();
  client.dispatch("http://localhost:" + dynamicPort.getNumber() + "/services/onewayWithSoapAction",MuleMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWaySpoofingTestProperties()).build(),HTTP_REQUEST_OPTIONS);
  AsyncServiceWithSoapAction component=(AsyncServiceWithSoapAction)getComponent("asyncServiceWithSoapAction");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
