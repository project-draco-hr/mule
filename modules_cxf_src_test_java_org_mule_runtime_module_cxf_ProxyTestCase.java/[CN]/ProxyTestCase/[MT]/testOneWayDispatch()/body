{
  MuleClient client=muleContext.getClient();
  client.dispatch("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",MuleMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWayTestProperties()).build(),HTTP_REQUEST_OPTIONS);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertTrue(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
