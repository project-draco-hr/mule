{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",MuleMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWayTestProperties()).build(),HTTP_REQUEST_OPTIONS);
  assertNotNull(result);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertTrue(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
