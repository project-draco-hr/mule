{
  MuleClient client=muleContext.getClient();
  InternalMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/oneway",InternalMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWayTestProperties()).build(),HTTP_REQUEST_OPTIONS).getRight();
  assertNotNull(result);
  AsyncService component=(AsyncService)getComponent("asyncService");
  assertTrue(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
