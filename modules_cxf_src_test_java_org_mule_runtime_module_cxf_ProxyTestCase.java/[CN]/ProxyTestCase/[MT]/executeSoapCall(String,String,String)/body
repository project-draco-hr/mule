{
  Map<String,Serializable> props=new HashMap<>();
  if (soapAction != null) {
    props.put("SOAPAction",soapAction);
  }
  MuleClient client=muleContext.getClient();
  return client.send("http://localhost:" + dynamicPort.getNumber() + path,MuleMessage.builder().payload(msg).outboundProperties(props).build(),HTTP_REQUEST_OPTIONS).getRight();
}
