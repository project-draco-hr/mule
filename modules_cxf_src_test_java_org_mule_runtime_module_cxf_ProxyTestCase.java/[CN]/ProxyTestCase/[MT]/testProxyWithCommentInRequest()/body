{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/envelope-proxy",MuleMessage.builder().payload(msgWithComment).build(),HTTP_REQUEST_OPTIONS).getRight();
  String resString=getPayloadAsString(result);
  assertTrue(resString.contains(doGoogleSearch));
}
