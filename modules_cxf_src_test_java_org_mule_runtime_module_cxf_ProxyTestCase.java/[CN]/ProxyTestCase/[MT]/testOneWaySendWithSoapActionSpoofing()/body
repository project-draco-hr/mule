{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/onewayWithSoapAction",new DefaultMuleMessage(prepareOneWayTestMessage(),prepareOneWaySpoofingTestProperties()),HTTP_REQUEST_OPTIONS);
  assertNotNull(result);
  AsyncServiceWithSoapAction component=(AsyncServiceWithSoapAction)getComponent("asyncServiceWithSoapAction");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
