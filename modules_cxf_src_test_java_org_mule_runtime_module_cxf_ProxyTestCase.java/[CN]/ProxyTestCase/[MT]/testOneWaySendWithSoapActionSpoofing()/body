{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send("http://localhost:" + dynamicPort.getNumber() + "/services/onewayWithSoapAction",MuleMessage.builder().payload(prepareOneWayTestMessage()).outboundProperties(prepareOneWaySpoofingTestProperties()).build(),HTTP_REQUEST_OPTIONS).getRight();
  assertNotNull(result);
  AsyncServiceWithSoapAction component=(AsyncServiceWithSoapAction)getComponent("asyncServiceWithSoapAction");
  assertFalse(component.getLatch().await(1000,TimeUnit.MILLISECONDS));
}
