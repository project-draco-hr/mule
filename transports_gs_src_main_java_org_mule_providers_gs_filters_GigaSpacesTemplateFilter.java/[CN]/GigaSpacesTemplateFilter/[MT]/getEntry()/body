{
  if (entry == null) {
    if (expectedType == null) {
      return null;
    }
    Object entryType=ClassUtils.instanciateClass(expectedType,ClassUtils.NO_ARGS);
    if (fields.size() > 0) {
      BeanUtils.populate(entryType,fields);
    }
    entry=converter.toEntry(entryType,null);
  }
  return entry;
}
