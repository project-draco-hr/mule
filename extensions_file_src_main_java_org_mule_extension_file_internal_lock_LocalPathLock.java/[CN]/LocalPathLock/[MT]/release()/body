{
  if (lock != null) {
    try {
      lock.release();
    }
 catch (    IOException e) {
      if (LOGGER.isInfoEnabled()) {
        LOGGER.info(String.format("Found exception attempting to release lock on path '%s'",path),e);
      }
    }
 finally {
      lock=null;
    }
  }
  IOUtils.closeQuietly(channel);
  channel=null;
}
