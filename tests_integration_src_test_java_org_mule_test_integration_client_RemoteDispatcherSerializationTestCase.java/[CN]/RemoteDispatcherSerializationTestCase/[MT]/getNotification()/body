{
  Map<String,Object> props=new HashMap<String,Object>();
  props.put("key1","value1");
  Apple apple=new Apple();
  apple.wash();
  MuleMessage message=new DefaultMuleMessage(apple,props,muleContext);
  RemoteDispatcherNotification notification=new RemoteDispatcherNotification(message,RemoteDispatcherNotification.ACTION_SEND,"vm://foo");
  notification.setProperty("foo","bar");
  return notification;
}
