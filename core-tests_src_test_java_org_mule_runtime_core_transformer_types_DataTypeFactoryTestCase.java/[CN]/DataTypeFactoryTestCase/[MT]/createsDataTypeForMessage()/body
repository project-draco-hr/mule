{
  MuleContext muleContext=mock(MuleContext.class);
  when(muleContext.getConfiguration()).thenReturn(mock(MuleConfiguration.class));
  DataType<?> dataType=DataTypeFactory.createFromObject(new DefaultMuleMessage("test",null,null,null,muleContext,new SimpleDataType<>(String.class,"text/plain")));
  assertThat(dataType,like(String.class,"text/plain",null));
}
