{
  try {
    InterfaceClassScanner scanner=new InterfaceClassScanner(interfaceClass,classLoader);
    URL clasURL=getClassURL(name);
    ClassReader r=new ClassReader(clasURL.openStream());
    r.accept(scanner,0);
    return scanner;
  }
 catch (  IOException e) {
    throw new RuntimeException(name,e);
  }
}
