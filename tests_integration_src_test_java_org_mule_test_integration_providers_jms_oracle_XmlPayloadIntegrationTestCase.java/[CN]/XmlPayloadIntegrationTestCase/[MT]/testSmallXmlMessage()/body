{
  AQUtil.createOrReplaceXmlQueue(session,connector.getUsername(),TestConfig.QUEUE_XML);
  MuleUtil.sendXmlMessageToQueue(TestConfig.QUEUE_XML,TestConfig.XML_MESSAGE);
  assertXMLEqual(TestConfig.XML_MESSAGE,MuleUtil.receiveXmlMessageAsString(TestConfig.QUEUE_XML).trim());
  AQUtil.dropQueue(session,connector.getUsername(),TestConfig.QUEUE_XML,false);
}
