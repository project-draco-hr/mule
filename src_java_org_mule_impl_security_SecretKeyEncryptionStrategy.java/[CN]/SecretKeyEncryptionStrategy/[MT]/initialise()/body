{
  if (key == null) {
    if (keyFactory == null) {
      throw new InitialisationException(new Message(Messages.X_IS_NULL,"Key / KeyFactory"),this);
    }
 else {
      try {
        key=(byte[])keyFactory.create();
      }
 catch (      Exception e) {
        throw new InitialisationException(e,this);
      }
    }
  }
  super.initialise();
}
