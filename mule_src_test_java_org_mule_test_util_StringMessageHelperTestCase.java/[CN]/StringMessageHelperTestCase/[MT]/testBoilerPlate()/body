{
  List msgs=new ArrayList();
  msgs.add("This");
  msgs.add("is a");
  msgs.add("Boiler Plate");
  String plate=StringMessageHelper.getBoilerPlate(msgs,'*',12);
  assertEquals(Utility.CRLF + "************" + Utility.CRLF+ "* This     *"+ Utility.CRLF+ "* is a     *"+ Utility.CRLF+ "* Boiler   *"+ Utility.CRLF+ "* Plate    *"+ Utility.CRLF+ "************",plate);
}
