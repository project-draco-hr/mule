{
  MuleClient client=new MuleClient();
  MuleMessage reply=client.send("axis:http://localhost:63381/services/AxisService?method=receive",new DefaultMuleMessage("test",muleContext));
  assertNotNull(reply);
  assertNotNull(reply.getPayload());
  assertTrue(reply.getPayload() instanceof String);
  assertEquals("Received: test",reply.getPayloadAsString());
}
