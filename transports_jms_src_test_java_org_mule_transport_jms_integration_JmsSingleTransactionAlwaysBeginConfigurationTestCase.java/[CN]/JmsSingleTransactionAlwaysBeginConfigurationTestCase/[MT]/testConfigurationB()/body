{
  send(JMS_QUEUE_INPUT_CONF_B,DEFAULT_INPUT_MESSAGE,commit);
  assertPayloadEquals(DEFAULT_OUTPUT_MESSAGE,receive(JMS_QUEUE_OUTPUT_CONF_B,getTimeout(),rollback));
  assertPayloadEquals(DEFAULT_OUTPUT_MESSAGE,receive(JMS_QUEUE_OUTPUT_CONF_B,getTimeout(),commit));
  assertNull(receive(JMS_QUEUE_OUTPUT_CONF_B,getTimeout(),null));
}
