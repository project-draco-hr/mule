{
  send(JMS_QUEUE_INPUT_CONF_A,DEFAULT_INPUT_MESSAGE,commit);
  assertPayloadEquals(DEFAULT_OUTPUT_MESSAGE,receive(JMS_QUEUE_OUTPUT_CONF_A,getTimeout(),rollback));
  assertPayloadEquals(DEFAULT_OUTPUT_MESSAGE,receive(JMS_QUEUE_OUTPUT_CONF_A,getTimeout(),commit));
  assertNull(receive(JMS_QUEUE_OUTPUT_CONF_A,getTimeout(),null));
}
