{
  StringToEmailMessage trans=new StringToEmailMessage();
  UMOImmutableEndpoint endpoint=managementContext.getRegistry().lookupEndpointFactory().getOutboundEndpoint("smtp://a:a@a.com",managementContext);
  try {
    endpoint.getConnector().initialise();
  }
 catch (  AlreadyInitialisedException e) {
    assertNotNull(e);
  }
  trans.setEndpoint(endpoint);
  return trans;
}
