{
  if (config != null) {
    int numConfigs=getMuleConfigurations().size();
    if (numConfigs > 0) {
      IMuleConfiguration last=(IMuleConfiguration)getMuleConfigurations().get(numConfigs - 1);
      if (config.getId().equals(last.getId())) {
        return true;
      }
    }
  }
  return false;
}
