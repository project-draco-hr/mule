def printer(self, num, coin):
    if num:
        if (coin in self.coin_lookup):
            if (self.result == ''):
                self.result = ('%1.0f %s' % (str(num), self.coin_lookup[coin]))
            else:
                self.result = ('%s, %1.0f %s' % (self.result, str(num), self.coin_lookup[coin]))
