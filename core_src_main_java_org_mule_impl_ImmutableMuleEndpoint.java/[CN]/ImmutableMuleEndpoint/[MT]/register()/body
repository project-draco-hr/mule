{
  ComponentReference ref=MuleManager.getInstance().getRegistry().getComponentReferenceInstance();
  if (connector == null || connector.getRegistryId() == null)   throw new RegistrationException("Unable to find the endpoint's connector registry ID");
  ref.setParentId(connector.getRegistryId());
  ref.setType("UMOImmutableEndpoint");
  ref.setComponent(this);
  registryId=MuleManager.getInstance().getRegistry().registerComponent(ref);
}
