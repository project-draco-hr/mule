{
  ImmutableEndpoint endpoint=muleContext.getRegistry().lookupEndpointBuilder("endpointWithXaTransaction").buildInboundEndpoint();
  assertNotNull(endpoint);
  assertEquals(XaTransactionFactory.class,endpoint.getTransactionConfig().getFactory().getClass());
  assertEquals(MuleTransactionConfig.ACTION_ALWAYS_BEGIN,endpoint.getTransactionConfig().getAction());
}
