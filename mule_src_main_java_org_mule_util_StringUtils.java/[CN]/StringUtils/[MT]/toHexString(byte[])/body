{
  if (bytes == null) {
    return null;
  }
  int numBytes=bytes.length;
  StringBuffer str=new StringBuffer(numBytes * 2);
  for (int i=0; i < numBytes; i++) {
    str.append(HEX_CHARACTERS.charAt(bytes[i] >>> 4 & 0x0f));
    str.append(HEX_CHARACTERS.charAt(bytes[i] & 0x0f));
  }
  return str.toString();
}
