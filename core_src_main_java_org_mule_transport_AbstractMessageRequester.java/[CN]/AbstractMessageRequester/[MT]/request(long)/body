{
  try {
    connectionStrategy.connect(this);
    if (connector.isEnableMessageEvents()) {
      connector.fireNotification(new MessageNotification(null,endpoint,null,MessageNotification.MESSAGE_REQUESTED));
    }
    MuleMessage result=doRequest(timeout);
    if (result != null && connector.isEnableMessageEvents()) {
      connector.fireNotification(new MessageNotification(result,endpoint,null,MessageNotification.MESSAGE_RECEIVED));
    }
    return result;
  }
 catch (  DispatchException e) {
    disposeAndLogException();
    throw e;
  }
catch (  Exception e) {
    disposeAndLogException();
    throw new ReceiveException(endpoint,timeout,e);
  }
}
