{
  String pathInfo=context.getEndpointURI().getPath();
  if (pathInfo == null || "".equals(pathInfo))   return context.getEndpointURI().getHost();
  String serviceName;
  int i=pathInfo.lastIndexOf("/");
  if (i > -1) {
    serviceName=pathInfo.substring(i + 1);
  }
 else {
    serviceName=pathInfo;
  }
  return serviceName;
}
