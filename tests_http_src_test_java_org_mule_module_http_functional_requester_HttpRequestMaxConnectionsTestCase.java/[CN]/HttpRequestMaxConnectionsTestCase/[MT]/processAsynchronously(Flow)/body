{
  Thread thread=new Thread(new Runnable(){
    @Override public void run(){
      try {
        FlowRunner runner=flowRunner("limitedConnections");
        setEventTimeout(runner.buildEvent(),RECEIVE_TIMEOUT);
        runner.run();
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
);
  thread.start();
  return thread;
}
