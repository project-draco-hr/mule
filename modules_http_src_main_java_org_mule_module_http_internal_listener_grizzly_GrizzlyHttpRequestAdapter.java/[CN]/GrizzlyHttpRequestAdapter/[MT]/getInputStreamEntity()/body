{
  if (this.grizzlyContent == null) {
    return null;
  }
  if (getHeaderValue(HttpHeaders.Names.TRANSFER_ENCODING) != null) {
    return new InputStreamHttpEntity(grizzlyContent);
  }
  if (contentLength > 0) {
    return new InputStreamHttpEntity(contentLength,grizzlyContent);
  }
  return null;
}
