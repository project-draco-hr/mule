{
  assureStarted();
synchronized (context) {
    if (logger.isDebugEnabled()) {
      logger.debug("Beginning transaction " + context);
    }
    doBegin(context);
    context.status=Status.STATUS_ACTIVE;
    if (logger.isDebugEnabled()) {
      logger.debug("Begun transaction " + context);
    }
  }
  globalTransactions.add(context);
}
