{
  try {
    return compiledExpressionsCache.get(expression,new Callable<Serializable>(){
      @Override public Serializable call(){
        return MVEL.compileExpression(expression,new ParserContext(parserContext.getParserConfiguration()));
      }
    }
);
  }
 catch (  UncheckedExecutionException e) {
    throw (RuntimeException)e.getCause();
  }
catch (  ExecutionException e) {
    throw new MuleRuntimeException(e);
  }
}
