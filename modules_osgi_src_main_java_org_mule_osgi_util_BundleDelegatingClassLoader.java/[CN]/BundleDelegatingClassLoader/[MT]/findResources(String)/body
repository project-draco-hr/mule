{
  boolean trace=log.isTraceEnabled();
  if (trace)   log.trace("Looking for resources " + name);
  Enumeration enm=this.backingBundle.getResources(name);
  if (trace && enm != null && enm.hasMoreElements())   log.trace("Found resource " + name + " at "+ this.backingBundle.getLocation());
  return enm;
}
