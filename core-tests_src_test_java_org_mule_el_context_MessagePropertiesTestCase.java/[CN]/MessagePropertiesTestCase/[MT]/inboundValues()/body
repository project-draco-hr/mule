{
  DefaultMuleMessage message=(DefaultMuleMessage)event.getMessage();
  message.setInboundProperty("foo","abc");
  message.setInboundProperty("bar","xyz");
  Collection<DataHandler> values=(Collection<DataHandler>)evaluate("message.inboundProperties.values()",event);
  assertEquals(2,values.size());
  values.contains("abc");
  values.contains("xyz");
}
