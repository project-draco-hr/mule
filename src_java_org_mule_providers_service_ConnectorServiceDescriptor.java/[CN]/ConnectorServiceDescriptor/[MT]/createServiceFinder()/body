{
  if (serviceFinder == null) {
    return null;
  }
  if (connectorServiceFinder == null) {
    try {
      connectorServiceFinder=(ConnectorServiceFinder)ClassUtils.instanciateClass(serviceFinder,ClassUtils.NO_ARGS);
    }
 catch (    Exception e) {
      throw new ConnectorServiceException(new Message(Messages.CANT_INSTANCIATE_FINDER_X,serviceFinder),e);
    }
  }
  return connectorServiceFinder;
}
