{
  List<ConfigurationProvider> providers=extensionRegistry.getConfigurationProviders(extensionModel);
  int matches=providers.size();
  if (matches == 1) {
    return Optional.of(providers.get(0));
  }
 else   if (matches > 1) {
    throw new IllegalStateException(String.format("No config-ref was specified for operation of extension '%s', but %d are registered. Please specify which to use",extensionModel.getName(),matches));
  }
  return Optional.empty();
}
