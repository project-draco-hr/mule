{
  Properties properties=new Properties();
  properties.put("testKey1","testValue1");
  properties.put("testKey2","testValue2");
  try {
    MuleContext muleContext=muleContextFactory.createMuleContext("my-resource.xml",properties);
  }
 catch (  ConfigurationException e) {
    assertEquals("No suitable configuration builder for resource \"[my-resource.xml]\" found.  Check you have configuration module ion your classpath and are using correct file extension.",e.getCause().getMessage());
  }
  assertNull(muleContext);
}
