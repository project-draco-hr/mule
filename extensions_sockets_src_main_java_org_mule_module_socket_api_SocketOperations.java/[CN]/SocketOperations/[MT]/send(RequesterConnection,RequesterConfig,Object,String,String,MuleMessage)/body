{
  SocketClient client=connection.getClient();
  if (outputEncoding == null) {
    outputEncoding=config.getDefaultEncoding();
  }
  client.write(content,outputEncoding);
  return Boolean.valueOf(hasResponse) ? OperationResult.builder().output(client.read()).attributes(client.getAttributes()).build() : OperationResult.builder(muleMessage).build();
}
