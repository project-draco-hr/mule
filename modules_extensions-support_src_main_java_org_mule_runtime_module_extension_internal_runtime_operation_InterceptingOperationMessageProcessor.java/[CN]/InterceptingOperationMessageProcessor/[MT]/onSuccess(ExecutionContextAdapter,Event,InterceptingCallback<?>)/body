{
  LOGGER.debug("Intercepting operation '{}' success",operationContext.getComponentModel().getName());
  try {
    interceptingCallback.onSuccess(resultEvent.getMessage());
  }
 catch (  Exception e) {
    throw new MessagingException(createStaticMessage(format("Intercepting operation '%s' executed intercepted chain but failed to process the obtained response",operationContext.getComponentModel().getName())),resultEvent,e,this);
  }
}
