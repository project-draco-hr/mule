{
  Iterator i=resources.entrySet().iterator();
  while (i.hasNext()) {
    Map.Entry entry=(Map.Entry)i.next();
    if (entry.getValue() instanceof MuleXaObject) {
      try {
        ((MuleXaObject)entry.getValue()).delist();
      }
 catch (      Exception e) {
        logger.error("Cann't delist resource " + entry.getValue() + " "+ e);
      }
    }
  }
}
