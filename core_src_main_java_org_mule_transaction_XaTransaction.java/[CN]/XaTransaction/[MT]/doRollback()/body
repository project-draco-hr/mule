{
  try {
    TransactionManager txManager=MuleServer.getMuleContext().getTransactionManager();
    delistResources();
    txManager.rollback();
  }
 catch (  SystemException e) {
    throw new TransactionRollbackException(e);
  }
catch (  Exception e) {
    throw new TransactionRollbackException(e);
  }
 finally {
    this.transaction=null;
    closeResources();
  }
}
