{
  boolean found=false;
  for (  Class<? extends ServerNotification> event : eventToSenders.keySet()) {
    if (event.isAssignableFrom(notfnClass)) {
      found=true;
      for (      Sender sender : eventToSenders.get(event)) {
        sender.dispatch(notification);
      }
    }
  }
  return found;
}
