{
  final String wsdl=muleClient.request("http://localhost:6099/simple-maths?wsdl",getTestTimeoutSecs() * 1000L).getPayloadAsString();
  assertTrue(wsdl.contains("addTen"));
  final int result=(Integer)muleClient.send("wsdl-cxf:http://localhost:6099/simple-maths?wsdl&method=addTen",90,null,getTestTimeoutSecs() * 1000).getPayload();
  assertEquals(100,result);
}
