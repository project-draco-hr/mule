{
  if (null != map) {
    Iterator mapNames=new TreeMap(map).keySet().iterator();
    while (mapNames.hasNext()) {
      String name=(String)mapNames.next();
      String value=(String)map.get(name);
      int pos=names.indexOf(name);
      if (pos >= 0) {
        values.set(pos,value);
      }
 else {
        names.add(name);
        values.add(value);
      }
    }
  }
}
