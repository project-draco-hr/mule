{
  RequestLine requestLine=request.getRequestLine();
  String method=requestLine.getMethod();
  if (method.equals(METHOD_GET) || method.equals(METHOD_HEAD) || method.equals(METHOD_POST)|| method.equals(METHOD_OPTIONS)|| method.equals(METHOD_PUT)|| method.equals(METHOD_DELETE)|| method.equals(METHOD_TRACE)|| method.equals(METHOD_CONNECT)|| method.equals(METHOD_PATCH)) {
    return doRequest(request);
  }
 else {
    return doBad(requestLine);
  }
}
