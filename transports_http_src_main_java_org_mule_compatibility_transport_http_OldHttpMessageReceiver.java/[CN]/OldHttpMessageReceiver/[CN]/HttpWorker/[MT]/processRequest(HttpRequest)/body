{
  RequestLine requestLine=request.getRequestLine();
  String method=requestLine.getMethod();
  if (method.equals(HttpConstants.METHOD_GET) || method.equals(HttpConstants.METHOD_HEAD) || method.equals(HttpConstants.METHOD_POST)|| method.equals(HttpConstants.METHOD_OPTIONS)|| method.equals(HttpConstants.METHOD_PUT)|| method.equals(HttpConstants.METHOD_DELETE)|| method.equals(HttpConstants.METHOD_TRACE)|| method.equals(HttpConstants.METHOD_CONNECT)|| method.equals(HttpConstants.METHOD_PATCH)) {
    return doRequest(request);
  }
 else {
    return doBad(requestLine);
  }
}
