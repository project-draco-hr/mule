{
  MuleMessage message;
  if (response instanceof MuleMessage) {
    message=(MuleMessage)response;
  }
 else {
    message=new DefaultMuleMessage(response,getEndpoint().getMuleContext());
  }
  return (HttpResponse)connector.getMuleContext().getTransformationService().applyTransformers(message,null,defaultResponseTransformers).getPayload();
}
