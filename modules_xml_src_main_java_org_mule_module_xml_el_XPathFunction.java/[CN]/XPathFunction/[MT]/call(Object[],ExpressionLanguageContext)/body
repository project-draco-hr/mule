{
  try {
    if (params.length == 1) {
      return muleContext.getExpressionManager().evaluate("xpath-branch:" + params[0],new DefaultMuleMessage(((MessageContext)context.getVariable("message")).getPayload(),muleContext));
    }
 else {
      return muleContext.getExpressionManager().evaluate("xpath-node:" + params[0],new DefaultMuleMessage(params[1],muleContext));
    }
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
