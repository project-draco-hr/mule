{
  if (event != null) {
    if (FLOW.equals(name)) {
      return new SimpleValueResolver(new FlowContext(event.getFlowConstruct()));
    }
 else     if (MVELExpressionLanguageContext.MULE_EVENT_INTERNAL_VARIABLE.equals(name)) {
      return new SimpleValueResolver(event);
    }
  }
  return null;
}
