{
  ExpiryMonitor monitor=new ExpiryMonitor("test",100,null,false);
  Expirable e=new Expirable(){
    public void expired(){
      expired=true;
    }
  }
;
  monitor.addExpirable(800,TimeUnit.MILLISECONDS,e);
  Thread.sleep(300);
  assertTrue(!expired);
  Thread.sleep(800);
  assertTrue(expired);
  assertTrue(!monitor.isRegistered(e));
}
