{
  ExpiryMonitor monitor=new ExpiryMonitor("test",100,null,false);
  Expirable e=new Expirable(){
    public void expired(){
      expired=true;
    }
  }
;
  monitor.addExpirable(1000,TimeUnit.MILLISECONDS,e);
  Thread.sleep(200);
  assertTrue(!expired);
  Thread.sleep(200);
  monitor.removeExpirable(e);
  Thread.sleep(800);
  assertTrue(!expired);
  assertTrue(!monitor.isRegistered(e));
}
