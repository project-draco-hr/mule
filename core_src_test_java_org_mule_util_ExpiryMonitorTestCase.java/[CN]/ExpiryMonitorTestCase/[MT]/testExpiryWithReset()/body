{
  ExpiryMonitor monitor=new ExpiryMonitor("test",100);
  Expirable e=new Expirable(){
    public void expired(){
      expired=true;
    }
  }
;
  monitor.addExpirable(600,TimeUnit.MILLISECONDS,e);
  Thread.sleep(200);
  assertTrue(!expired);
  monitor.resetExpirable(e);
  Thread.sleep(200);
  assertTrue(!expired);
  Thread.sleep(600);
  assertTrue(expired);
  assertTrue(!monitor.isRegistered(e));
}
