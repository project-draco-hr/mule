{
  EList warnings=ecoreResource.getWarnings();
  EList errors=ecoreResource.getErrors();
  MuleCorePlugin.getDefault().clearMarkers(eclipseResource);
  if (warnings.size() > 0) {
    Iterator it=warnings.iterator();
    while (it.hasNext()) {
      Diagnostic diag=(Diagnostic)it.next();
      Integer lineNumber=new Integer(diag.getLine());
      MuleCorePlugin.getDefault().createMarker(eclipseResource,IMarker.SEVERITY_WARNING,diag.getMessage(),lineNumber);
    }
  }
  if (errors.size() > 0) {
    Iterator it=errors.iterator();
    while (it.hasNext()) {
      Diagnostic diag=(Diagnostic)it.next();
      Integer lineNumber=new Integer(diag.getLine());
      MuleCorePlugin.getDefault().createMarker(eclipseResource,IMarker.SEVERITY_ERROR,diag.getMessage(),lineNumber);
    }
  }
  return Status.OK_STATUS;
}
