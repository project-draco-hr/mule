{
  try {
    return (Integer)processTemplate.execute(new ProcessCallback<Object,Object>(){
      @Override @SuppressWarnings("unchecked") public Object process(      Object object) throws Exception {
        return delegate.getTotalResults(object);
      }
      @Override public List<Class<? extends Exception>> getManagedExceptions(){
        return managedExceptions;
      }
      @Override public boolean isProtected(){
        return isProtected;
      }
    }
,originalMessageProcessor,originalEvent);
  }
 catch (  Exception e) {
    throw new MuleRuntimeException(e);
  }
}
