{
  for (  String str : new String[]{"A","B","C"}) {
    Flow flow=getTestFlow();
    Event event=Event.builder(DefaultEventContext.create(flow,TEST_CONNECTOR)).message(InternalMessage.builder().payload(str).build()).exchangePattern(ONE_WAY).flow(flow).session(new DefaultMuleSession()).build();
    events.put(buildQueueKey(event,getTestFlow(),muleContext),event);
  }
}
