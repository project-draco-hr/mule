{
  for (  String str : new String[]{"A","B","C"}) {
    Flow flow=getTestFlow();
    MuleEvent event=MuleEvent.builder(DefaultMessageContext.create(flow,TEST_CONNECTOR)).message(MuleMessage.builder().payload(str).build()).exchangePattern(ONE_WAY).flow(flow).session(new DefaultMuleSession()).build();
    events.put(buildQueueKey(event,getTestFlow(),muleContext),event);
  }
}
