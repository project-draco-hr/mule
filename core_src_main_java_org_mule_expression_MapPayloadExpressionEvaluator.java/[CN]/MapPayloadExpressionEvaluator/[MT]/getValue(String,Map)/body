{
  boolean required;
  if (key.endsWith(OPTIONAL_ARGUMENT)) {
    key=key.substring(0,key.length() - OPTIONAL_ARGUMENT.length());
    required=false;
  }
 else {
    required=true;
  }
  Object val=map.get(key);
  if (val != null) {
    return val;
  }
 else   if (required) {
    throw new RequiredValueException(CoreMessages.expressionEvaluatorReturnedNull(NAME,key));
  }
  return null;
}
