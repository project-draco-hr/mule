{
  String configFilePath="/org/mule/transport/cxf/support/test-proxy-mule-config-no-namespace.xml";
  try {
    startServer(configFilePath);
  }
 catch (  RuntimeException e) {
    Throwable rootCause=ExceptionUtils.getRootCause(e);
    assertTrue("Exception must be of type " + ComponentNotFoundRuntimeException.class + ", instead of "+ rootCause,rootCause instanceof ComponentNotFoundRuntimeException);
  }
}
