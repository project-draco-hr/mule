{
  MuleMessage message=muleContext.getClient().send("vm://service","hello",null);
  assertNotNull(message);
  assertNotNull(message.getExceptionPayload());
  assertEquals(EntryPointNotFoundException.class,message.getExceptionPayload().getRootException().getClass());
  assertTrue(ExceptionUtils.getRootCauseMessage(message.getExceptionPayload().getException()).indexOf("Found too many possible methods on object") > -1);
}
