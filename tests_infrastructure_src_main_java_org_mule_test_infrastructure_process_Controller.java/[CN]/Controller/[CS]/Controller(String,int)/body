{
  this.muleHome=muleHome;
  this.muleBin=getMuleBin();
  this.domainsDir=new File(muleHome + "/domains");
  this.appsDir=new File(muleHome + "/apps/");
  this.libsDir=new File(muleHome + "/lib/user");
