{
  UMOServerEvent event;
  while (!disposed) {
    try {
      event=(UMOServerEvent)eventQueue.poll(5000,TimeUnit.MILLISECONDS);
      if (event != null) {
        notifyListeners(event);
      }
    }
 catch (    InterruptedException e) {
      if (!disposed) {
        logger.error("Failed to take event from server event queue",e);
      }
    }
  }
}
