{
  for (int i=0; i < processors.size(); i++) {
    MessageProcessor processor=processors.get(i);
    event=messageProcessorExecutionTemplate.execute(processor,event);
    if (event == null) {
      return null;
    }
 else     if (event instanceof VoidMuleEvent) {
      return event;
    }
  }
  return event;
}
