{
  if (isSourceTypeSupported(UMOMessage.class,true)) {
    src=context.getMessage();
  }
 else {
    if (src instanceof byte[]) {
      return src;
    }
  }
  try {
    return SerializationUtils.serialize((Serializable)src);
  }
 catch (  Exception e) {
    throw new TransformerException(this,e);
  }
}
