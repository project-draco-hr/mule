{
  if (id == null) {
    throw new IllegalArgumentException(CoreMessages.objectIsNull("id").toString());
  }
synchronized (store) {
    if (store.values().contains(id)) {
      return false;
    }
    boolean written=false;
    while (!written) {
      written=(store.putIfAbsent(new Long(Utils.nanoTime()),id) == null);
    }
    return true;
  }
}
