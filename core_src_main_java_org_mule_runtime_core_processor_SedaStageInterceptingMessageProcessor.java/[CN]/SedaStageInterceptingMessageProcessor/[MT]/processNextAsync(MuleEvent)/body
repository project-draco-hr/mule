{
  try {
    if (isStatsEnabled()) {
      queueStatistics.incQueuedEvent();
    }
    enqueue(event);
  }
 catch (  Exception e) {
    throw new FailedToQueueEventException(CoreMessages.interruptedQueuingEventFor(getStageDescription()),event,e,this);
  }
  if (logger.isTraceEnabled()) {
    logger.trace("MuleEvent added to queue for: " + getStageDescription());
  }
}
