{
  if (RegistryContext.getRegistry() != null) {
    try {
      RegistryContext.getRegistry().unregisterObject(name);
    }
 catch (    UMOException e) {
    }
    try {
      RegistryContext.getRegistry().registerObject(name,value);
    }
 catch (    RegistrationException e) {
      logger.error(e);
    }
  }
}
