{
  boolean oldSingleton=singleton;
  boolean oldSingletonESet=singletonESet;
  singleton=SINGLETON_EDEFAULT;
  singletonESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,SchemaPackage.MULE_DESCRIPTOR_TYPE__SINGLETON,oldSingleton,SINGLETON_EDEFAULT,oldSingletonESet));
