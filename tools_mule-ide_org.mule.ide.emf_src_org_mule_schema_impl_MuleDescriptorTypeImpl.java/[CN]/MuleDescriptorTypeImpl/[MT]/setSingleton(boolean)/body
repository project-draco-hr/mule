{
  boolean oldSingleton=singleton;
  singleton=newSingleton;
  boolean oldSingletonESet=singletonESet;
  singletonESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.MULE_DESCRIPTOR_TYPE__SINGLETON,oldSingleton,singleton,!oldSingletonESet));
}
