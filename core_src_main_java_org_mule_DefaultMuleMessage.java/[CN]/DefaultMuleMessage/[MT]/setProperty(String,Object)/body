{
  assertAccess(WRITE);
  if (key != null) {
    if (value != null) {
      properties.setProperty(key,value,PropertyScope.OUTBOUND);
    }
 else {
      logger.warn("setProperty(key, value) called with null value; removing key: " + key,new Throwable());
      properties.removeProperty(key);
    }
    updateDataTypeWithProperty(key,value);
  }
 else {
    logger.warn("setProperty(key, value) ignored because of null key for object: " + value,new Throwable());
  }
}
