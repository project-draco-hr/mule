{
  assertAccess(WRITE);
  if (key != null) {
    if (value != null) {
      properties.setProperty(key,value,PropertyScope.OUTBOUND);
    }
 else {
      logger.warn("setProperty(key, value) called with null value; removing key: " + key + "; please report the following stack trace to "+ MuleManifest.getDevListEmail(),new Throwable());
      properties.removeProperty(key);
    }
  }
 else {
    logger.warn("setProperty(key, value) ignored because of null key for object: " + value + "; please report the following stack trace to "+ MuleManifest.getDevListEmail(),new Throwable());
  }
}
