{
  int current=getPhaseIndex(currentPhase);
  int end=getPhaseIndex(phase);
  if (end < current) {
    logger.warn("Phase: " + phase + " has already been fired for this object: "+ object);
    return;
  }
  LifecyclePhase li;
  current++;
  while (current <= end) {
    li=getPhaseForIndex(current);
    li.applyLifecycle(object);
    current++;
  }
}
