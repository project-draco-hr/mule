{
  if (!getMessageProcessors().isEmpty()) {
    try {
      event.getMessage().setExceptionPayload(new DefaultExceptionPayload(t));
      MuleEvent result=configuredMessageProcessors.process(event);
      processOutboundRouterStatistics(event);
      return result;
    }
 catch (    Exception e) {
      logFatal(event,e);
    }
  }
  return event;
}
