{
  MuleClient client=muleContext.getClient();
  MuleMessage message=client.send(String.format("http://localhost:%s?param1=1&param2=3",port1.getNumber()),MuleMessage.builder().payload("message").mediaType(APPLICATION_X_WWW_FORM_URLENCODED).build(),newOptions().method(POST.name()).build());
  assertThat(message.getExceptionPayload(),nullValue());
}
