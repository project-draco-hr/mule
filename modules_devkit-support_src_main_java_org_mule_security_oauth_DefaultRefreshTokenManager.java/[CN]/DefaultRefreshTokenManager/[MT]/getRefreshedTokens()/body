{
  if (this.refreshedTokens == null) {
    ObjectStoreManager osManager=this.muleContext.getObjectStoreManager();
    this.refreshedTokens=osManager.getObjectStore("RefreshTokenStore",false,ObjectStoreManager.NO_MAX_ENTRIES,this.minRefreshInterval,this.minRefreshInterval);
  }
  return this.refreshedTokens;
}
