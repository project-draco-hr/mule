{
  assertAccess(READ);
  String encoding=null;
  if (getDataType() != null) {
    encoding=getDataType().getEncoding();
    if (encoding != null) {
      return encoding;
    }
  }
  encoding=getOutboundProperty(MULE_ENCODING_PROPERTY);
  if (encoding != null) {
    return encoding;
  }
 else {
    return getDefaultEncoding(muleContext);
  }
}
