{
  if (fullExpression == null) {
    if (evaluator.equalsIgnoreCase("custom")) {
      evaluator=customEvaluator;
    }
    fullExpression=MessageFormat.format("{0}{1}:{2}{3}",ExpressionManager.DEFAULT_EXPRESSION_PREFIX,evaluator,expression,ExpressionManager.DEFAULT_EXPRESSION_POSTFIX);
    logger.debug("Full expression for EndpointSelector is: " + fullExpression);
  }
  return fullExpression;
}
