{
  try {
    return contexts.get(computeKey(classLoader),new Callable<LoggerContext>(){
      @Override public LoggerContext call() throws Exception {
        return buildContext(classLoader);
      }
    }
);
  }
 catch (  ExecutionException e) {
    throw new MuleRuntimeException(MessageFactory.createStaticMessage("Could not init logger context "),e);
  }
}
