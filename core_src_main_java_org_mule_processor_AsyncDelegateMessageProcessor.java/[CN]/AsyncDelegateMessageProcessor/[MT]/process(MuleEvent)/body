{
  if (event.isTransacted()) {
    throw new MessagingException(CoreMessages.asyncDoesNotSupportTransactions(),event,this);
  }
  if (target != null) {
    MuleEvent newEvent=new DefaultMuleEvent((MuleMessage)((ThreadSafeAccess)event.getMessage()).newThreadCopy(),event,false);
    target.process(newEvent);
  }
  return event;
}
