{
  if (delegate != null) {
    try {
      workManagerSource.getWorkManager().scheduleWork(new AsyncMessageProcessorWorker(event),WorkManager.INDEFINITE,null,new AsyncWorkListener(delegate));
    }
 catch (    Exception e) {
      new MessagingException(CoreMessages.errorSchedulingMessageProcessorForAsyncInvocation(delegate),event,e);
    }
  }
  return event;
}
