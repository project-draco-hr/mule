{
  if (isDisposed()) {
    return;
  }
  if (started.get()) {
    if (logger.isInfoEnabled())     logger.info("Stopping Connector: " + getClass().getName());
    stopConnector();
    started.set(false);
    if (logger.isInfoEnabled())     logger.info("Connector " + getClass().getName() + " has been stopped");
  }
}
