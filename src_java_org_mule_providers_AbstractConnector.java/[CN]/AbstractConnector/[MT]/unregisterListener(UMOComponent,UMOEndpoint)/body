{
  if (endpoint == null || component == null || endpoint.getEndpointURI() == null)   throw new IllegalArgumentException("The endpoint and component and endpointUri cannot be null when you unregister a listener");
  UMOEndpointURI endpointUri=endpoint.getEndpointURI();
  if (logger.isInfoEnabled())   logger.info("removing listener on endpointUri: " + endpointUri);
  UMOMessageReceiver receiver=(UMOMessageReceiver)receivers.remove(getReceiverKey(component,endpoint));
  if (receiver != null) {
    destroyReceiver(receiver,endpoint);
    receiver.dispose();
  }
}
