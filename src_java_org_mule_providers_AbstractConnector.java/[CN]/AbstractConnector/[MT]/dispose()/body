{
  disposing.set(true);
  if (logger.isInfoEnabled()) {
    logger.info("Disposing Connector: " + getClass().getName());
    logger.debug("Disposing Receivers");
  }
  disposeReceivers();
  disposeDispatchers();
  doDispose();
  disposed.set(true);
  if (logger.isInfoEnabled()) {
    logger.info("Connector " + getClass().getName() + " has been disposed.");
  }
  receivers=null;
  dispatchers=null;
}
