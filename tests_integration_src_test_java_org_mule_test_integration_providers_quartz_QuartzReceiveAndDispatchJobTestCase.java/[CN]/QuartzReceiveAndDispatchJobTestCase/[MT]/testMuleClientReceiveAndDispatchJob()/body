{
  countDown=new CountDownLatch(3);
  new MuleClient().send("vm://event.queue","quartz test",null);
  new MuleClient().send("vm://event.queue","quartz test",null);
  new MuleClient().send("vm://event.queue","quartz test",null);
  new MuleClient().send("vm://quartz.scheduler","test",null);
  assertTrue(countDown.await(5000,TimeUnit.MILLISECONDS));
}
