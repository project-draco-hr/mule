{
  Bindings bindings=script.getScriptEngine().createBindings();
  if (proxies.size() > 0) {
    bindings.putAll(proxies);
  }
  script.populateBindings(bindings,event);
  try {
    return script.runScript(bindings);
  }
 catch (  Exception e) {
    throw e;
  }
 finally {
    bindings.clear();
  }
}
