{
  if (license == null) {
    try {
      license=IOUtils.getResourceAsString("MULE_LICENSE.txt",getClass());
      license=StringMessageUtils.getBoilerPlate(license,' ',80);
    }
 catch (    IOException e) {
      logger.warn("Failed to load MULE_LICENSE.txt",e);
    }
    if (license == null) {
      license="Failed to load license";
    }
  }
  return license;
}
