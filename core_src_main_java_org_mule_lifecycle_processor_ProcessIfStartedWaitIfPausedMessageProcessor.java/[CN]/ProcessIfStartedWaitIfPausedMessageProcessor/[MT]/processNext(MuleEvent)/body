{
  if (isPaused()) {
    try {
      if (logger.isDebugEnabled()) {
        logger.debug(startable.getClass().getName() + " " + getStartableName(startable)+ " is paused. Blocking call until resumd");
      }
      while (isPaused()) {
        Thread.sleep(500);
      }
    }
 catch (    InterruptedException e) {
      throw new MessagingException(CoreMessages.interruptedWaitingForPaused(getStartableName(startable)),event,e);
    }
  }
  return super.processNext(event);
}
