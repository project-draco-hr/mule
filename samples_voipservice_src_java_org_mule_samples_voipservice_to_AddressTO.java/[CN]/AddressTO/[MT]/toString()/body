{
  StringBuffer stringBuffer=new StringBuffer();
  if (this.houseNumber != null) {
    stringBuffer.append("[HouseNumber : " + houseNumber + "; ");
  }
  if (this.street != null) {
    stringBuffer.append("Street : " + street + "; ");
  }
  if (this.houseNumber != null) {
    stringBuffer.append("City : " + city + "]");
  }
  return stringBuffer.toString();
}
