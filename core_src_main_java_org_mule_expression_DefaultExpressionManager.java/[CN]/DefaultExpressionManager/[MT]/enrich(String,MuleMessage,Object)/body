{
  expression=removeExpressionMarker(expression);
  if (isEvaluatorExpression(expression)) {
    String[] parts=expression.split(":",2);
    enrich(parts[1],parts[0],message,object);
  }
 else {
    expressionLanguage.evaluate(createEnrichmentExpression(expression),message,Collections.singletonMap(OBJECT_FOR_ENRICHMENT,object));
  }
}
