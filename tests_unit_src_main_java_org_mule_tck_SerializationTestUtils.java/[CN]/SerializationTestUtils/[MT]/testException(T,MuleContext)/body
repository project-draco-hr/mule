{
  ObjectStore<T> os=getObjectStore(muleContext);
  try {
    os.store(key,exception);
    return os.retrieve(key);
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
 finally {
    try {
      os.clear();
    }
 catch (    ObjectStoreException e) {
      throw new RuntimeException(e);
    }
  }
}
