{
  boolean verified=false;
  if (muleContext != null) {
    if (muleContext.getRegistry().lookupObject(MuleProperties.OBJECT_QUEUE_MANAGER) != null && muleContext.getRegistry().lookupObject(MuleProperties.OBJECT_SECURITY_MANAGER) != null && muleContext.getRegistry().lookupObject(MuleProperties.OBJECT_MULE_ENDPOINT_FACTORY) != null) {
      verified=true;
    }
  }
  return verified;
}
