{
  if (result instanceof MuleMessage) {
    return new DefaultMuleEvent((MuleMessage)result,event);
  }
 else   if (result != null) {
    muleContext.getTransformationService().applyTransformers(event.getMessage(),event,Collections.<Transformer>singletonList(new TransformerTemplate(new TransformerTemplate.OverwitePayloadCallback(result))));
    return event;
  }
 else {
    return new DefaultMuleEvent(new DefaultMuleMessage(NullPayload.getInstance(),event.getMuleContext()),event);
  }
}
