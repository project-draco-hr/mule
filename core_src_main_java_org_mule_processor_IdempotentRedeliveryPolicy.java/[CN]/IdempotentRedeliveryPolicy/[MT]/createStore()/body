{
  ObjectStoreManager objectStoreManager=(ObjectStoreManager)muleContext.getRegistry().get(MuleProperties.OBJECT_STORE_MANAGER);
  return objectStoreManager.getLockableObjectStore(objectStoreManager.getObjectStore(flowConstruct.getName() + "." + getClass().getName(),OBJECT_STORE_NO_PERSISTENCE,OBJECT_STORE_NO_ENTRY_LIMIT,OBJECT_STORE_FIVE_MINUTES_TTL,OBJECT_STORE_EXPIRATION_INTERVAL));
}
