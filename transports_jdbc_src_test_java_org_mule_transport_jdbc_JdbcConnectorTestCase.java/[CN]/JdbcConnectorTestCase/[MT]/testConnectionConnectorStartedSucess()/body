{
  getConnector().start();
  assertTrue(getConnector().isStarted());
  assertTrue(getConnector().isConnected());
  assertTrue(getConnector() instanceof Testable);
  assertTrue(((Testable)getConnector()).test().getStatus() == TestResult.Status.SUCCESS);
  assertTrue(getConnector().isStarted());
  assertTrue(getConnector().isConnected());
}
