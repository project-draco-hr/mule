{
  XMLStreamReader parser=null;
  try {
    parser=XMLUtils.toXMLStreamReader(factory,obj);
    if (parser == null) {
      return false;
    }
    while (parser.next() != XMLStreamConstants.END_DOCUMENT) {
    }
    return true;
  }
 catch (  XMLStreamException ex) {
    return false;
  }
 finally {
    if (parser != null) {
      try {
        parser.close();
      }
 catch (      XMLStreamException ignored) {
      }
    }
  }
}
