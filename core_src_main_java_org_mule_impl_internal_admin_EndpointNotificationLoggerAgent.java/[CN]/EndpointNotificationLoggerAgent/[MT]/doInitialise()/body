{
  try {
    if (endpointAddress != null) {
      logEndpoint=RegistryContext.getRegistry().getOrCreateEndpointForUri(endpointAddress,UMOEndpoint.ENDPOINT_TYPE_SENDER,managementContext);
    }
 else {
      throw new InitialisationException(CoreMessages.propertiesNotSet("endpointAddress"),this);
    }
    session=new MuleSession(new MuleMessage(NullPayload.getInstance(),(Map)null),new NullSessionHandler());
  }
 catch (  Exception e) {
    throw new InitialisationException(e,this);
  }
}
