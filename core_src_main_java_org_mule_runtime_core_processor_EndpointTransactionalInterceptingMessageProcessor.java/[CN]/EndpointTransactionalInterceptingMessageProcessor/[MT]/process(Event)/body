{
  if (next == null) {
    return event;
  }
 else {
    ExecutionTemplate<Event> executionTemplate=TransactionalExecutionTemplate.createTransactionalExecutionTemplate(muleContext,transactionConfig);
    ExecutionCallback<Event> processingCallback=new ExecutionCallback<Event>(){
      @Override public Event process() throws Exception {
        return processNext(event);
      }
    }
;
    try {
      return executionTemplate.execute(processingCallback);
    }
 catch (    MuleException e) {
      throw e;
    }
catch (    Exception e) {
      throw new DefaultMuleException(CoreMessages.errorInvokingMessageProcessorWithinTransaction(next,transactionConfig),e);
    }
  }
}
