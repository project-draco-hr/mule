{
  if (getCurrentState().equals(UNKNOWN)) {
    throw new RegistryException("Illegal status: " + getCurrentState());
  }
  if (getCurrentState().equals(RUNNING)) {
    return;
  }
  try {
    doStart();
  }
 catch (  Exception e) {
    throw new RegistryException(e);
  }
  setCurrentState(RUNNING);
}
