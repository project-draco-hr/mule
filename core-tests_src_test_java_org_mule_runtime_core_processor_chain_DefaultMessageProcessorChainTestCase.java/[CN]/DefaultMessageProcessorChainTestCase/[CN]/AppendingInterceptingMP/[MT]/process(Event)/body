{
  if (stopProcessing) {
    return event;
  }
  Event intermediateEvent=Event.builder(event).message(InternalMessage.builder().payload(event.getMessage().getPayload().getValue() + "before" + appendString).build()).build();
  Event result=processNext(intermediateEvent);
  if (result != null && !result.equals(VoidMuleEvent.getInstance()) && !result.equals(NonBlockingVoidMuleEvent.getInstance())) {
    return Event.builder(result).message(InternalMessage.builder().payload(result.getMessage().getPayload().getValue() + "after" + appendString).build()).build();
  }
 else {
    return result;
  }
}
