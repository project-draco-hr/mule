{
  if (stopProcessing) {
    return event;
  }
  MuleEvent intermediateEvent=new DefaultMuleEvent(MuleMessage.builder().payload(event.getMessage().getPayload() + "before" + appendString).build(),event);
  MuleEvent result=processNext(intermediateEvent);
  if (result != null && !result.equals(VoidMuleEvent.getInstance()) && !result.equals(NonBlockingVoidMuleEvent.getInstance())) {
    return new DefaultMuleEvent(MuleMessage.builder().payload(result.getMessage().getPayload() + "after" + appendString).build(),result);
  }
 else {
    return result;
  }
}
