{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(getAppendingMP("1"),new DefaultMessageProcessorChainBuilder().chain(getAppendingMP("a"),new ReturnVoidMP(),getAppendingMP("b")).build(),new ReturnVoidMP(),getAppendingMP("2"));
