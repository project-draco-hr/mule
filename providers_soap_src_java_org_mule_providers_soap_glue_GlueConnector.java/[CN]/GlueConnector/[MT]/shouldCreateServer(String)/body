{
  URI uri=new URI(endpoint);
  String ep=uri.getScheme() + "://" + uri.getHost();
  if (uri.getPort() != -1)   ep+=":" + uri.getPort();
  for (Iterator iterator=serverEndpoints.iterator(); iterator.hasNext(); ) {
    String s=(String)iterator.next();
    if (s.startsWith(ep)) {
      return false;
    }
  }
  return true;
}
