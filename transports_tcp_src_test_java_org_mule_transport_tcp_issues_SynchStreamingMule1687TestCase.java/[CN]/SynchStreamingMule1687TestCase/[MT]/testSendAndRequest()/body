{
  MuleClient client=new MuleClient();
  MuleMessage message=client.send("tcp://localhost:65432",new DefaultMuleMessage(new DefaultMessageAdapter(new ByteArrayInputStream(TEST_MESSAGE.getBytes())),muleContext));
  assertNotNull(message);
  Object payload=message.getPayload();
  assertTrue(payload instanceof InputStream);
  assertEquals("Some value - set to make test ok",message.getPayloadAsString());
}
