{
  Class<? extends Connector> connectorClass=getJettyConnectorClass() != null ? getJettyConnectorClass() : DEFAULT_JETTY_CONNECTOR_CLASS;
  try {
    Connector cnn=ClassUtils.instanciateClass(connectorClass,new Object[]{});
    if (cnn instanceof org.eclipse.jetty.server.AbstractConnector) {
      ((org.eclipse.jetty.server.AbstractConnector)cnn).setAcceptors(getAcceptors());
    }
    return cnn;
  }
 catch (  Exception e) {
    throw new InitialisationException(CoreMessages.createStaticMessage("Error initializing Jetty Connector: " + connectorClass,e),this);
  }
}
