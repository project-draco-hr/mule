{
  try {
    httpServer.start();
    if (deployer != null) {
      deployer.start();
    }
    for (    ConnectorHolder<?,?> contextHolder : holders.values()) {
      contextHolder.start();
    }
  }
 catch (  Exception e) {
    throw new LifecycleException(CoreMessages.failedToStart("Jetty Http Receiver"),e,this);
  }
}
