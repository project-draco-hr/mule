{
  Object service;
  try {
    if (classType == null) {
      service=context.getBean(name);
    }
 else {
      service=context.getBean(name,classType);
    }
    return service;
  }
 catch (  BeansException e) {
    throw new TransportServiceException(CoreMessages.failedToCreateObjectWith(name,classType),e);
  }
}
