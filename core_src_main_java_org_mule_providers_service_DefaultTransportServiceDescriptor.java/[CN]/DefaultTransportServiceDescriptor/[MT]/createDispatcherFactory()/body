{
  if (dispatcherFactory != null) {
    try {
      return (UMOMessageDispatcherFactory)ClassUtils.instanciateClass(dispatcherFactory,ClassUtils.NO_ARGS);
    }
 catch (    Exception e) {
      throw new TransportServiceException(new Message(Messages.FAILED_TO_CREATE_X_WITH_X,"Message Dispatcher Factory",dispatcherFactory),e);
    }
  }
 else {
    throw new TransportServiceException(new Message(Messages.X_NOT_SET_IN_SERVICE_X,"Message Dispatcher Factory",getService()));
  }
}
