{
  if (!disposed) {
    try {
      try {
        this.disconnect();
      }
 catch (      Exception e) {
        logger.warn(e.getMessage(),e);
      }
      this.doDispose();
      if (workManager != null) {
        workManager.dispose();
      }
    }
  finally {
      disposed=true;
    }
  }
}
