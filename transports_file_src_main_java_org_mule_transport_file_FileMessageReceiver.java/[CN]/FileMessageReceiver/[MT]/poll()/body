{
  try {
    File[] files=this.listFiles();
    if (logger.isDebugEnabled()) {
      logger.debug("Files: " + Arrays.toString(files));
    }
    Comparator comparator=getComparator();
    if (comparator != null) {
      Arrays.sort(files,comparator);
    }
    for (    File file : files) {
      if (file.isFile()) {
        this.processFile(file);
      }
    }
  }
 catch (  Exception e) {
    this.handleException(e);
  }
}
