{
  File[] files;
  if (fileFilter != null) {
    files=currentDirectory.listFiles(fileFilter);
  }
 else {
    files=currentDirectory.listFiles(filenameFilter);
  }
  if (files == null) {
    return;
  }
  for (  File file : files) {
    if (!file.isDirectory()) {
      discoveredFiles.add(file);
    }
 else {
      if (fileConnector.isRecursive()) {
        this.basicListFiles(file,discoveredFiles);
      }
    }
  }
}
