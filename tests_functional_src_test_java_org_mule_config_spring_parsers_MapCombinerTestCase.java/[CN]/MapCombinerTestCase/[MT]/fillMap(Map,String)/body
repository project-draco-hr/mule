{
  spec=drop(spec,"[");
  while (!spec.startsWith("]")) {
    assertTrue("spec finished early (missing ']'?)",spec.length() > 1);
    String key=spec.substring(0,1);
    spec=drop(spec,key);
    spec=drop(spec,":");
    if (spec.startsWith("[")) {
      Map value=new HashMap();
      spec=fillMap(value,spec);
      map.put(key,value);
    }
 else {
      String value=spec.substring(0,1);
      spec=drop(spec,value);
      map.put(key,value);
    }
    if (spec.startsWith(",")) {
      spec=drop(spec,",");
    }
  }
  return drop(spec,"]");
}
