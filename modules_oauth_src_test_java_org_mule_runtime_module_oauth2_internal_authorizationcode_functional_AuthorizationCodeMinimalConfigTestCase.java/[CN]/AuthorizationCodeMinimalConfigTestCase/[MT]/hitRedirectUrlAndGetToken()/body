{
  configureWireMockToExpectTokenPathRequestForAuthorizationCodeGrantType();
  Request.Get(localCallbackUrl.getValue() + "?" + CODE_PARAMETER+ "="+ AUTHENTICATION_CODE).connectTimeout(REQUEST_TIMEOUT).socketTimeout(REQUEST_TIMEOUT).execute();
  verifyRequestDoneToTokenUrlForAuthorizationCode();
  OAuthContextFunctionAsserter.createFrom(muleContext.getExpressionManager(),"tokenManagerConfig").assertAccessTokenIs(ACCESS_TOKEN).assertRefreshTokenIs(REFRESH_TOKEN);
}
