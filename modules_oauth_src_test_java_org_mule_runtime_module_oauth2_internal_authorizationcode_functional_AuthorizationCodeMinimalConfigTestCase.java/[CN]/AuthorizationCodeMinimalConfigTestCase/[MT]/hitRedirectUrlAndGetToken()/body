{
  configureWireMockToExpectTokenPathRequestForAuthorizationCodeGrantType();
  Request.Get(redirectUrl.getValue() + "?" + OAuthConstants.CODE_PARAMETER+ "="+ AUTHENTICATION_CODE).connectTimeout(REQUEST_TIMEOUT).socketTimeout(REQUEST_TIMEOUT).execute();
  verifyRequestDoneToTokenUrlForAuthorizationCode();
  OAuthContextFunctionAsserter.createFrom(muleContext.getExpressionLanguage(),"tokenManagerConfig").assertAccessTokenIs(ACCESS_TOKEN).assertRefreshTokenIs(REFRESH_TOKEN);
}
