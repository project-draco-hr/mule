{
  Collection values=row.values();
  String[] stringValues=new String[values.size()];
  int i=0;
  for (Iterator v=values.iterator(); v.hasNext(); i++) {
    Object value=v.next();
    if (value != null) {
      stringValues[i]=value.toString();
    }
 else {
      stringValues[i]="";
    }
  }
  writer.writeNext(stringValues);
}
