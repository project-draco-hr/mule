{
  Object theObject=object;
  Collection processors=lookupObjects(ObjectProcessor.class,(null != getParent() && getParent().isInitialised()) ? getDefaultScope() : SCOPE_IMMEDIATE);
  for (Iterator iterator=processors.iterator(); iterator.hasNext(); ) {
    ObjectProcessor o=(ObjectProcessor)iterator.next();
    theObject=o.process(theObject);
  }
  return theObject;
}
