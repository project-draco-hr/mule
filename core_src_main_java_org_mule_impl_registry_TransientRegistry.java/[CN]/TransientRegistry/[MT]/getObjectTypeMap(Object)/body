{
  if (o == null) {
    o=Object.class;
  }
  Object key;
  if (o instanceof Class) {
    key=o;
  }
 else   if (o instanceof String) {
    key=o;
  }
 else {
    key=o.getClass();
  }
  Map objects=(Map)registry.get(key);
  if (objects == null) {
    objects=new HashMap(8);
    registry.put(key,objects);
  }
  return objects;
}
