{
  Lock lock=fileSystem.createMuleLock(String.format("%s-mkdirs-%s",getClass().getName(),directoryPath));
  lock.lock();
  try {
    if (exists(config,directoryPath)) {
      return;
    }
    doMkDirs(config,directoryPath);
  }
  finally {
    lock.unlock();
  }
  LOGGER.debug("Directory '{}' created",directoryPath);
}
