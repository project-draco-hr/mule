{
  if (this.oauthCallback == null) {
    this.oauthCallback=this.callbackFactory.createCallback(adapter,this.getAuthCodeRegex(),fetchAccessTokenMessageProcessor,this.listener,muleContext,flowConstruct);
    this.oauthCallback.start();
  }
  fetchAccessTokenMessageProcessor.setRedirectUri(this.oauthCallback.getUrl());
}
