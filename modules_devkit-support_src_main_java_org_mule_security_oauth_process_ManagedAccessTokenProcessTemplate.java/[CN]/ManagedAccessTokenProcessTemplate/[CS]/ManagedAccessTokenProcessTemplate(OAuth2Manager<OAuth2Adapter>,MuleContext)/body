{
  ProcessInterceptor<P,OAuth2Adapter> processCallbackProcessInterceptor=new ProcessCallbackProcessInterceptor<P,OAuth2Adapter>();
  ProcessInterceptor<P,OAuth2Adapter> refreshTokenProcessInterceptor=(ProcessInterceptor<P,OAuth2Adapter>)new RefreshTokenProcessInterceptor<P>(processCallbackProcessInterceptor,muleContext);
  ProcessInterceptor<P,OAuth2Adapter> managedAccessTokenProcessInterceptor=new ManagedAccessTokenProcessInterceptor<P>(refreshTokenProcessInterceptor,oauthManager);
  processInterceptor=managedAccessTokenProcessInterceptor;
}
