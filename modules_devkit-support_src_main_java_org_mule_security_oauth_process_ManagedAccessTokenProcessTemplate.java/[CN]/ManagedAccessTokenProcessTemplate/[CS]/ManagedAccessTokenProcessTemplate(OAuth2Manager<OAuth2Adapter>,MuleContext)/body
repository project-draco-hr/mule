{
  ProcessInterceptor<P,OAuth2Adapter> processCallbackProcessInterceptor=new ProcessCallbackProcessInterceptor<P,OAuth2Adapter>();
  @SuppressWarnings("unchecked") ProcessInterceptor<P,OAuth2Adapter> refreshTokenProcessInterceptor=(ProcessInterceptor<P,OAuth2Adapter>)new RefreshTokenProcessInterceptor(processCallbackProcessInterceptor);
  ProcessInterceptor<P,OAuth2Adapter> managedAccessTokenProcessInterceptor=new ManagedAccessTokenProcessInterceptor<P>(refreshTokenProcessInterceptor,oauthManager,muleContext);
  processInterceptor=managedAccessTokenProcessInterceptor;
}
