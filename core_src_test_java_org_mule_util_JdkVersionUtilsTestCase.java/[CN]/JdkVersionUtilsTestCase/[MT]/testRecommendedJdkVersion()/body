{
  setJdkVersion("1.6.0_129");
  assertTrue(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.6.20");
  assertTrue(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.7.0_03");
  assertTrue(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.8");
  assertTrue(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.4.2");
  assertFalse(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.6");
  assertFalse(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.6.0_5");
  assertFalse(JdkVersionUtils.isRecommendedJdkVersion());
  setJdkVersion("1.7.0");
  assertFalse(JdkVersionUtils.isRecommendedJdkVersion());
}
