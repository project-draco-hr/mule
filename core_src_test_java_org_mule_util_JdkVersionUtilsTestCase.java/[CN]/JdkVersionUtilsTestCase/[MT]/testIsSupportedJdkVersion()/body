{
  assertTrue(JdkVersionUtils.isSupportedJdkVersion());
  setJdkVersion("1.6");
  assertTrue(JdkVersionUtils.isSupportedJdkVersion());
  setJdkVersion("1.7");
  assertTrue(JdkVersionUtils.isSupportedJdkVersion());
  setJdkVersion("1.8");
  assertFalse(JdkVersionUtils.isSupportedJdkVersion());
  setJdkVersion("1.4.2");
  assertFalse(JdkVersionUtils.isSupportedJdkVersion());
  setJdkVersion("1.4.2_12");
  assertFalse(JdkVersionUtils.isSupportedJdkVersion());
}
