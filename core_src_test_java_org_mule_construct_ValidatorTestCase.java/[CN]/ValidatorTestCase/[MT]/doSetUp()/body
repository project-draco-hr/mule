{
  super.doSetUp();
  final OutboundEndpoint testOutboundEndpoint=MuleTestUtils.getTestOutboundEndpoint(MessageExchangePattern.ONE_WAY,muleContext);
  testConnector=testOutboundEndpoint.getConnector();
  muleContext.getRegistry().registerConnector(testConnector);
  testConnector.start();
  validator=new Validator("test-validator",muleContext,directInboundMessageSource,testOutboundEndpoint,new PayloadTypeFilter(Integer.class),"#[string:GOOD:#[message:payload]]","#[string:BAD:#[message:payload]]");
}
