{
  System.out.println(now.getHours());
  if (now.getHours() == 23) {
    Assert.assertTrue((Boolean)now.isAfter(new DateTime().changeTimeZone("UTC").plusHours(1).getTime()));
  }
 else {
    Assert.assertTrue((Boolean)now.isBefore(new DateTime().changeTimeZone("UTC").plusHours(1).getTime()));
  }
}
