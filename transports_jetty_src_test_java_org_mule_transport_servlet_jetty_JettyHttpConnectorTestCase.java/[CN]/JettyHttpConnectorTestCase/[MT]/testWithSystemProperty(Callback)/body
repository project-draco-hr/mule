{
  String orginalSystemPropertyValue=System.getProperty(JettyHttpConnector.JETTY_CONNECTOR_SYSTEM_PROPERTY);
  try {
    System.setProperty(JettyHttpConnector.JETTY_CONNECTOR_SYSTEM_PROPERTY,"org.eclipse.jetty.server.bio.SocketConnector");
    callback.run();
  }
  finally {
    if (orginalSystemPropertyValue == null) {
      System.clearProperty(JettyHttpConnector.JETTY_CONNECTOR_SYSTEM_PROPERTY);
    }
 else {
      System.setProperty(JettyHttpConnector.JETTY_CONNECTOR_SYSTEM_PROPERTY,orginalSystemPropertyValue);
    }
  }
}
