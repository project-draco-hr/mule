{
  StringBuffer uri=new StringBuffer();
  uri.append("tasks?");
  this.appendType(uri,endpoint);
  if (this.getStart() != null) {
    uri.append("&start=");
    uri.append(this.getStart());
  }
  if (this.getSize() != null) {
    uri.append("&size=");
    uri.append(this.getSize());
  }
  return new URI(uri.toString(),false);
}
