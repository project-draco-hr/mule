{
  ParameterDescriptor descriptor=field.getAnnotation(Optional.class) != null ? with.optionalParameter(TLS_ATTRIBUTE_NAME) : with.requiredParameter(TLS_ATTRIBUTE_NAME);
  return descriptor.ofType(TlsContextFactory.class).withExpressionSupport(ExpressionSupport.NOT_SUPPORTED).withModelProperty(new DeclaringMemberModelProperty(field));
}
