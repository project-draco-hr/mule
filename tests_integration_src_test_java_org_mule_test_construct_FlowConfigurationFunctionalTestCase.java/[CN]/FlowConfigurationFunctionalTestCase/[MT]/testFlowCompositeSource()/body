{
  final Flow flow=muleContext.getRegistry().lookupObject("flow2");
  assertEquals(StartableCompositeMessageSource.class,flow.getMessageSource().getClass());
  assertEquals(2,flow.getMessageProcessors().size());
  assertEquals("01xyz",getPayloadAsString(muleContext.getClient().send("vm://in2",new DefaultMuleMessage("0",muleContext))));
  assertEquals("01xyz",getPayloadAsString(muleContext.getClient().send("vm://in3",new DefaultMuleMessage("0",muleContext))));
}
