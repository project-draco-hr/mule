{
  assertEquals("foo Hello foo",getPayloadAsString(muleContext.getClient().send("vm://choice2-in",new DefaultMuleMessage("foo",muleContext))));
  assertEquals("bar Hello bar",getPayloadAsString(muleContext.getClient().send("vm://choice2-in",new DefaultMuleMessage("bar",muleContext))));
  assertEquals("egh Hello ?",getPayloadAsString(muleContext.getClient().send("vm://choice2-in",new DefaultMuleMessage("egh",muleContext))));
}
