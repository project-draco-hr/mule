{
  TransportEndpointDefinitionParser endpointDefinitionParser=new TransportEndpointDefinitionParser(XmppConnector.XMPP,TransportEndpointDefinitionParser.PROTOCOL,InboundEndpointFactoryBean.class,TransportEndpointDefinitionParser.RESTRICTED_ENDPOINT_ATTRIBUTES,REQUIRED_INBOUND_ATTRIBUTE_GROUPS,REQUIRED_MESSAGE_PROPERTY_GROUPS);
  endpointDefinitionParser.addAlias(XmppConnector.XMPP_TYPE,URIBuilder.HOST);
  endpointDefinitionParser.addAlias(XmppConnector.XMPP_FROM,URIBuilder.PATH);
  registerBeanDefinitionParser("inbound-endpoint",endpointDefinitionParser);
}
