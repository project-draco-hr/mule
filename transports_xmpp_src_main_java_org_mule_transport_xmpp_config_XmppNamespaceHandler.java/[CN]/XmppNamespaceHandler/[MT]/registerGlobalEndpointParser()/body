{
  TransportGlobalEndpointDefinitionParser endpointDefinitionParser=new TransportGlobalEndpointDefinitionParser(XmppConnector.XMPP,TransportGlobalEndpointDefinitionParser.PROTOCOL,TransportGlobalEndpointDefinitionParser.RESTRICTED_ENDPOINT_ATTRIBUTES,REQUIRED_ALL_ATTRIBUTE_GROUPS,REQUIRED_MESSAGE_PROPERTY_GROUPS);
  endpointDefinitionParser.addAlias(XmppConnector.XMPP_TYPE,URIBuilder.HOST);
  endpointDefinitionParser.addAlias(XmppConnector.XMPP_RECIPIENT,URIBuilder.PATH);
  endpointDefinitionParser.addAlias(XmppConnector.XMPP_FROM,URIBuilder.PATH);
  registerBeanDefinitionParser("endpoint",endpointDefinitionParser);
}
