{
  started.set(false);
  stopping.set(true);
  fireSystemEvent(new ManagerNotification(id,null,null,ManagerNotification.MANAGER_STOPPING));
  if (queueManager != null) {
    queueManager.stop();
  }
  stopping.set(false);
  fireSystemEvent(new ManagerNotification(id,null,null,ManagerNotification.MANAGER_STOPPED));
}
