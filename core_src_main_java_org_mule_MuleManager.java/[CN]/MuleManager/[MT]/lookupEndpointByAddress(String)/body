{
  UMOEndpoint endpoint=null;
  if (address != null) {
    boolean found=false;
    Iterator iterator=endpoints.keySet().iterator();
    while (!found && iterator.hasNext()) {
      endpoint=(UMOEndpoint)endpoints.get(iterator.next());
      found=(address.equals(endpoint.getEndpointURI().toString()));
    }
  }
  return endpoint;
}
