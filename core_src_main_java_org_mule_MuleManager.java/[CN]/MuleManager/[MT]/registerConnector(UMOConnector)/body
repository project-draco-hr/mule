{
  connectors.put(connector.getName(),connector);
  if (initialised.get() || initialising.get()) {
    connector.initialise();
    try {
      connector.register();
    }
 catch (    RegistrationException re) {
    }
  }
  if ((started.get() || starting.get()) && !connector.isStarted()) {
    connector.startConnector();
  }
}
