{
  File document=new File(documentURI);
  if (document.isDirectory()) {
    return documentURI;
  }
  String fileName=document.getName();
  int fileNameIndex=documentURI.lastIndexOf(fileName);
  if (fileNameIndex == -1) {
    return documentURI;
  }
  return documentURI.substring(0,fileNameIndex);
}
