{
  byte[] tmp=(byte[])super.read(is);
  if (tmp == null) {
    return null;
  }
 else {
    MessageObject serializableObject;
    try (ClassLoaderObjectInputStream ois=new ClassLoaderObjectInputStream(currentThread().getContextClassLoader(),new ByteArrayInputStream(tmp))){
      serializableObject=((MessageObject)ois.readObject());
    }
 catch (    ClassNotFoundException e) {
      throw new SerializationException(e);
    }
    return new NonSerializableMessageObject(serializableObject.i,serializableObject.s,serializableObject.b);
  }
}
