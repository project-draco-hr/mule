{
  if (endpoint.startsWith("glue") || endpoint.startsWith("axis") || endpoint.startsWith("soap")) {
    int i=endpoint.indexOf('?');
    if (i > -1) {
      endpoint=endpoint.replaceFirst("\\?","/" + endpoint + "?method=getLoanQuote\\&");
    }
 else {
      endpoint+="/" + endpoint + "?method=getLoanQuote";
    }
  }
  return endpoint;
}
