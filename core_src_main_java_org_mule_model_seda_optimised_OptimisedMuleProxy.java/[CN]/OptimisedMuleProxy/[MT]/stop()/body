{
  checkDisposed();
  if (started && pojoService instanceof Stoppable) {
    started=false;
    try {
      return ((Stoppable)pojoService).stop();
    }
 catch (    Exception e) {
      throw new ModelException(CoreMessages.failedToStop("Service '" + service.getName() + "'"),e);
    }
  }
 else {
    return LifecycleTransitionResult.OK;
  }
}
