{
  File f=new File("win32/dot.exe");
  if (!f.exists()) {
    String home=SystemUtils.getenv(MULE_HOME);
    if (StringUtils.isNotBlank(home)) {
      f=new File(home + "/tools/config-graph/win32/dot.exe");
    }
  }
  if (f.exists()) {
    env.getConfig().setExecuteCommand(f.getAbsolutePath());
  }
}
