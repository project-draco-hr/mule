{
  String[] endpointProps=new String[]{"frontend","bindingUri","bridge","endpointName","serviceName","wsdlLocation"};
  registerBeanDefinitionParser("connector",new OrphanDefinitionParser(CxfConnector.class,true));
  registerBeanDefinitionParser("endpoint",new TransportGlobalEndpointDefinitionParser("cxf",TransportGlobalEndpointDefinitionParser.META,endpointProps,new String[]{}));
  registerBeanDefinitionParser("inbound-endpoint",new TransportEndpointDefinitionParser("cxf",TransportEndpointDefinitionParser.META,InboundEndpointFactoryBean.class,endpointProps,new String[]{}));
}
