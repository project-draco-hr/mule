{
  registerBeanDefinitionParser("connector",new OrphanDefinitionParser(CxfConnector.class,true));
  registerBeanDefinitionParser("endpoint",new TransportGlobalEndpointDefinitionParser("cxf",TransportGlobalEndpointDefinitionParser.META,new String[0]));
  registerBeanDefinitionParser("inbound-endpoint",new TransportEndpointDefinitionParser("cxf",TransportEndpointDefinitionParser.META,InboundEndpointFactoryBean.class,new String[0]));
}
