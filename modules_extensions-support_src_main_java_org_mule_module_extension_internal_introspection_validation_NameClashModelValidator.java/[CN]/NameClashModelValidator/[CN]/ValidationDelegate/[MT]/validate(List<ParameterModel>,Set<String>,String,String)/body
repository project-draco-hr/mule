{
  Set<String> repeatedParameters=getRepeatedParameters(parameters);
  if (!repeatedParameters.isEmpty()) {
    throw new IllegalModelDefinitionException(format("Extension '%s' defines the %s '%s' which has parameters " + "with repeated names. Offending parameters are: [%s]",extensionModel.getName(),ownerType,ownerName,Joiner.on(",").join(repeatedParameters)));
  }
  validateTopLevelParameters(parameters,ownerName,ownerType);
  if (!accumulator.add(ownerName)) {
    throw new IllegalArgumentException(format("Extension '%s' defines more than one %s of name '%s'. Please make sure %s names are unique",extensionModel.getName(),ownerType,ownerName,ownerType));
  }
}
