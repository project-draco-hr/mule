{
  Sample l=samples.isEmpty() ? null : (Sample)samples.getFirst();
  long t=getTime();
  if (l == null || t > l.time) {
    Sample s=new Sample(getBase().nextValue(),t);
    samples.addFirst(s);
  }
 else {
    l.value+=getBase().nextValue();
  }
  while (samples.size() > length) {
    samples.removeLast();
  }
}
