{
  MuleConfigurationDialog dialog=new MuleConfigurationDialog(muleModel.getProject(),getConfigsTable().getTable().getShell());
  if (dialog.open() == Window.OK) {
    IMuleConfiguration config=muleModel.createNewMuleConfiguration(dialog.getDescription(),dialog.getPath());
    muleModel.addMuleConfiguration(config);
    getConfigsTable().refresh();
  }
}
