{
  ImmutableEndpoint registryEndpoint=muleContext.getRegistry().lookupEndpoint(ENDPOINT_REGISTRY_PREFIX + endpoint.hashCode());
  if (registryEndpoint == null) {
    muleContext.getRegistry().registerObject(ENDPOINT_REGISTRY_PREFIX + endpoint.hashCode(),endpoint);
    registryEndpoint=endpoint;
  }
  return registryEndpoint;
}
