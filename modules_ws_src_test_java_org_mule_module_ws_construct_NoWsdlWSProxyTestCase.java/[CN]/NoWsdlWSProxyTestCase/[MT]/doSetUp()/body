{
  super.doSetUp();
  final OutboundEndpoint testOutboundEndpoint=MuleTestUtils.getTestOutboundEndpoint(MessageExchangePattern.REQUEST_RESPONSE,muleContext);
  testConnector=testOutboundEndpoint.getConnector();
  muleContext.getRegistry().registerConnector(testConnector);
  testConnector.start();
  wsProxy=new WSProxy(muleContext,"no-wsdl-ws-proxy",directInboundMessageSource,testOutboundEndpoint);
}
