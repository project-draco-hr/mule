{
  if (transactionFactory != null) {
    try {
      return (TransactionFactory)ClassUtils.instanciateClass(transactionFactory,ClassUtils.NO_ARGS,classLoader);
    }
 catch (    Exception e) {
      throw new TransportServiceException(CoreMessages.failedToCreateObjectWith("Transaction Factory",transactionFactory),e);
    }
  }
 else {
    return null;
  }
}
