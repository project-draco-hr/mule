{
  XPath xpath=cache.get(expression + getClass().getName());
  if (xpath == null) {
    xpath=createXPath(expression,object);
synchronized (xpath) {
      if (namespaceManager != null) {
        addNamespaces(namespaceManager,xpath);
      }
    }
    cache.put(expression + getClass().getName(),xpath);
  }
  return xpath;
}
