{
  try {
    if (getTestInfo().getRunCount() == getTestInfo().getTestCount()) {
      log("Pre suiteTearDown for test: " + getTestInfo());
      suitePreTearDown();
    }
    doTearDown();
    if (!getTestInfo().isDisposeManagerPerSuite()) {
      disposeManager();
    }
  }
  finally {
    getTestInfo().incRunCount();
    if (getTestInfo().getRunCount() == getTestInfo().getTestCount()) {
      try {
        log("Post suiteTearDown for test: " + getTestInfo());
        suitePostTearDown();
      }
  finally {
        clearCounter();
        disposeManager();
      }
    }
  }
}
