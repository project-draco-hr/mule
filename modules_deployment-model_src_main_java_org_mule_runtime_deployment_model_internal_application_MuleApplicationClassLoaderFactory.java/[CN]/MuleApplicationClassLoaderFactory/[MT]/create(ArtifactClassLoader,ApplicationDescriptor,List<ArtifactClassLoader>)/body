{
  List<URL> urls=getApplicationResourceUrls(descriptor);
  final ClassLoaderLookupPolicy classLoaderLookupPolicy=getApplicationClassLoaderLookupPolicy(parent,descriptor);
  return new MuleApplicationClassLoader(descriptor,parent.getClassLoader(),nativeLibraryFinderFactory.create(descriptor.getName()),urls,classLoaderLookupPolicy,artifactPluginClassLoaders);
}
