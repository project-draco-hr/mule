{
  String use=event.getMessage().getStringProperty(AxisConnector.USE,null);
  if (use != null) {
    Use u=Use.getUse(use);
    if (u == null) {
      throw new IllegalArgumentException(CoreMessages.valueIsInvalidFor(use,AxisConnector.USE).toString());
    }
 else {
      call.setOperationUse(u);
    }
  }
}
