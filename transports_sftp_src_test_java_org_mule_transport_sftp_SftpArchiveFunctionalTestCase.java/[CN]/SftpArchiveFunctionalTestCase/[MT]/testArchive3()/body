{
  executeBaseTest(INBOUND_ENDPOINT3,"vm://test.upload3",FILE3_TXT,SEND_SIZE,"receiving3",TIMEOUT);
  assertFilesInLocalFilesystem(archive,FILE3_TMP_REGEXP);
  assertNoFilesInEndpoint(INBOUND_ENDPOINT3);
