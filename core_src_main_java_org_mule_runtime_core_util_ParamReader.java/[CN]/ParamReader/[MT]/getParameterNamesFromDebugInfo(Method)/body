{
  int numParams=method.getParameterTypes().length;
  if (numParams == 0) {
    return null;
  }
  Class c=method.getDeclaringClass();
  if (Proxy.isProxyClass(c)) {
    return null;
  }
  try {
    ParamReader pr=new ParamReader(c);
    return pr.getParameterNames(method);
  }
 catch (  IOException e) {
    return null;
  }
}
