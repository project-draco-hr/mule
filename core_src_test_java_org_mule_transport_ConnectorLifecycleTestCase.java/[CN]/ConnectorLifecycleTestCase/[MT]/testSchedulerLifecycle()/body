{
  assertFalse(connector.getScheduler().isShutdown());
  assertFalse(connector.getScheduler().isTerminated());
  connector.start();
  assertFalse(connector.getScheduler().isShutdown());
  assertFalse(connector.getScheduler().isTerminated());
  connector.stop();
  assertFalse(connector.getScheduler().isShutdown());
  assertFalse(connector.getScheduler().isTerminated());
  connector.start();
  assertFalse(connector.getScheduler().isShutdown());
  assertFalse(connector.getScheduler().isTerminated());
  connector.dispose();
  assertFalse(connector.getScheduler().isShutdown());
  assertFalse(connector.getScheduler().isTerminated());
}
