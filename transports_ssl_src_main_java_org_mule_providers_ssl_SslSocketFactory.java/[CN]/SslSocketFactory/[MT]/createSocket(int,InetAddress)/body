{
  try {
    return tls.getSocketFactory().createSocket(inetAddress,port);
  }
 catch (  NoSuchAlgorithmException e) {
    throw (IOException)new IOException(e.getMessage()).initCause(e);
  }
catch (  KeyManagementException e) {
    throw (IOException)new IOException(e.getMessage()).initCause(e);
  }
}
