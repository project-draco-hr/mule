{
  if (o == null) {
    return;
  }
  if (ignoreType(o.getClass())) {
    return;
  }
  if (!getLifecycleClass().isAssignableFrom(o.getClass())) {
    return;
  }
  try {
    lifecycleMethod.invoke(o);
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new LifecycleException(CoreMessages.failedToInvokeLifecycle(lifecycleMethod.getName(),o),e,this);
  }
}
