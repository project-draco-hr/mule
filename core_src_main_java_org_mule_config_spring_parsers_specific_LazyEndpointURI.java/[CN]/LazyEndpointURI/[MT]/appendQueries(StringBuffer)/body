{
  if (null != queries) {
    boolean first=buffer.indexOf(QUERY) > -1;
    Iterator keys=new TreeSet(queries.keySet()).iterator();
    while (keys.hasNext()) {
      if (first) {
        buffer.append(QUERY);
        first=false;
      }
 else {
        buffer.append(AND);
      }
      String key=(String)keys.next();
      buffer.append(key);
      buffer.append(EQUALS);
      buffer.append((String)queries.get(key));
    }
  }
}
