{
  try {
    if (getSubscriptionFilter() == null)     setSubscriptionFilter(WildcardFilter.class);
    UMOFilter filter=(UMOFilter)ClassHelper.instanciateClass(getSubscriptionFilter(),new Object[]{pattern});
    return filter;
  }
 catch (  Exception e) {
    logger.error("Failed to load filter: " + getSubscriptionFilter() + " : "+ e.getMessage());
    return new WildcardFilter(pattern);
  }
}
