{
  try {
    if (getSubscriptionFilter() == null) {
      setSubscriptionFilter(WildcardFilter.class);
    }
    ObjectFilter filter=(ObjectFilter)ClassUtils.instanciateClass(getSubscriptionFilter(),new Object[]{pattern});
    return filter;
  }
 catch (  Exception e) {
    exceptionListener.exceptionThrown(e);
    return new WildcardFilter(pattern);
  }
}
