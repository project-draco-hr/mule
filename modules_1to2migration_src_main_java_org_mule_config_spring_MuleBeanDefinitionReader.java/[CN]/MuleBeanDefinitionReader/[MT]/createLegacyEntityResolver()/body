{
  if (dtdResolver == null) {
    MuleDtdResolver muleSpringResolver=new MuleDtdResolver("mule-spring-configuration.dtd","mule-1-to-spring-migration.xsl",new BeansDtdResolver());
    dtdResolver=new MuleDtdResolver("mule-configuration.dtd","mule-1-to-spring-migration.xsl",muleSpringResolver);
  }
  return dtdResolver;
}
