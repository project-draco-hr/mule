{
  String[] stackEntries=item.split(SystemUtils.LINE_SEPARATOR);
  if (stackEntries.length != expectedEntries.length) {
    return false;
  }
  for (  String expectedEntry : expectedEntries) {
    if (!stackEntries[i].matches(expectedEntry)) {
      return false;
    }
    ++i;
  }
  return true;
}
