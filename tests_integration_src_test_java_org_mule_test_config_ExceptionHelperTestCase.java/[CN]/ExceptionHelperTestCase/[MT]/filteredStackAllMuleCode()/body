{
  try {
    throw new RuntimeException(new DefaultMuleException(MessageFactory.createStaticMessage("foo")));
  }
 catch (  Exception e) {
    assertThat(ExceptionHelper.getExceptionStack(e),StringByLineMatcher.matchesLineByLine("foo (org.mule.api.DefaultMuleException)","  org.mule.test.config.ExceptionHelperTestCase:"));
  }
}
