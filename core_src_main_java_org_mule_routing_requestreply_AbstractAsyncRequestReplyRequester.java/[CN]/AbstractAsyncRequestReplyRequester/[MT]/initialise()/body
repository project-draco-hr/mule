{
  name=String.format(NAME_TEMPLATE,storePrefix,ThreadNameHelper.getPrefix(muleContext),flowConstruct == null ? "" : flowConstruct.getName());
  store=((ObjectStoreManager)muleContext.getRegistry().get(MuleProperties.OBJECT_STORE_MANAGER)).getObjectStore(name,false,MAX_PROCESSED_GROUPS,UNCLAIMED_TIME_TO_LIVE,UNCLAIMED_INTERVAL);
}
