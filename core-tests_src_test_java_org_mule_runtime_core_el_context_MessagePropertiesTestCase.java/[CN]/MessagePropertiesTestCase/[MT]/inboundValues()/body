{
  InternalMessage message=event.getMessage();
  event=Event.builder(event).message(InternalMessage.builder(message).addInboundProperty("foo","abc").addInboundProperty("bar","xyz").build()).build();
  Collection<DataHandler> values=(Collection<DataHandler>)evaluate("message.inboundProperties.values()",event);
  assertEquals(2,values.size());
  values.contains("abc");
  values.contains("xyz");
}
