{
  try {
    final String wsdlContents=getWsdlContents(event);
    event.getMessage().setPayload(wsdlContents);
    event.setStopFurtherProcessing(true);
    return event;
  }
 catch (  final Exception e) {
    throw new MessagingException(MessageFactory.createStaticMessage("Impossible to retrieve WSDL for proxied service"),event.getMessage(),e);
  }
}
