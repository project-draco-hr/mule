{
  final String httpRequest=event.getMessage().<String>getInboundProperty(HTTP_REQUEST);
  if (httpRequest == null) {
    logger.warn("WS Proxy can't rewrite WSDL for non-HTTP " + event);
    return false;
  }
  final String lowerHttpRequest=httpRequest.toLowerCase();
  return (lowerHttpRequest.indexOf(WSDL_PARAM_1) != -1) || (lowerHttpRequest.indexOf(WSDL_PARAM_2) != -1) || (lowerHttpRequest.indexOf(XSD_PARAM_1) != -1)|| (lowerHttpRequest.indexOf(XSD_PARAM_2) != -1);
}
