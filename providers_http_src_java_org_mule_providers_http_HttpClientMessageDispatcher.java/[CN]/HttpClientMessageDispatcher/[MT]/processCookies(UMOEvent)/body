{
  Cookie[] cookies=(Cookie[])event.removeProperty(HttpConnector.HTTP_COOKIES_PROPERTY);
  if (cookies != null && cookies.length > 0) {
    client.getParams().setCookiePolicy(CookieHelper.getCookiePolicy((String)event.removeProperty(HttpConnector.HTTP_COOKIE_SPEC_PROPERTY)));
    client.getState().addCookies(cookies);
  }
}
