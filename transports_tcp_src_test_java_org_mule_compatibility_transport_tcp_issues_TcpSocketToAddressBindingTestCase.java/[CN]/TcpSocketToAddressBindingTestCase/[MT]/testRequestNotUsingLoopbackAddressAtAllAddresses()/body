{
  MuleClient client=muleContext.getClient();
  MuleMessage result;
  for (  InetAddress inetAddress : localInetAddresses) {
    result=client.send(getTransportName() + "://" + inetAddress.getHostAddress()+ ":"+ dynamicPort3.getNumber(),getTestMuleMessage(TEST_MESSAGE)).getRight();
    assertEquals(TEST_MESSAGE + " Received",getPayloadAsString(result));
  }
}
