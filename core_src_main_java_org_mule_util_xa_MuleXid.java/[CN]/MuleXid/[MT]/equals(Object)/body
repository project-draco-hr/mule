{
  if (obj == null) {
    return false;
  }
  if (!(obj instanceof Xid)) {
    return false;
  }
  Xid other=(Xid)obj;
  return Arrays.equals(getGlobalTransactionId(),other.getGlobalTransactionId()) && Arrays.equals(getBranchQualifier(),other.getBranchQualifier()) && getFormatId() == other.getFormatId();
}
