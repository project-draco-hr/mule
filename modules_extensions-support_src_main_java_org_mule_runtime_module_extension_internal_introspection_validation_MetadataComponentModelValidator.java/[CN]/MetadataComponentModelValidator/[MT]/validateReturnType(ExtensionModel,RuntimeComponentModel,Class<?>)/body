{
  if (Object.class.equals(returnType) || Map.class.isAssignableFrom(returnType)) {
    if (component.getMetadataResolverFactory().getOutputResolver() instanceof NullMetadataResolver) {
      throw new IllegalOperationModelDefinitionException(String.format("Component '%s' in Extension '%s' specifies '%s' as a return type. Operations with " + "return type such as Object or Map must have defined a not null MetadataOutputResolver",component.getName(),extensionModel.getName(),returnType.getName()));
    }
  }
}
