{
  String id=this.getIdForEvent(event);
  String value=this.getValueForEvent(event);
  try {
    store.store(id,value);
    return super.processNext(event);
  }
 catch (  Exception e) {
    throw new RoutingException(CoreMessages.failedToWriteMessageToStore(id,assignedComponentName),event,this,e);
  }
}
