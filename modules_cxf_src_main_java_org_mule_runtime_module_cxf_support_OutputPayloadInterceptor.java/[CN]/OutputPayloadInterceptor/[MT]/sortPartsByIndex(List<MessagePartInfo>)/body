{
  Collections.sort(parts,new Comparator<MessagePartInfo>(){
    public int compare(    MessagePartInfo o1,    MessagePartInfo o2){
      if (o1.getIndex() < o2.getIndex()) {
        return -1;
      }
 else       if (o1.getIndex() == o2.getIndex()) {
        return 0;
      }
 else {
        return 1;
      }
    }
  }
);
}
