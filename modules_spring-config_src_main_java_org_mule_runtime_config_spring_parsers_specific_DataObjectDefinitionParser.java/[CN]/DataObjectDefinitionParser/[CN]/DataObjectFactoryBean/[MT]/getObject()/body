{
  if (data != null) {
    return data;
  }
  if (file != null) {
    if (binary) {
      data=IOUtils.toByteArray(IOUtils.getResourceAsStream(file,getClass()));
    }
 else {
      data=IOUtils.getResourceAsString(file,getClass());
    }
  }
 else   if (ref != null) {
    data=context.getBean(ref);
  }
  if (data == null) {
    throw new IllegalArgumentException("Data is null was not found");
  }
  return data;
}
