{
  parser.addAlias(ADDRESS_ATTRIBUTE,ENDPOINT_URI_ATTRIBUTE);
  parser.addMapping("createConnector","GET_OR_CREATE=0,ALWAYS_CREATE=1,NEVER_CREATE=2");
  parser.addAlias("transformers","transformer");
