{
  assertFalse(getService().isStarted());
  assertFalse(getService().isPaused());
  getService().dispose();
  getService().initialise();
  assertFalse(getService().isStarted());
  getService().dispose();
  assertFalse(getService().isStarted());
  try {
    getService().dispose();
    fail("Exception expected: Cannot dispose a service that is already disposed");
  }
 catch (  IllegalStateException e) {
  }
}
