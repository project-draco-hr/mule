{
  try {
    service.start();
    fail("Exception expected: Cannot start an uninitialised service");
  }
 catch (  Exception e) {
  }
  service.initialise();
  service.start();
  try {
    service.initialise();
    fail("Exception expected: Cannot initialise an already initialised service");
  }
 catch (  InitialisationException e) {
  }
  service.dispose();
}
