{
  assertFalse(service.isStarted());
  assertFalse(service.isPaused());
  service.stop();
  try {
    service.resume();
    fail("Exception expected: Cannot stop an uninitialised service");
  }
 catch (  MuleException e) {
  }
  service.initialise();
  assertFalse(service.isStarted());
  service.stop();
  assertFalse(service.isStarted());
  service.start();
  assertTrue(service.isStarted());
  service.stop();
  assertFalse(service.isStarted());
  service.stop();
  assertFalse(service.isStarted());
  service.dispose();
}
