{
  if (event.getExchangePattern().hasResponse()) {
    if (logger.isDebugEnabled()) {
      logger.debug("Service: " + service.getName() + " has received synchronous event on: "+ event.getEndpoint().getEndpointURI());
    }
  }
 else {
    if (logger.isDebugEnabled()) {
      logger.debug("Service: " + service.getName() + " has received asynchronous event on: "+ event.getEndpoint().getEndpointURI());
    }
  }
  return event;
}
