{
  assertEquals("BAR",map.get("FOO"));
  assertEquals("BAR",map.get("foo"));
  assertEquals("BAR",map.get("Foo"));
  assertEquals(new Integer(3),map.get("DOO"));
  assertEquals(new Integer(3),map.get("doo"));
  assertEquals(new Integer(3),map.get("Doo"));
  assertEquals(2,map.size());
  for (  Object o : map.keySet()) {
    assertTrue(o.equals("FOO") || o.equals("DOO"));
    assertFalse(o.equals("foo") || o.equals("doo"));
  }
}
