{
  ObjectStore objectStore=(ObjectStore)registryBroker.lookupObject(MuleProperties.OBJECT_STORE);
  if (objectStore == null) {
    Collection temp=registryBroker.lookupObjects(ObjectStore.class);
    if (temp.size() > 0) {
      objectStore=((ObjectStore)temp.iterator().next());
    }
  }
  return objectStore;
}
