{
  ListableObjectStore<Serializable> objectStore=(ListableObjectStore<Serializable>)registryBroker.lookupObject(MuleProperties.OBJECT_STORE);
  if (objectStore == null) {
    Collection<ListableObjectStore> temp=registryBroker.lookupObjects(ListableObjectStore.class);
    if (temp.size() > 0) {
      objectStore=temp.iterator().next();
    }
  }
  return objectStore;
}
