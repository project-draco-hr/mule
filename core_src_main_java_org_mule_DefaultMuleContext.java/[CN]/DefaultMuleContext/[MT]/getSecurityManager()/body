{
  SecurityManager securityManager=(SecurityManager)getRegistry().lookupObject(MuleProperties.OBJECT_SECURITY_MANAGER);
  if (securityManager == null) {
    Collection temp=getRegistry().lookupObjects(SecurityManager.class);
    if (temp.size() > 0) {
      securityManager=((SecurityManager)temp.iterator().next());
    }
  }
  return securityManager;
}
