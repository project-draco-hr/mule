{
  String includes=getProperty("Bundle-Includes");
  if (includes == null)   includes=getProperty("Include-Resource");
 else   warning("Please use Include-Resource instead of Bundle-Includes");
  if (includes == null)   return;
  Map<String,Map<String,String>> clauses=parseHeader(includes);
  for (Iterator<Map.Entry<String,Map<String,String>>> i=clauses.entrySet().iterator(); i.hasNext(); ) {
    Map.Entry<String,Map<String,String>> entry=i.next();
    doIncludeResource(jar,entry.getKey(),entry.getValue());
  }
}
