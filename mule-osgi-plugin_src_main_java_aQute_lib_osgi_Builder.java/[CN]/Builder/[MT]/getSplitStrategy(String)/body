{
  if (type == null)   return SPLIT_DEFAULT;
  if (type.equals("merge-last"))   return SPLIT_MERGE_LAST;
  if (type.equals("merge-first"))   return SPLIT_MERGE_FIRST;
  if (type.equals("error"))   return SPLIT_ERROR;
  if (type.equals("first"))   return SPLIT_FIRST;
  error("Invalid strategy for split-package: " + type);
  return SPLIT_DEFAULT;
}
