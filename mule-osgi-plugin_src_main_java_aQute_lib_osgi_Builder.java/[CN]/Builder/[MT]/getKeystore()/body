{
  if (keystore != null) {
    return keystore;
  }
  Map<String,Map<String,String>> header=parseHeader(getProperty("-keystore","../cnf/keystore"));
  if (header.size() == 0) {
    error("Keystore needed but no -keystore specified");
    return null;
  }
  if (header.size() > 1) {
    warning("Multiple keystores specified, can only specify one: %s",header);
  }
  for (  Map.Entry<String,Map<String,String>> entry : header.entrySet()) {
    return keystore=getKeystore(entry.getKey(),entry.getValue().get("provider:"),entry.getValue().get("password:"));
  }
  return null;
}
