{
  super(new URL[0],parent);
  try {
    if (domain == null) {
      throw new IllegalArgumentException("Domain name cannot be null");
    }
    this.domain=domain;
    validateAndGetDomainFolders();
    addUrls();
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Throwable t) {
    throw new RuntimeException(t);
  }
}
