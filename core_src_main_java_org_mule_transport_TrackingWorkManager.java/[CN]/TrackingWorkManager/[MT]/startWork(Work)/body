{
  try {
    workTracker.addWork(work);
    Work wrappedWork=new TrackeableWork(work);
    return delegate.startWork(wrappedWork);
  }
 catch (  WorkException e) {
    workTracker.removeWork(work);
    throw e;
  }
catch (  RuntimeException e) {
    workTracker.removeWork(work);
    throw e;
  }
}
