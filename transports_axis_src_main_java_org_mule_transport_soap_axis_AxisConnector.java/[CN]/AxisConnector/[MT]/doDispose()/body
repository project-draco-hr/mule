{
  try {
    Field field=AxisProperties.class.getDeclaredField("loaders");
    field.setAccessible(true);
    field.set(null,null);
    field=AxisProperties.class.getDeclaredField("nameDiscoverer");
    field.setAccessible(true);
    field.set(null,null);
  }
 catch (  Exception e) {
    logger.error("Error disposing AxisConnector, this may cause a memory leak. Error is: " + e.getMessage(),e);
  }
}
