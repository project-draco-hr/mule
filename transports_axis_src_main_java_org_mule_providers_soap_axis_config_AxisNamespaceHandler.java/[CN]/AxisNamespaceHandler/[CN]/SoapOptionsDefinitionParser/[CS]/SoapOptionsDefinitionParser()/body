{
  addDelegate(new ChildSingletonMapDefinitionParser("properties")).addCollection("properties").setIgnoredDefault(true).removeIgnored(MapEntryCombiner.KEY).registerPreProcessor(new AddAttribute(MapEntryCombiner.KEY,"axisOptions"));
  addChildDelegate(new AttributePropertiesDefinitionParser(MapEntryCombiner.VALUE)).addIgnored(AbstractMuleBeanDefinitionParser.ATTRIBUTE_NAME).addIgnored(MapEntryCombiner.KEY);
}
