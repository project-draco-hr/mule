{
  DataInputStream dis=new DataInputStream(is);
  byte[] buffer=new byte[32];
  int length;
  dis.mark(32);
  while ((length=dis.read(buffer)) == 0) {
  }
  if (length == -1) {
    return null;
  }
  dis.reset();
  length=dis.readInt();
  buffer=new byte[length];
  dis.readFully(buffer);
  return buffer;
}
