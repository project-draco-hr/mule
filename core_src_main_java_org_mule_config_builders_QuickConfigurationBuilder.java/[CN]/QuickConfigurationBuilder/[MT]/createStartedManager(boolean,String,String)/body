{
  if (manager.isStarted()) {
    throw new InitialisationException(new Message(Messages.MANAGER_ALREADY_STARTED),this);
  }
  if (serverUrl == null) {
    serverUrl="";
  }
  if (!MODEL_NOT_SET.equals(modeltype)) {
    manager.setModel(ModelFactory.createModel(modeltype));
  }
  manager.start();
  return manager;
}
