{
  try {
    getLifecycleManager().fireLifecycle(Disposable.PHASE_NAME);
  }
 catch (  LifecycleException e) {
    logger.error("Failed to shut down registry cleanly: " + getRegistryId(),e);
  }
  try {
    doDispose();
  }
 catch (  Exception e) {
    logger.error("Failed to cleanly dispose: " + e.getMessage(),e);
  }
}
