{
  if (isDisposed()) {
    return;
  }
  try {
    doDispose();
    lifecycleManager.firePhase(MuleServer.getMuleContext(),Disposable.PHASE_NAME);
    PropertyExtractorManager.clear();
    ExpressionEvaluatorManager.clearEvaluators();
  }
 catch (  Exception e) {
    logger.error("Failed to cleanly dispose: " + e.getMessage(),e);
  }
}
