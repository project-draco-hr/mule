{
  try {
    client.logout();
  }
 catch (  FTPConnectionClosedException e) {
  }
catch (  Exception e) {
    LOGGER.warn("Exception found trying to logout from ftp at " + toURL(null),e);
  }
 finally {
    try {
      client.disconnect();
    }
 catch (    Exception e) {
      LOGGER.warn("Exception found trying to disconnect from ftp at " + toURL(null),e);
    }
  }
}
