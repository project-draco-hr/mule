{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(new AppendingInterceptingMP("1"),new ReturnVoidMPInterceptongMP(),new AppendingMP("2"),new AppendingMP("3"),new AppendingInterceptingMP("4"),new AppendingMP("5"));
  assertSame("0",builder.build().process(getTestEventUsingFlow("0")).getMessage().getPayload());
}
