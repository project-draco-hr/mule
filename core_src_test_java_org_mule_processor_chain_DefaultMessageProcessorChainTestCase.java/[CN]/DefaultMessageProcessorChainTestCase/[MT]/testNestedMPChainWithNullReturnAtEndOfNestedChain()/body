{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(new AppendingMP("1"),new DefaultMessageProcessorChainBuilder().chain(new AppendingMP("a"),new AppendingMP("b"),new ReturnNullMP()).build(),new AppendingMP("2"));
  assertSame(VoidMuleEvent.getInstance(),builder.build().process(getTestEventUsingFlow("0")));
}
