{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(new AppendingInterceptingMP("1"),new AppendingInterceptingMP("2"),new AppendingInterceptingMP("3"));
  assertEquals("0before1before2before3after3after2after1",process(builder.build(),getTestEventUsingFlow("0")).getMessage().getPayload());
