{
  DefaultMessageProcessorChainBuilder builder=new DefaultMessageProcessorChainBuilder();
  builder.chain(new AppendingInterceptingMP("1"),new AppendingMP("2"),new ReturnNullMP(),new AppendingMP("3"),new AppendingInterceptingMP("4"),new AppendingMP("5"));
  assertEquals("0before123before45after4after1",builder.build().process(getTestEventUsingFlow("0")).getMessageAsString());
}
