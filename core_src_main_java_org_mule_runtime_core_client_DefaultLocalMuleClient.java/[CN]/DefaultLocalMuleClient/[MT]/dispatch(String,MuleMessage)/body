{
  final MessageProcessor connectorMessageProcessor=getConnectorMessageProcessLocator().locateConnectorOperation(url,newOptions().outbound().build(),MessageExchangePattern.ONE_WAY);
  if (connectorMessageProcessor != null) {
    connectorMessageProcessor.process(createOneWayMuleEvent(message));
  }
 else {
    throw createUnsupportedUrlException(url);
  }
}
