{
  if (muleEvent == null) {
    ErrorType anyErrorType=muleContext.getErrorTypeLocator().getAnyErrorType();
    return left(builder(new MuleRuntimeException(createStaticMessage(MESSAGE_FILTERED_ERROR_MESSAGE))).errorType(anyErrorType).build());
  }
  if (muleEvent.getError() == null) {
    return right(muleEvent.getMessage());
  }
  return left(muleEvent.getError());
}
