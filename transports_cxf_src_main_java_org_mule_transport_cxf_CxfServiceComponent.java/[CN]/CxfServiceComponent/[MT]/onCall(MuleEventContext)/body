{
  if (logger.isDebugEnabled()) {
    logger.debug(eventContext);
  }
  String requestPath=parseHttpRequestProperty(eventContext.getMessage().getStringProperty(HttpConnector.HTTP_REQUEST_PROPERTY,PropertyScope.INBOUND,StringUtils.EMPTY));
  if (requestPath.indexOf('?') > -1) {
    return generateWSDLOrXSD(eventContext,requestPath);
  }
 else {
    return sendToDestination(eventContext);
  }
}
