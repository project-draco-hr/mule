{
  String exeCmd=dotExeFileName + " -T" + outFormat+ " "+ dotFileName;
  Process p=Runtime.getRuntime().exec(exeCmd);
  InputStream is=p.getInputStream();
  byte buf[]=new byte[32768];
  do {
    int len=is.read(buf);
    if (len > 0) {
      out.write(buf,0,len);
    }
 else {
      is.close();
      return;
    }
  }
 while (true);
}
