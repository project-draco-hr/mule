{
  doDispatch(event);
  if (event.getEndpoint().isRemoteSync()) {
    DatagramPacket result=receive(socket,event.getTimeout());
    if (result == null) {
      return null;
    }
    return new MuleMessage(connector.getMessageAdapter(result),event.getMessage());
  }
 else {
    return event.getMessage();
  }
}
