{
  super.postProcess(context,assembler,element);
  String propertyName=getPropertyName(element);
  if (null != propertyName) {
    if (!assembler.getBean().getBeanDefinition().isSingleton()) {
      assembler.insertBeanInTarget(propertyName);
    }
 else {
      assembler.getTarget().getPropertyValues().addPropertyValue(propertyName,new RuntimeBeanReference(element.getAttribute(AbstractMuleBeanDefinitionParser.ATTRIBUTE_NAME)));
    }
  }
}
