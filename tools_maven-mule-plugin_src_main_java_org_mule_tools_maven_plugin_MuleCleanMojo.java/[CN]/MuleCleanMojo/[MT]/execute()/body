{
  final File app=new File(this.outputDirectory,this.finalName);
  if (app.exists()) {
    final boolean success=app.delete();
    if (success) {
      getLog().info("Deleted Mule App: " + app);
    }
 else {
      getLog().info("Failed to delete Mule App: " + app);
    }
  }
 else {
    getLog().info("Nothing to clean");
  }
}
