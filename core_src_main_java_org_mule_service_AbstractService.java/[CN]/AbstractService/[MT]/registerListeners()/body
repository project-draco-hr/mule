{
  @SuppressWarnings("unchecked") List<InboundEndpoint> endpoints=getIncomingEndpoints();
  for (  InboundEndpoint endpoint : endpoints) {
    try {
      endpoint.getConnector().registerListener(this,endpoint);
    }
 catch (    MuleException e) {
      throw e;
    }
catch (    Exception e) {
      throw new ModelException(CoreMessages.failedtoRegisterOnEndpoint(name,endpoint.getEndpointURI()),e);
    }
  }
}
