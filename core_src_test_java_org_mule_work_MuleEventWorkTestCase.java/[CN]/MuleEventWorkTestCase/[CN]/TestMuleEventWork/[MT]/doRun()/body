{
  assertNotSame("MuleEvent",event,originalEvent);
  assertNotNull("RequestContext.getEvent() is null",RequestContext.getEvent());
  try {
    ((DefaultMuleMessage)event.getMessage()).assertAccess(ThreadSafeAccess.WRITE);
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
  latch.countDown();
}
