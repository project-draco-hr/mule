{
  File f;
  if (src instanceof String) {
    f=new File(src.toString());
    if (!f.exists()) {
      URL url=ClassUtils.getResource(src.toString(),getClass());
      f=new File(new URI(url.toString()));
      return Arrays.asList(f.listFiles()).iterator();
    }
  }
  f=(File)src;
  return Arrays.asList(f.listFiles()).iterator();
}
