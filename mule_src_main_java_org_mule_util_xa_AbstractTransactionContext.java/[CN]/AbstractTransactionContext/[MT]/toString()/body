{
  StringBuffer sb=new StringBuffer();
  sb.append(id).append("[");
  sb.append(getStatusString());
  if (suspended) {
    sb.append(", suspended");
  }
  if (readOnly) {
    sb.append(", readonly");
  }
  if (finished) {
    sb.append(", finished");
  }
  sb.append("]");
  return sb.toString();
}
