{
  for (  ClassLoader classLoader : classLoaders) {
    String library=findLibrary(s,classLoader);
    if (library != null) {
      if (logger.isDebugEnabled()) {
        logger.debug(String.format("Library '%s' found in classLoader '%s",s,classLoader));
      }
      return library;
    }
  }
  return null;
}
