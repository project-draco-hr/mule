{
  try {
    RequestContext.setEvent(event);
    doDispatch(event);
  }
 catch (  Exception e) {
    try {
      dispose();
    }
 catch (    UMOException e1) {
    }
    getConnector().handleException(e);
  }
 finally {
    if (disposeOnCompletion) {
      try {
        dispose();
      }
 catch (      UMOException e) {
        logger.error("Failed to dispose dispatcher: " + e,e);
      }
    }
  }
}
