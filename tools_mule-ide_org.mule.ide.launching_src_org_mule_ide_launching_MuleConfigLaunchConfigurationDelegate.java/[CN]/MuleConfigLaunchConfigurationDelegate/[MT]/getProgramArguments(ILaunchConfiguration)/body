{
  String configName=configuration.getAttribute(IMuleConfigLaunchConfigurationConstants.ATTR_CONFIG_FILE_NAME,(String)null);
  String projectName=configuration.getAttribute(IJavaLaunchConfigurationConstants.ATTR_PROJECT_NAME,(String)null);
  if (configName == null || projectName == null)   abort("Missing launcher data for Mule UMO config launch",null,42);
  IProject project=ResourcesPlugin.getWorkspace().getRoot().getProject(projectName);
  if (project == null)   abort("No project found called " + projectName,null,43);
  IFile file=project.getFile(configName);
  if (file == null)   abort("No file found called " + configName,null,43);
  return "-config \"" + file.getRawLocation() + "\"";
}
