{
  String value=name.replaceAll("//",SEPARATOR);
  value=value.replaceAll("/",SEPARATOR);
  value=value.replaceAll("\\?",SEPARATOR);
  value=value.replaceAll("&",SEPARATOR);
  value=value.replaceAll(":",SEPARATOR);
  value=value.replaceAll("=","-");
  value=value.replaceAll("\\" + SEPARATOR + "\\"+ SEPARATOR,SEPARATOR);
  return value;
}
