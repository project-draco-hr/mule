{
  List<Object> list=CastUtils.cast(cxfMessage.getContent(List.class));
  if (list == null) {
    Object object=cxfMessage.getContent(Object.class);
    if (object != null) {
      return object;
    }
 else {
      return new Object[0];
    }
  }
  if ((list.size() == 1) && (list.get(0) != null)) {
    return list.get(0);
  }
 else {
    return list.toArray();
  }
}
