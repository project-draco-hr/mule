{
  String temp="j:namespace";
  String result=PropertiesHelper.removeXmlNamespacePrefix(temp);
  assertEquals("namespace",result);
  temp="this-namespace";
  result=PropertiesHelper.removeNamespacePrefix(temp);
  assertEquals("this-namespace",result);
  temp="namespace";
  result=PropertiesHelper.removeNamespacePrefix(temp);
  assertEquals("namespace",result);
}
