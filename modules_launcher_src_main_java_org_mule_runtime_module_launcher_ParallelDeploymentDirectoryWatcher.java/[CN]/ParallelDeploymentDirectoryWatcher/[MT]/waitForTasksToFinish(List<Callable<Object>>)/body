{
  try {
    final List<Future<Object>> futures=threadPoolExecutor.invokeAll(tasks);
    for (    Future<Object> future : futures) {
      try {
        future.get();
      }
 catch (      ExecutionException e) {
      }
    }
  }
 catch (  InterruptedException e) {
    Thread.currentThread().interrupt();
  }
}
