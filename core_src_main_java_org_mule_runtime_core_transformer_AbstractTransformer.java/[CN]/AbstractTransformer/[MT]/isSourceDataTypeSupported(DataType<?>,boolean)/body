{
  int numTypes=sourceTypes.size();
  if (numTypes == 0) {
    return !exactMatch;
  }
  for (  DataType<?> sourceType : sourceTypes) {
    if (exactMatch) {
      if (sourceType.equals(dataType)) {
        return true;
      }
    }
 else {
      if (sourceType.isCompatibleWith(dataType)) {
        return true;
      }
    }
  }
  return false;
}
