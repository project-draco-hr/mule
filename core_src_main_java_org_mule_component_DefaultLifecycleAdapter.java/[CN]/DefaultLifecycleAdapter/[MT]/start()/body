{
  if (isStartable) {
    try {
      return LifecycleTransitionResult.startOrStopAll(((Startable)componentObject).start(),new LifecycleTransitionResult.Closure(){
        public LifecycleTransitionResult doContinue(){
          started=true;
          return LifecycleTransitionResult.OK;
        }
      }
);
    }
 catch (    Exception e) {
      throw new DefaultMuleException(CoreMessages.failedToStart("UMO Service: " + component.getService().getName()),e);
    }
  }
 else {
    started=true;
    return LifecycleTransitionResult.OK;
  }
}
