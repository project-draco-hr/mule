{
  if (isDisposable) {
    try {
      muleContext.getRegistry().unregisterObject(createRegistryHardRefName(component));
      componentObject.clear();
      componentObject.enqueue();
    }
 catch (    Exception e) {
      logger.error("failed to dispose: " + component.getService().getName(),e);
    }
  }
  disposed=true;
}
