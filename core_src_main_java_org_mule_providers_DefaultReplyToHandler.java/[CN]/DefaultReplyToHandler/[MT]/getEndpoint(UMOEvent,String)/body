{
  UMOImmutableEndpoint endpoint=(UMOImmutableEndpoint)endpointCache.get(endpointUri);
  if (endpoint == null) {
    endpoint=RegistryContext.getRegistry().lookupEndpointFactory().getOutboundEndpoint(endpointUri,event.getManagementContext());
    endpointCache.put(endpointUri,endpoint);
  }
  return endpoint;
}
