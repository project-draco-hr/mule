{
  readOnly=false;
  if (added == null) {
    added=new HashMap();
  }
  List queueAdded=(List)added.get(queue);
  if (queueAdded == null) {
    queueAdded=new ArrayList();
    added.put(queue,queueAdded);
  }
  if (queue.offer(null,queueAdded.size(),timeout)) {
    queueAdded.add(item);
    return true;
  }
 else {
    return false;
  }
}
