{
  if (!disposed.get()) {
    disposing.set(true);
    try {
      if (started.get()) {
        stop();
      }
    }
 catch (    MuleException e) {
      logger.error(CoreMessages.failedToStop(toString()));
    }
    try {
      doDispose();
    }
 catch (    Exception e) {
      logger.warn(CoreMessages.failedToDispose(toString()),e);
    }
 finally {
      disposed.set(true);
      disposing.set(false);
      initialised.set(false);
    }
  }
}
