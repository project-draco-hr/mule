{
  StringBuilder sb=new StringBuilder();
  if (flowConstruct != null) {
    sb.append(flowConstruct.getName());
    sb.append(".");
  }
  sb.append("component");
  if (!(flowConstruct instanceof Service || flowConstruct instanceof SimpleService)) {
    sb.append(".");
    sb.append(System.identityHashCode(this));
  }
  return sb.toString();
}
