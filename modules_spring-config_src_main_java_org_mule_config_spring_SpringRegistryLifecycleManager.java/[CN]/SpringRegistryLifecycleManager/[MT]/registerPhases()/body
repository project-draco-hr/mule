{
  registerPhase(NotInLifecyclePhase.PHASE_NAME,NOT_IN_LIFECYCLE_PHASE,new EmptyLifecycleCallback<AbstractRegistryBroker>());
  registerPhase(Initialisable.PHASE_NAME,new SpringContextInitialisePhase());
  registerPhase(Startable.PHASE_NAME,new MuleContextStartPhase(),new EmptyLifecycleCallback<AbstractRegistryBroker>());
  registerPhase(Stoppable.PHASE_NAME,new MuleContextStopPhase(),new EmptyLifecycleCallback<AbstractRegistryBroker>());
  registerPhase(Disposable.PHASE_NAME,new SpringContextDisposePhase());
}
