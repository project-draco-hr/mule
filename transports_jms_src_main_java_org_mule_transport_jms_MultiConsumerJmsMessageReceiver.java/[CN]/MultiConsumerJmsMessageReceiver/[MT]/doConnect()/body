{
  logger.debug("doConnect()");
  if (!consumers.isEmpty()) {
    throw new IllegalStateException("List should be empty, there may be a concurrency issue here (see EE-1275)");
  }
  SubReceiver sub;
  for (int i=0; i < receiversCount; i++) {
    sub=new SubReceiver();
    sub.doConnect();
    consumers.add(sub);
  }
}
