{
  MuleEvent event=MuleEvent.builder(create(flowConstruct,getConfigurationProviderName())).message((org.mule.runtime.core.api.MuleMessage)message).exchangePattern(REQUEST_RESPONSE).flow(flowConstruct).build();
  messageProcessingManager.processMessage(new ExtensionFlowProcessingTemplate(event,messageProcessor,downCast(completionHandler)),createProcessingContext());
}
