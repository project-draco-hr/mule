{
  TcpSocketKey socketKey=(TcpSocketKey)key;
  boolean keepSocketOpen=MapUtils.getBooleanValue(socketKey.getEndpoint().getProperties(),TcpConnector.KEEP_SEND_SOCKET_OPEN_PROPERTY,socketKey.getConnector().isKeepSendSocketOpen());
  Socket socket=(Socket)object;
  if (!keepSocketOpen) {
    try {
      if (socket != null) {
        socket.close();
      }
    }
 catch (    IOException e) {
      logger.debug("Failed to close socket after dispatch: " + e.getMessage());
    }
  }
}
