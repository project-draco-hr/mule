{
  if (template == null) {
    return null;
  }
  String value=getEnvironment().getProperties().getProperty(template,null);
  if (value == null && template.startsWith("${")) {
    value=getEnvironment().getProperties().getProperty(template.substring(2,template.length() - 1),template);
  }
 else {
    value=template;
  }
  return value;
}
