{
  MuleMessage message=MuleMessage.builder().payload("").build();
  MuleEvent event=MuleEvent.builder(context).message(message).exchangePattern(ONE_WAY).flow(flowConstruct).addFlowVariable("foo","bar_old").build();
  evaluate("flowVars['foo']='bar'",event);
  assertEquals("bar",event.getFlowVariable("foo"));
}
