{
  if (node instanceof Assignment) {
    String assignmentVar=((Assignment)node).getAssignmentVar();
    if (PAYLOAD.equals(assignmentVar) || MESSAGE_PAYLOAD.equals(assignmentVar)) {
      builder.message(MuleMessage.builder(event.getMessage()).payload(typedValue.getValue()).mediaType(typedValue.getDataType().getMediaType()).build());
      return true;
    }
  }
  return false;
}
