{
  try {
    VelocityContext velocityContext=new VelocityContext();
    for (Iterator iter=context.keySet().iterator(); iter.hasNext(); ) {
      String key=(String)iter.next();
      String value=(String)context.get(key);
      velocityContext.put(key,value);
    }
    velocityContext.put("graph",graph);
    Template t=ve.getTemplate("./src/resources/template/mule-config.vm");
    File file=new File(env.getConfig().getOutputDirectory() + "/" + context.get("htmlFileName"));
    FileWriter writer=new FileWriter(file);
    env.log("generating " + file);
    t.merge(velocityContext,writer);
    writer.flush();
    writer.close();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
