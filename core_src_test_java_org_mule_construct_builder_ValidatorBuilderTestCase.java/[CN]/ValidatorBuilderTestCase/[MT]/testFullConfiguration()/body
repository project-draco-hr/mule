{
  Validator validator=new ValidatorBuilder().name("test-validator-full").inboundAddress("test://foo.in").validationFilter(new PayloadTypeFilter(Integer.class)).ackExpression("#[string:GOOD:#[message:payload]]").nackExpression("#[string:BAD:#[message:payload]]").outboundAddress("test://foo.out").exceptionStrategy(new DefaultServiceExceptionStrategy(muleContext)).build(muleContext);
  assertEquals("test-validator-full",validator.getName());
}
