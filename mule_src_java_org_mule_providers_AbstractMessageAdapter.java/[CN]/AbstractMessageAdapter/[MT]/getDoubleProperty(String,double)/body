{
  Object result=properties.get(name);
  if (result != null) {
    if (result instanceof Double) {
      return ((Double)result).doubleValue();
    }
 else {
      try {
        return Double.parseDouble(result.toString());
      }
 catch (      NumberFormatException e) {
        return defaultValue;
      }
    }
  }
 else {
    return defaultValue;
  }
}
