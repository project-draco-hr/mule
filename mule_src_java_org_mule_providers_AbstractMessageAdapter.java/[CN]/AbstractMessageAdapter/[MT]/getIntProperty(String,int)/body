{
  Object result=properties.get(name);
  if (result != null) {
    if (result instanceof Integer) {
      return ((Integer)result).intValue();
    }
 else {
      try {
        return Integer.parseInt(result.toString());
      }
 catch (      NumberFormatException e) {
        return defaultValue;
      }
    }
  }
 else {
    return defaultValue;
  }
}
