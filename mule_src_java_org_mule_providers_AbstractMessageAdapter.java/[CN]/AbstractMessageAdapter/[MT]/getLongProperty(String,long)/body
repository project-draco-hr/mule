{
  Object result=properties.get(name);
  if (result != null) {
    if (result instanceof Long) {
      return ((Long)result).longValue();
    }
 else {
      try {
        return Long.parseLong(result.toString());
      }
 catch (      NumberFormatException e) {
        return defaultValue;
      }
    }
  }
 else {
    return defaultValue;
  }
}
