{
  if (props != null) {
synchronized (props) {
      for (Iterator iter=props.entrySet().iterator(); iter.hasNext(); ) {
        Map.Entry entry=(Map.Entry)iter.next();
        String key=(String)entry.getKey();
        Object value=entry.getValue();
        if (value != null) {
          setProperty(key,value);
        }
      }
    }
  }
}
