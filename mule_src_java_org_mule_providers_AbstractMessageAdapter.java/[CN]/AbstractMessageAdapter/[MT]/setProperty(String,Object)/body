{
  if (key != null) {
    if (value != null) {
      properties.put(key,value);
    }
 else {
      logger.warn("setProperty(key, value) called with null value; removing key: " + key + "; please report the following stack trace to dev@mule.codehaus.org.",new Throwable());
      properties.remove(key);
    }
  }
 else {
    logger.warn("setProperty(key, value) ignored because of null key for object: " + value + "; please report the following stack trace to dev@mule.codehaus.org.",new Throwable());
  }
}
