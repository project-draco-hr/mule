{
  MuleEvent muleEvent=buildMuleEvent();
  verifyLogCall(buildLoggerMessageProcessorForExpressionEvaluation(level),level,level,muleEvent,"text to log");
  verifyLogCall(buildLoggerMessageProcessorForExpressionEvaluation(level),level,"not" + level,muleEvent,"text to log");
  verifyExpressionEvaluation(buildLoggerMessageProcessorForExpressionEvaluation(level),level,level,muleEvent,times(1));
  verifyExpressionEvaluation(buildLoggerMessageProcessorForExpressionEvaluation(level),level,"not" + level,muleEvent,never());
}
