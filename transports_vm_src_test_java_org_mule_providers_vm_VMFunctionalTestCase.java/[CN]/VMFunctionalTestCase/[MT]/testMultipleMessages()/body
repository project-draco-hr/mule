{
  MuleClient client=new MuleClient();
  client.dispatch("vm://in","Marco",null);
  client.dispatch("vm://in","Marco",null);
  client.dispatch("vm://in","Marco",null);
  UMOMessage response;
  for (int i=0; i < 3; ++i) {
    response=client.receive("vm://out",1000);
    assertNotNull("Response is null",response);
    assertEquals("Polo",response.getPayload());
  }
}
