{
  if (soapBodyParts.isEmpty()) {
    Map parts=inputMessage.getParts();
    if (parts.size() == 1) {
      return (Part)parts.values().iterator().next();
    }
  }
 else {
    if (soapBodyParts.size() == 1) {
      String partName=soapBodyParts.get(0);
      return inputMessage.getPart(partName);
    }
  }
  return null;
}
