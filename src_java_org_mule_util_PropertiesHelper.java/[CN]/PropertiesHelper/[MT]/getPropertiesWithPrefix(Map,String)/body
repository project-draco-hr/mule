{
  Map newProps=new HashMap();
  Map.Entry entry;
  for (Iterator iterator=props.entrySet().iterator(); iterator.hasNext(); ) {
    entry=(Map.Entry)iterator.next();
    if (entry.getKey().toString().startsWith(prefix)) {
      newProps.put(entry.getValue(),entry.getKey());
    }
  }
  return newProps;
}
