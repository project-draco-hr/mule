{
  if (endpoint == null) {
    return;
  }
  String name;
  if (endpoint instanceof ImmutableEndpoint) {
    name=((ImmutableEndpoint)endpoint).getName();
  }
 else {
    name=endpoint.toString();
  }
  Long cpt=(Long)routed.get(name);
  long count=0;
  if (cpt != null) {
    count=cpt.longValue();
  }
  routed.put(name,new Long(++count));
  totalRouted++;
  totalReceived++;
}
