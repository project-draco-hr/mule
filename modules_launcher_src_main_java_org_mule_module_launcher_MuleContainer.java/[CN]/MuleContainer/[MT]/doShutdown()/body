{
  for (  MuleCoreExtension extension : coreExtensions.values()) {
    try {
      extension.dispose();
    }
 catch (    Exception ex) {
      logger.fatal("Error shutting down core extension " + extension.getName());
    }
  }
  if (deploymentService != null) {
    deploymentService.stop();
  }
  System.exit(0);
}
