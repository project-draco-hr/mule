{
  if (registerShutdownHook) {
    registerShutdownHook();
  }
  final MuleContainerStartupSplashScreen splashScreen=new MuleContainerStartupSplashScreen();
  splashScreen.doBody();
  logger.info(splashScreen.toString());
  try {
    deployer=new DefaultMuleDeployer();
    deployer.deploy();
  }
 catch (  Throwable e) {
    shutdown(e);
  }
}
