{
  int numTypes=sourceTypes.size();
  if (numTypes == 0) {
    return !exactMatch;
  }
  for (int i=0; i < numTypes; i++) {
    Class anotherClass=sourceTypes.get(i);
    if (exactMatch) {
      if (anotherClass.equals(aClass)) {
        return true;
      }
    }
 else     if (anotherClass.isAssignableFrom(aClass)) {
      return true;
    }
  }
  return false;
}
