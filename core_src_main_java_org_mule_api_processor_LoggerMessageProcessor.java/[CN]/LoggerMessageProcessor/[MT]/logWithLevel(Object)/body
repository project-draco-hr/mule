{
  if ("ERROR".equals(level)) {
    logger.error(object);
  }
 else   if ("WARN".equals(level)) {
    logger.warn(object);
  }
 else   if ("INFO".equals(level)) {
    if (logger.isInfoEnabled()) {
      logger.info(object);
    }
  }
 else   if ("DEBUG".equals(level)) {
    if (logger.isDebugEnabled()) {
      logger.debug(object);
    }
  }
 else   if ("TRACE".equals(level)) {
    if (logger.isTraceEnabled()) {
      logger.trace(object);
    }
  }
}
