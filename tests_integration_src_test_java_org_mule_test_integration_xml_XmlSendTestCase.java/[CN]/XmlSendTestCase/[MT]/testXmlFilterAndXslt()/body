{
  InputStream xml=getClass().getResourceAsStream("request.xml");
  assertNotNull(xml);
  MuleClient client=muleContext.getClient();
  InternalMessage message=client.send("http://localhost:" + dynamicPort.getNumber() + "/xml-xslt-parse",InternalMessage.of(xml),httpOptions).getRight();
  assertThat(200,is(message.<Integer>getInboundProperty(HTTP_STATUS_PROPERTY)));
}
