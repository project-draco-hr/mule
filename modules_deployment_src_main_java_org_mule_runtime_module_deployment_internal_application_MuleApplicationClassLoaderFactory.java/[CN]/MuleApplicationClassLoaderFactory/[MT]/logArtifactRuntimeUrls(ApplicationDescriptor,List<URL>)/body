{
  StringBuilder sb=new StringBuilder();
  sb.append(String.format("[%s] Loading the following jars:%n",descriptor.getName()));
  sb.append("=============================").append(LINE_SEPARATOR);
  for (  URL url : urls) {
    sb.append(url).append(LINE_SEPARATOR);
  }
  sb.append("=============================").append(LINE_SEPARATOR);
  logger.info(sb.toString());
}
