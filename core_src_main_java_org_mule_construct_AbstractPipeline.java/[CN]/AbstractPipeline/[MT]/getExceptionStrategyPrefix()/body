{
  String esPrefix="/" + getName() + "/es";
  String globalName=null;
  if (exceptionListener instanceof GlobalNameableObject) {
    globalName=((GlobalNameableObject)exceptionListener).getGlobalName();
  }
  if (globalName != null) {
    esPrefix="/" + getName() + "/"+ globalName+ "/es";
  }
  return esPrefix;
}
