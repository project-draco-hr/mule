{
  Callable c=new Callable(){
    public Object call() throws InterruptedException {
      Thread.sleep(200);
      wasCalled=true;
      return null;
    }
  }
;
  FutureMessageResult f=new FutureMessageResult(c);
  f.execute();
  try {
    f.getMessage(50);
    fail();
  }
 catch (  TimeoutException tex) {
  }
  assertFalse(wasCalled);
}
