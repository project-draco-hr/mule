{
  super(new URL[0]);
  try {
    final List<URL> urlsList=classPath.getURLs();
    for (    URL url : urlsList) {
      if (logger.isDebugEnabled()) {
        logger.debug("adding URL " + url);
      }
      addURL(url);
    }
  }
 catch (  Exception e) {
    if (logger.isDebugEnabled()) {
      logger.debug(e);
    }
  }
}
