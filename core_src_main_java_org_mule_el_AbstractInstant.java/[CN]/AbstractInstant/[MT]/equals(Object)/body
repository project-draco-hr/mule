{
  if (!(obj instanceof AbstractInstant)) {
    return false;
  }
 else {
    AbstractInstant other=(AbstractInstant)obj;
    return calendar.getTimeInMillis() == other.calendar.getTimeInMillis() && calendar.getFirstDayOfWeek() == other.calendar.getFirstDayOfWeek() && calendar.getFirstDayOfWeek() == other.calendar.getFirstDayOfWeek() && calendar.getTimeZone().getOffset(calendar.getTimeInMillis()) == other.calendar.getTimeZone().getOffset(other.calendar.getTimeInMillis());
  }
}
