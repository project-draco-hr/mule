{
  String realmHeader="Basic realm=";
  if (realm != null) {
    realmHeader+="\"" + realm + "\"";
  }
  String finalRealmHeader=realmHeader;
  return Event.builder(event).message(InternalMessage.builder(event.getMessage()).addOutboundProperty(WWW_AUTHENTICATE,finalRealmHeader).addOutboundProperty(HTTP_STATUS_PROPERTY,UNAUTHORIZED.getStatusCode()).build()).build();
}
