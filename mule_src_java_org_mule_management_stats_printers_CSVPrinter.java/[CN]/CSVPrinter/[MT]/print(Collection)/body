{
  try {
    String[][] table=getTable(stats);
    int i=(printHeaders ? 0 : 1);
    for (; i < table.length; i++) {
      for (int j=0; j < table[0].length; j++) {
        print(table[i][j]);
        if (j + 1 != table[i].length)         print(delim);
      }
      println();
    }
  }
 catch (  Throwable e) {
    e.printStackTrace();
  }
}
