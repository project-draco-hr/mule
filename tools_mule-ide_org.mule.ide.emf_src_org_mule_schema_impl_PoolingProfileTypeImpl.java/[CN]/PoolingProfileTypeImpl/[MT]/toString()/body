{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", exhaustedAction: ");
  if (exhaustedActionESet)   result.append(exhaustedAction);
 else   result.append("<unset>");
  result.append(", factory: ");
  if (factoryESet)   result.append(factory);
 else   result.append("<unset>");
  result.append(", initialisationPolicy: ");
  if (initialisationPolicyESet)   result.append(initialisationPolicy);
 else   result.append("<unset>");
  result.append(", maxActive: ");
  result.append(maxActive);
  result.append(", maxIdle: ");
  result.append(maxIdle);
  result.append(", maxWait: ");
  result.append(maxWait);
  result.append(')');
  return result.toString();
}
