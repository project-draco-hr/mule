{
  MuleClient client=new MuleClient(muleContext);
  MuleMessage reply=client.send("axis:http://localhost:" + dynamicPort1.getNumber() + "/services/AxisService?method=receive",new DefaultMuleMessage("test",muleContext));
  assertNotNull(reply);
  assertNotNull(reply.getPayload());
  assertTrue(reply.getPayload() instanceof String);
  assertEquals("Received: test",reply.getPayloadAsString());
}
