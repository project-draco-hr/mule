{
  MuleClient client=new MuleClient(muleContext);
  try {
    client.send("axis:http://localhost:" + getPorts().get(0) + "/services/AxisService?method=throwsException",new DefaultMuleMessage("test",muleContext));
    fail("should have thrown exception");
  }
 catch (  DispatchException e) {
    assertEquals(TestComponentException.class.getName() + ": " + TestComponentException.MESSAGE_PREFIX+ TestComponent.EXCEPTION_MESSAGE,e.getCause().getMessage());
  }
}
