{
  if (value == null) {
    throw new ConversionException("No value specified");
  }
  if (value instanceof UMOTransactionFactory) {
    return (value);
  }
  try {
    Object factory=ClassHelper.loadClass(value.toString(),getClass()).newInstance();
    return factory;
  }
 catch (  Exception e) {
    throw new ConversionException(e);
  }
}
