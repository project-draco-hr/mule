  IOUtils.createDirs(new File(rootDir,ASSEMBLIES_DIR));
  IOUtils.createDirs(getAutoInstallDir(rootDir));
  IOUtils.createDirs(getAutoDeployDir(rootDir));
  IOUtils.createDirs(getAutoDeployProcessedDir(rootDir));
}
