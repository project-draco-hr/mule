{
  IOUtils.createDirs(rootDir);
  IOUtils.createDirs(new File(rootDir,ENGINES_DIR));
  IOUtils.createDirs(new File(rootDir,BINDINGS_DIR));
  IOUtils.createDirs(new File(rootDir,WORKSPACE_DIR));
