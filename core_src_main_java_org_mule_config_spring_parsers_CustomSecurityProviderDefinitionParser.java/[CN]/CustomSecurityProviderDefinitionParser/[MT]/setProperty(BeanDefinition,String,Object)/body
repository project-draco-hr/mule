{
  if (NAME.equals(name)) {
    this.name=(String)value;
    if (null != provider) {
      setName();
    }
  }
 else {
    super.setProperty(target,name,value);
    if (PROVIDERS.equals(name)) {
      provider=(BeanDefinition)value;
      if (null != this.name) {
        setName();
      }
    }
  }
}
