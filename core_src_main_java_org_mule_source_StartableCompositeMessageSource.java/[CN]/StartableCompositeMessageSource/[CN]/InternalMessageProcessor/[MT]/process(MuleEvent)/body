{
  if (started.get() || starting.get()) {
    if (listener instanceof OutboundEndpoint) {
      event=new DefaultMuleEvent(event.getMessage(),(OutboundEndpoint)listener,event.getSession());
    }
    return listener.process(event);
  }
 else {
    log.warn(String.format("A message was receieved from MessageSource, but message source is stopped. Message will be discarded.%n" + "  Message: %s%n" + "  MessageSource:%s",event,this));
    return null;
  }
}
