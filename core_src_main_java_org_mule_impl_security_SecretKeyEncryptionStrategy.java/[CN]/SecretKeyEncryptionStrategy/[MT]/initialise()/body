{
  if (key == null) {
    if (keyFactory == null) {
      throw new InitialisationException(CoreMessages.objectIsNull("Key / KeyFactory"),this);
    }
 else {
      try {
        key=(byte[])keyFactory.create();
      }
 catch (      Exception e) {
        throw new InitialisationException(e,this);
      }
    }
  }
  super.initialise();
}
