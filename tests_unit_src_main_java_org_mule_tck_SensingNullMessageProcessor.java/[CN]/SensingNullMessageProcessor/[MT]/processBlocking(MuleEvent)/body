{
  sense(event);
  if (StringUtils.isNotEmpty(appendString)) {
    event=append(event);
  }
  latch.countDown();
  if (source.listener != null) {
    return source.listener.process(event);
  }
 else {
    if (event.getExchangePattern().hasResponse()) {
      return event;
    }
 else {
      return VoidMuleEvent.getInstance();
    }
  }
}
