{
  Object obj=objects.get(id);
  if (obj != null) {
    return obj;
  }
  try {
    RecordLocation loc=(RecordLocation)marks.get(id);
    Packet packet=journal.read(loc);
    return readStorePacket(packet.sliceAsBytes());
  }
 catch (  IOException e) {
    throw e;
  }
catch (  Exception e) {
    throw (IOException)new IOException().initCause(e);
  }
}
