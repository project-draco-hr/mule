{
  String param;
  if (src instanceof byte[]) {
    param=new String((byte[])src);
  }
 else {
    param=src.toString();
  }
  int equals=param.indexOf("=");
  if (equals > -1) {
    return param.substring(equals + 1);
  }
 else {
    throw new TransformerException("Failed to parse param string: " + param);
  }
}
