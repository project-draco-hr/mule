{
  ClassLoader originalClassLoader=Thread.currentThread().getContextClassLoader();
  try {
    Thread.currentThread().setContextClassLoader(new MyClassClassLoader());
    assertFalse((Boolean)muleContext.getExpressionManager().evaluate("groovy:payload instanceof MyClass",new DefaultMuleMessage("test",muleContext)));
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
 finally {
    Thread.currentThread().setContextClassLoader(originalClassLoader);
  }
}
