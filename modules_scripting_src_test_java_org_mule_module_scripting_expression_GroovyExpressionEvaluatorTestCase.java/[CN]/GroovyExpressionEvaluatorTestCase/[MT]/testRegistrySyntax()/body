{
  Apple apple=new Apple();
  muleContext.getRegistry().registerObject("name.with.dots",apple);
  Object result=muleContext.getExpressionManager().evaluate("#[groovy:registry.lookupObject('name.with.dots')]",null);
  assertNotNull(result);
  assertSame(apple,result);
  result=muleContext.getExpressionManager().evaluate("#[groovy:registry.'name.with.dots']",null);
  assertNotNull(result);
  assertSame(apple,result);
  result=muleContext.getExpressionManager().evaluate("#[groovy:registry['name.with.dots']]",null);
  assertNotNull(result);
  assertSame(apple,result);
  result=muleContext.getExpressionManager().evaluate("#[groovy:registry.'name.with.dots'.washed]",null);
  assertNotNull(result);
  assertEquals(false,result);
}
