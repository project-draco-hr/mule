{
  InternalMessage message;
  if (response instanceof InternalMessage) {
    message=(InternalMessage)response;
  }
 else {
    message=InternalMessage.builder().payload(response).build();
  }
  return (HttpResponse)getMuleContext().getTransformationService().applyTransformers(message,null,getMessageReceiver().getResponseTransportTransformers()).getPayload().getValue();
}
