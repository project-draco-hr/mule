{
  MuleMessage message;
  if (response instanceof MuleMessage) {
    message=(MuleMessage)response;
  }
 else {
    message=MuleMessage.builder().payload(response).build();
  }
  return (HttpResponse)getMuleContext().getTransformationService().applyTransformers(message,null,getMessageReceiver().getResponseTransportTransformers()).getPayload();
}
