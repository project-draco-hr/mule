{
  boolean oldRequired=required;
  boolean oldRequiredESet=requiredESet;
  required=REQUIRED_EDEFAULT;
  requiredESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,MulePackage.CONTAINER_ENTRY_TYPE__REQUIRED,oldRequired,REQUIRED_EDEFAULT,oldRequiredESet));
}
