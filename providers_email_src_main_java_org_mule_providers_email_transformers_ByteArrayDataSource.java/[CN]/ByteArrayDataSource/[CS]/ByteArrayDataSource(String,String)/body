{
  this.type=type;
  try {
    baos=new ByteArrayOutputStream();
    baos.write(data.getBytes("iso-8859-1"));
    baos.flush();
    baos.close();
  }
 catch (  UnsupportedEncodingException uex) {
  }
catch (  IOException ignored) {
  }
 finally {
    try {
      if (baos != null) {
        baos.close();
      }
    }
 catch (    IOException ignored) {
    }
  }
}
