{
  ByteArrayInputStream Bis=null;
  try {
    Bis=new ByteArrayInputStream(data);
    this.byteArrayDataSource(Bis,type);
  }
 catch (  IOException ioex) {
    throw ioex;
  }
 finally {
    try {
      if (Bis != null) {
        Bis.close();
      }
    }
 catch (    IOException ignored) {
    }
  }
}
