{
  ClasspathContainerInitializer initializer=JavaCore.getClasspathContainerInitializer(MuleCorePlugin.ID_MULE_CLASSPATH_CONTAINER);
  if (initializer != null) {
    try {
      initializer.requestClasspathContainerUpdate(MuleClasspathContainer.PATH,JavaCore.create(getProject()),(IClasspathContainer)null);
    }
 catch (    CoreException e) {
      throw new MuleModelException(e.getStatus());
    }
  }
}
