{
  final int PRIME=31;
  int result=1;
  result=PRIME * result + (bitten ? 1231 : 1237);
  result=PRIME * result + ((brand == null) ? 0 : brand.hashCode());
  result=PRIME * result + ((radius == null) ? 0 : radius.hashCode());
  result=PRIME * result + ((segments == null) ? 0 : segments.hashCode());
  return result;
}
