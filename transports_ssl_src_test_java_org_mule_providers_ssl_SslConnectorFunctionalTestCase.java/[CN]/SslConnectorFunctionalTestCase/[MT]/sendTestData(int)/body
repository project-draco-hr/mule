{
  URI uri=getUri();
  for (int i=0; i < iterations; i++) {
    socket=createSocket(uri);
    DataOutputStream dos=new DataOutputStream(new BufferedOutputStream(socket.getOutputStream()));
    dos.write(TEST_MESSAGE.getBytes());
    dos.flush();
    logger.info("Sent message: " + i);
    dos.close();
  }
}
