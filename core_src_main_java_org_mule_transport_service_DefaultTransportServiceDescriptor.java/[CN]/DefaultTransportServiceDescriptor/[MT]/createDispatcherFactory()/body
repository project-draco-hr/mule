{
  if (dispatcherFactory != null) {
    try {
      return (MessageDispatcherFactory)ClassUtils.instanciateClass(dispatcherFactory,ClassUtils.NO_ARGS);
    }
 catch (    Exception e) {
      throw new TransportServiceException(CoreMessages.failedToCreateObjectWith("Message Dispatcher Factory",dispatcherFactory),e);
    }
  }
 else {
    return null;
  }
}
