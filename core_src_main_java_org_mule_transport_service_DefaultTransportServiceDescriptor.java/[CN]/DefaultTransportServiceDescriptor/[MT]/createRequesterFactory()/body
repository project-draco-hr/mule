{
  if (requesterFactory != null) {
    try {
      return (MessageRequesterFactory)ClassUtils.instanciateClass(requesterFactory,ClassUtils.NO_ARGS);
    }
 catch (    Exception e) {
      throw new TransportServiceException(CoreMessages.failedToCreateObjectWith("Message Requester Factory",requesterFactory),e);
    }
  }
 else {
    return null;
  }
}
