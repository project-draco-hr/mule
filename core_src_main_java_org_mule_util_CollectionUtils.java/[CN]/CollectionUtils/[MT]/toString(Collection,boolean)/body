{
  StringBuffer buf=new StringBuffer(128);
  Object item;
  for (Iterator iterator=c.iterator(); iterator.hasNext(); ) {
    item=iterator.next();
    if (item instanceof Class) {
      buf.append(((Class)item).getName()).append(", ");
    }
 else {
      buf.append(item).append(", ");
    }
    if (newLine)     buf.append("\n");
  }
  return buf.toString();
}
