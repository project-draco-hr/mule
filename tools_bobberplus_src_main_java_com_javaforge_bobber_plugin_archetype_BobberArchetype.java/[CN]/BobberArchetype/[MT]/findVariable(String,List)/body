{
  for (int i=0; i < variables.size(); i++) {
    Variable var=(Variable)variables.get(i);
    if (variableName.equals(var.getName())) {
      return var;
    }
 else     if (var.getVariables() != null) {
      Object o=findVariable(variableName,var.getVariables());
      if (o != null) {
        return o;
      }
    }
  }
  return null;
}
