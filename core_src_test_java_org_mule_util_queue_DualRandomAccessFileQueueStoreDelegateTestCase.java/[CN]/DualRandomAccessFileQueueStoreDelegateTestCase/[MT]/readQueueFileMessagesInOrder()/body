{
  new SystemPropertyExecutionContext().addSystemProperty(DualRandomAccessFileQueueStoreDelegate.MAX_LENGTH_PER_FILE_PROPERTY_KEY,String.valueOf(MAXIMUM_NUMBER_OF_BYTES)).execute(new SystemPropertyExecutionContext.TestMethod(){
    @Override public void execute() throws Exception {
