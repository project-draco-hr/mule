{
  String header="<header xmlns=\"http://consumer.ws.module.runtime.mule.org/\">HEADER_VALUE</header>";
  Event event=flowRunner("noParamsWithHeader").withPayload(TEST_MESSAGE).withOutboundProperty("soap.header",header).run();
  String expectedResponse="<ns2:noParamsWithHeaderResponse xmlns:ns2=\"http://consumer.ws.module.runtime.mule.org/\">" + "<text>HEADER_VALUE</text></ns2:noParamsWithHeaderResponse>";
  assertXMLEqual(expectedResponse,getPayloadAsString(event.getMessage()));
}
