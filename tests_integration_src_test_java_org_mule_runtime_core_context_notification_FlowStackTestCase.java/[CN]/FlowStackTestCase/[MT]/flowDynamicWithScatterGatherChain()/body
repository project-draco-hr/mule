{
  flowRunner("flowDynamicWithScatterGatherChain").withPayload("payload").run();
  assertThat(FlowStackAsserter.stackToAssert,not(nullValue()));
  assertStackElements(FlowStackAsserter.stackToAssert,isFlowStackElement("flow","/flow/processors/0"),isFlowStackElement("flowDynamicWithScatterGatherChain","/flowDynamicWithScatterGatherChain/processors/0/1/0"));
}
