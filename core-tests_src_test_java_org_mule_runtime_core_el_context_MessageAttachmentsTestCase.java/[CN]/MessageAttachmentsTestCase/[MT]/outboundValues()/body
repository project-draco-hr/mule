{
  MuleEvent event=getTestEvent("");
  MuleMessage message=event.getMessage();
  DataHandler valA=mock(DataHandler.class);
  DataHandler valB=mock(DataHandler.class);
  event.setMessage(MuleMessage.builder(message).addOutboundAttachment("foo",valA).addOutboundAttachment("bar",valB).build());
  Collection<DataHandler> values=(Collection<DataHandler>)evaluate("message.outboundAttachments.values()",event);
  assertEquals(2,values.size());
  values.contains(valA);
  values.contains(valB);
}
