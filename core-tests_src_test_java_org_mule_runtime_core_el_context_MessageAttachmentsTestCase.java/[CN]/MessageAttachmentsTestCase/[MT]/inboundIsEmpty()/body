{
  MuleEvent event=getTestEvent("");
  MuleMessage message=event.getMessage();
  assertTrue((Boolean)evaluate("message.inboundAttachments.isEmpty()",event));
  event.setMessage(MuleMessage.builder(message).addInboundAttachment("foo",mock(DataHandler.class)).addInboundAttachment("bar",mock(DataHandler.class)).build());
  assertFalse((Boolean)evaluate("message.inboundAttachments.isEmpty()",event));
}
