{
  MuleEvent event=getTestEvent("");
  MuleMessage message=event.getMessage();
  event.setMessage(MuleMessage.builder(message).addOutboundAttachment("foo",mock(DataHandler.class)).addOutboundAttachment("bar",mock(DataHandler.class)).build());
  evaluate("message.outboundAttachments.clear()",event);
  assertEquals(0,event.getMessage().getOutboundAttachmentNames().size());
}
