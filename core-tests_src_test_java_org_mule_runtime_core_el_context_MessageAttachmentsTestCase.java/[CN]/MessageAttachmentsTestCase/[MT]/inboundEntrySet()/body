{
  MuleEvent event=getTestEvent("");
  MuleMessage message=event.getMessage();
  DataHandler valA=mock(DataHandler.class);
  DataHandler valB=mock(DataHandler.class);
  event.setMessage(MuleMessage.builder(message).addInboundAttachment("foo",valA).addInboundAttachment("bar",valB).build());
  Set<Map.Entry<String,DataHandler>> entrySet=(Set<Entry<String,DataHandler>>)evaluate("message.inboundAttachments.entrySet()",event);
  assertEquals(2,entrySet.size());
  entrySet.contains(new DefaultMapEntry("foo",valA));
  entrySet.contains(new DefaultMapEntry("bar",valB));
}
