{
  List payloads=new ArrayList(messages.size());
  for (  MuleEvent muleEvent : messages) {
    MuleMessage result=muleEvent == null ? null : muleEvent.getMessage();
    if (result != null) {
      Object payload=postProcessMessage(result);
      if (payload != null) {
        payloads.add(payload);
      }
    }
  }
  return payloads;
}
