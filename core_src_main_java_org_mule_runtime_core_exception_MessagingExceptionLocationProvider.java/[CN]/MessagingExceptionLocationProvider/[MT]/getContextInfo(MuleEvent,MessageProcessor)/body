{
  Map<String,Object> contextInfo=new HashMap<>();
  contextInfo.put(INFO_LOCATION_KEY,resolveProcessorRepresentation(muleContext.getConfiguration().getId(),getProcessorPath(event,lastProcessed),lastProcessed));
  if (lastProcessed instanceof AnnotatedObject) {
    String sourceXML=getSourceXML((AnnotatedObject)lastProcessed);
    if (sourceXML != null) {
      contextInfo.put(INFO_SOURCE_XML_KEY,sourceXML);
    }
  }
  return contextInfo;
}
