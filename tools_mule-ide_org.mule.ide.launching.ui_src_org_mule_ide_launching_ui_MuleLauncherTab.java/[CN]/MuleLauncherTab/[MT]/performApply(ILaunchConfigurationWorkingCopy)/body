{
  IProject project=getCurrentProject();
  if (project == null) {
    config.setAttribute(IJavaLaunchConfigurationConstants.ATTR_PROJECT_NAME,(String)null);
  }
 else {
    config.setAttribute(IJavaLaunchConfigurationConstants.ATTR_PROJECT_NAME,project.getName());
  }
  IMuleConfigSet configSet=getCurrentConfigSet();
  if (configSet != null) {
    config.setAttribute(IMuleLaunchConfigurationConstants.ATTR_MULE_CONFIG_SET_ID,configSet.getId());
  }
 else {
    config.setAttribute(IMuleLaunchConfigurationConstants.ATTR_MULE_CONFIG_SET_ID,(String)null);
  }
}
