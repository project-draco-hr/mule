{
  if (configuration == null || configurationModel == null) {
    DeclaredConfiguration<Object> declaredConfiguration=StringUtils.isBlank(configurationProviderName) ? extensionManager.getConfiguration(extensionModel,this) : extensionManager.getConfiguration(extensionModel,configurationProviderName,this);
    configuration=declaredConfiguration.getValue();
    configurationModel=declaredConfiguration.getModel();
  }
}
