{
  MuleContext muleContext=pipeline.getMuleContext();
  Integer timeout=queueTimeout != null ? queueTimeout : muleContext.getConfiguration().getDefaultQueueTimeout();
  return new SedaStageInterceptingMessageProcessor(ThreadNameHelper.flow(pipeline.getMuleContext(),pipeline.getName()),new QueueProfile(maxQueueSize,queueStore),timeout,createThreadingProfile(muleContext),queueStatistics,muleContext);
}
