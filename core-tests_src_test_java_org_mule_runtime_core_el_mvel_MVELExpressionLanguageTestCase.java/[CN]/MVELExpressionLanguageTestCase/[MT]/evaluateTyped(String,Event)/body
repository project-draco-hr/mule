{
  if (variant.equals(Variant.EXPRESSION_WITH_DELIMITER)) {
    return mvel.evaluate("#[" + expression + "]",event,Event.builder(event),flowConstruct,BindingContext.builder().build());
  }
 else {
    return mvel.evaluate(expression,event,Event.builder(event),flowConstruct,BindingContext.builder().build());
  }
}
