{
  if (clientConfig == null) {
    String config=null;
    if (event != null) {
      config=event.getStringProperty(AxisConnector.AXIS_CLIENT_CONFIG_PROPERTY,null);
    }
    if (config != null) {
      clientConfig=new FileProvider(config);
    }
 else {
      clientConfig=connector.getClientProvider();
    }
  }
  return clientConfig;
}
