{
  Bindings bindings=script.getScriptEngine().createBindings();
  Flow flow=new Flow("",muleContext);
  MuleEvent event=MuleEvent.builder(create(flow,"ScriptFilter")).message(message).exchangePattern(ONE_WAY).flow(flow).build();
  script.populateBindings(bindings,event);
  try {
    return (Boolean)script.runScript(bindings);
  }
 catch (  Throwable e) {
    return false;
  }
}
