{
  Bindings bindings=script.getScriptEngine().createBindings();
  script.populateBindings(bindings,event,builder);
  try {
    return (Boolean)script.runScript(bindings);
  }
 catch (  Throwable e) {
    return false;
  }
 finally {
    builder.message((MuleMessage)bindings.get(BINDING_MESSAGE));
  }
}
