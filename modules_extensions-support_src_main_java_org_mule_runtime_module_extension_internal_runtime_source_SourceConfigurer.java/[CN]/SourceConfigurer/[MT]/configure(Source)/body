{
  ParameterGroupAwareObjectBuilder<Source> builder=new ParameterGroupAwareObjectBuilder<Source>(source.getClass(),model,resolverSet){
    @Override protected Source instantiateObject(){
      return source;
    }
  }
;
  try {
    return builder.build(MuleExtensionUtils.getInitialiserEvent(muleContext));
  }
 catch (  Exception e) {
    throw new MuleRuntimeException(createStaticMessage("Exception was found trying to configure source of type " + source.getClass().getName()),e);
  }
}
