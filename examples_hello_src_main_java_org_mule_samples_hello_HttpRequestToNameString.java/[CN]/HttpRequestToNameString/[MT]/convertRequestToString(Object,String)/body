{
  String srcAsString=null;
  if (src instanceof byte[]) {
    if (encoding != null) {
      try {
        srcAsString=new String((byte[])src,encoding);
      }
 catch (      UnsupportedEncodingException ex) {
        srcAsString=new String((byte[])src);
      }
    }
 else {
      srcAsString=new String((byte[])src);
    }
  }
 else {
    srcAsString=src.toString();
  }
  return srcAsString;
}
