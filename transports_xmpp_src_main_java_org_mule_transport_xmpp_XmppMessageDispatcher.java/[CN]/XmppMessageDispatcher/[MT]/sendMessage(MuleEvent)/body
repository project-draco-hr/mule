{
  Object payload=event.getMessage().getPayload();
  Message jabberMessage=event.getMessage().getPayload(Message.class);
  conversation.dispatch(jabberMessage);
  if (logger.isDebugEnabled()) {
    String recipient=XmppConnector.getRecipient(endpoint);
    logger.debug("Message \"" + jabberMessage.getBody() + "\" successfully sent to "+ recipient);
  }
}
