{
  this.dot=jar;
  this.properties=properties;
  this.manifest=jar.getManifest();
  if (manifest == null) {
    manifest=new Manifest();
    error("This file contains no manifest and is therefore not a bundle");
  }
  main=this.manifest.getMainAttributes();
  verifyHeaders(main);
  r3=getHeader(Analyzer.BUNDLE_MANIFESTVERSION) == null;
  usesRequire=getHeader(Analyzer.REQUIRE_BUNDLE) != null;
  fragment=getHeader(Analyzer.FRAGMENT_HOST) != null;
  bundleClasspath=getBundleClassPath();
  mimports=parseHeader(manifest.getMainAttributes().getValue(Analyzer.IMPORT_PACKAGE));
  mdynimports=parseHeader(manifest.getMainAttributes().getValue(Analyzer.DYNAMICIMPORT_PACKAGE));
  mexports=parseHeader(manifest.getMainAttributes().getValue(Analyzer.EXPORT_PACKAGE));
  ignore=parseHeader(manifest.getMainAttributes().getValue(Analyzer.IGNORE_PACKAGE));
}
