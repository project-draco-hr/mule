{
  Set<String> invalidExport=newSet(mexports.keySet());
  invalidExport.removeAll(contained.keySet());
  for (Iterator<String> i=invalidExport.iterator(); i.hasNext(); ) {
    String pack=i.next();
    if (isDuplicate(pack))     i.remove();
  }
  if (!invalidExport.isEmpty())   error("Exporting packages that are not on the Bundle-Classpath" + bundleClasspath + ": "+ invalidExport);
}
