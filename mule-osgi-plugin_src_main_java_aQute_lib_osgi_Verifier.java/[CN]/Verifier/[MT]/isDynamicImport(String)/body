{
  for (  String pattern : mdynimports.keySet()) {
    if (pattern.equals("*"))     return true;
    if (pattern.endsWith(".*")) {
      pattern=pattern.substring(0,pattern.length() - 2);
      if (pack.startsWith(pattern) && (pack.length() == pattern.length() || pack.charAt(pattern.length()) == '.'))       return true;
    }
 else {
      if (pack.equals(pattern))       return true;
    }
  }
  return false;
}
