{
  if (operationModel.getModelProperty(InterceptingModelProperty.class).isPresent()) {
    return new InterceptingOperationMessageProcessor(extensionModel,operationModel,configurationProvider,target,resolverSet,(ExtensionManagerAdapter)muleContext.getExtensionManager());
  }
 else   if (operationModel.getModelProperty(PagedOperationModelProperty.class).isPresent()) {
    return new PagedOperationMessageProcessor(extensionModel,operationModel,configurationProvider,target,resolverSet,(ExtensionManagerAdapter)muleContext.getExtensionManager());
  }
 else {
    return new OperationMessageProcessor(extensionModel,operationModel,configurationProvider,target,resolverSet,(ExtensionManagerAdapter)muleContext.getExtensionManager());
  }
}
