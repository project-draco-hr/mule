{
  if (operationModel.getModelProperty(InterceptingModelProperty.class).isPresent()) {
    return new InterceptingOperationMessageProcessor(extensionModel,operationModel,configurationProviderName,target,resolverSet,(ExtensionManagerAdapter)muleContext.getExtensionManager());
  }
 else {
    return new OperationMessageProcessor(extensionModel,operationModel,configurationProviderName,target,resolverSet,(ExtensionManagerAdapter)muleContext.getExtensionManager());
  }
}
