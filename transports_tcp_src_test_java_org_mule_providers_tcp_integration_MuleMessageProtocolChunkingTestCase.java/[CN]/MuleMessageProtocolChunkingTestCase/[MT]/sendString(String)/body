{
  MuleClient client=new MuleClient();
  for (int i=0; i < messages; i++) {
    client.dispatch("vm://in",new MuleMessage(message));
  }
  for (int i=0; i < messages; i++) {
    UMOMessage msg=client.request("vm://out",WAIT_MS);
    assertEquals(message,new String((byte[])msg.getPayload()));
  }
}
