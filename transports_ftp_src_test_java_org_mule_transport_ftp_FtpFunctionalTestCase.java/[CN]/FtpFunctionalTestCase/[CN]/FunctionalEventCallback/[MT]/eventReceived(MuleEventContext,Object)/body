{
  try {
    FunctionalTestComponent ftc=(FunctionalTestComponent)component;
    if (1 == latch.getCount()) {
      String o=new String((byte[])ftc.getLastReceivedMessage());
      message.set(o);
      latch.countDown();
    }
  }
 catch (  Exception e) {
    throw new IllegalStateException(e);
  }
}
