{
  if (notifier != null) {
    notifier.notify(SFTP_DELETE_ACTION,currentDirectory + "/" + fileName);
  }
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Will try to delete " + fileName);
    }
    channelSftp.rm(fileName);
  }
 catch (  SftpException e) {
    throw new IOException(e);
  }
}
