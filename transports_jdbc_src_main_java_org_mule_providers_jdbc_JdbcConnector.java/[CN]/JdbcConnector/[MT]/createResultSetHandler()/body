{
  try {
    if (resultSetHandler != null) {
      return (ResultSetHandler)resultSetHandler.getOrCreate();
    }
 else {
      return (ResultSetHandler)ClassUtils.instanciateClass(DEFAULT_RESULTSET_HANDLER,ClassUtils.NO_ARGS);
    }
  }
 catch (  Exception e) {
    throw new IllegalArgumentException("Error creating instance of the resultSetHandler class :" + getResultSetHandler() + System.getProperty("line.separator")+ ExceptionUtils.getFullStackTrace(e));
  }
}
