{
  Object value=null;
  for (Iterator iterator=scopedMap.values().iterator(); iterator.hasNext(); ) {
    Map props=(Map)iterator.next();
    value=props.get(key);
    if (value != null) {
      break;
    }
  }
  if (value == null && fallbackToRegistry) {
    value=applicationProperties.get(key);
  }
  return value;
}
