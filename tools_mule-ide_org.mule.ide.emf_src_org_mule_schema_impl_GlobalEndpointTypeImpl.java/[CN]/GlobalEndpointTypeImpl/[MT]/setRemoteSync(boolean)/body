{
  boolean oldRemoteSync=remoteSync;
  remoteSync=newRemoteSync;
  boolean oldRemoteSyncESet=remoteSyncESet;
  remoteSyncESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.GLOBAL_ENDPOINT_TYPE__REMOTE_SYNC,oldRemoteSync,remoteSync,!oldRemoteSyncESet));
}
