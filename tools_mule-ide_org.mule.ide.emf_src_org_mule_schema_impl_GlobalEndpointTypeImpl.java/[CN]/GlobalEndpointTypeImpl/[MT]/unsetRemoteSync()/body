{
  boolean oldRemoteSync=remoteSync;
  boolean oldRemoteSyncESet=remoteSyncESet;
  remoteSync=REMOTE_SYNC_EDEFAULT;
  remoteSyncESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,SchemaPackage.GLOBAL_ENDPOINT_TYPE__REMOTE_SYNC,oldRemoteSync,REMOTE_SYNC_EDEFAULT,oldRemoteSyncESet));
}
