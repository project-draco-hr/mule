{
  boolean oldSynchronous=synchronous;
  boolean oldSynchronousESet=synchronousESet;
  synchronous=SYNCHRONOUS_EDEFAULT;
  synchronousESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,MulePackage.GLOBAL_ENDPOINT_TYPE__SYNCHRONOUS,oldSynchronous,SYNCHRONOUS_EDEFAULT,oldSynchronousESet));
}
