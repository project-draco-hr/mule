{
  try {
    FileOutputStream out=new FileOutputStream(outputFile);
    out.write(muleContext.getObjectSerializer().getInternalProtocol().serialize(value));
    out.flush();
  }
 catch (  SerializationException se) {
    throw new ObjectStoreException(se);
  }
catch (  FileNotFoundException fnfe) {
    throw new ObjectStoreException(fnfe);
  }
catch (  IOException e) {
    throw new MuleRuntimeException(I18nMessageFactory.createStaticMessage("Could not write to file"),e);
  }
}
