{
  roundRobinRoutingStrategy=new RoundRobinRoutingStrategy(muleContext,new IdentifiableDynamicRouteResolver(){
    @Override public String getRouteIdentifier(    MuleEvent event) throws MessagingException {
      return event.getMessage().getInvocationProperty(ID_PROPERTY_NAME);
    }
    @Override public List<MessageProcessor> resolveRoutes(    MuleEvent event) throws MessagingException {
      return null;
    }
  }
);
}
