{
  if (variant.equals(ConfigVariant.FLOW)) {
    Flow flow=muleContext.getRegistry().lookupObject("component1");
    MuleEvent event=new DefaultMuleEvent(new DefaultMuleMessage("Test MuleEvent",muleContext),((InboundEndpoint)flow.getMessageSource()),flow);
    flow.process(event);
  }
 else {
    Service service=muleContext.getRegistry().lookupService("component1");
    MuleEvent event=new DefaultMuleEvent(new DefaultMuleMessage("Test MuleEvent",muleContext),((ServiceCompositeMessageSource)service.getMessageSource()).getEndpoints().get(0),service);
    service.sendEvent(event);
  }
}
