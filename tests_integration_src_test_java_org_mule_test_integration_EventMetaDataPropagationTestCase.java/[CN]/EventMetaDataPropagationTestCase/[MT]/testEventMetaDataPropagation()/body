{
  Service service=muleContext.getRegistry().lookupService("component1");
  MuleSession session=new DefaultMuleSession(service,muleContext);
  MuleEvent event=new DefaultMuleEvent(new DefaultMuleMessage("Test MuleEvent",muleContext),((ServiceCompositeMessageSource)service.getMessageSource()).getEndpoints().get(0),session);
  service.sendEvent(event);
}
