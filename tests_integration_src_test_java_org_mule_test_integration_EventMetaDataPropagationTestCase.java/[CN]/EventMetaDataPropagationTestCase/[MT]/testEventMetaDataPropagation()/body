{
  Service service=muleContext.getRegistry().lookupService("component1");
  MuleSession session=new DefaultMuleSession(service,muleContext);
  MuleEvent event=new DefaultMuleEvent(new DefaultMuleMessage("Test MuleEvent",muleContext),(ImmutableEndpoint)service.getInboundRouter().getEndpoints().get(0),session,true);
  session.sendEvent(event);
}
