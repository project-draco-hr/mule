{
  UMOComponent component=managementContext.getRegistry().lookupModel("main").getComponent("component1");
  UMOSession session=new MuleSession(component);
  UMOEvent event=new MuleEvent(new MuleMessage("Test Event"),(UMOImmutableEndpoint)component.getDescriptor().getInboundRouter().getEndpoints().get(0),session,true);
  session.sendEvent(event);
}
