{
  File dir=Utility.openDirectory("src");
  assertNotNull(dir);
  assertTrue(dir.exists());
  assertTrue(dir.canRead());
  assertTrue(dir.isDirectory());
  dir=Utility.openDirectory("doesNotExist");
  assertNotNull(dir);
  assertTrue(dir.exists());
  assertTrue(dir.canRead());
  assertTrue(dir.isDirectory());
  dir.delete();
}
