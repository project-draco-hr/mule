{
  final Properties properties=new Properties();
  properties.put("1",String.format("java.lang.String,%s=%s",SimpleRegistryBootstrap.APPLY_TO_ARTIFACT_TYPE_PARAMETER_KEY,SimpleRegistryBootstrap.APP_ARTIFACT_TYPE_VALUE));
  properties.put("2",String.format("java.util.Properties,%s=%s",SimpleRegistryBootstrap.APPLY_TO_ARTIFACT_TYPE_PARAMETER_KEY,SimpleRegistryBootstrap.DOMAIN_ARTIFACT_TYPE_VALUE));
  properties.put("3",String.format("java.util.ArrayList,%s=%s/%s",SimpleRegistryBootstrap.APPLY_TO_ARTIFACT_TYPE_PARAMETER_KEY,SimpleRegistryBootstrap.DOMAIN_ARTIFACT_TYPE_VALUE,SimpleRegistryBootstrap.APP_ARTIFACT_TYPE_VALUE));
  properties.put("jms.singletx.transaction.resource1",String.format("%s,optional)",TEST_TRANSACTION_FACTORY_CLASS));
  properties.put("test.singletx.transaction.factory1",FakeTransactionFactory.class.getName());
  properties.put("test.singletx.transaction.resource1",FakeTransactionResource.class.getName());
  SimpleRegistryBootstrap simpleRegistryBootstrap=new SimpleRegistryBootstrap(){
    @Override protected List<Properties> loadBootstrapProperties() throws InitialisationException {
      return Arrays.asList(properties);
    }
  }
;
  simpleRegistryBootstrap.setSupportedArtifactType(artifactType);
  simpleRegistryBootstrap.setMuleContext(muleContext);
  simpleRegistryBootstrap.initialise();
  return simpleRegistryBootstrap;
}
