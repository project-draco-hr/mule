{
  if (isEmpty()) {
    return null;
  }
  Lock lock=filesLock.readLock();
  lock.lock();
  byte[] bytes;
  try {
    if (readFile.isEmpty()) {
      readFile.clear();
      switchReadFile();
    }
    bytes=readFile.getFirst();
  }
  finally {
    lock.unlock();
  }
  return deserialize(bytes);
}
