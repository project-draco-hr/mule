{
  Connection con=null;
  try {
    try {
      con=this.connector.getConnection();
    }
 catch (    SQLException e) {
      throw new ConnectException(e,this);
    }
    Object[] readParams=connector.getParams(endpoint,this.readParams,null,this.endpoint.getEndpointURI().getAddress());
    Object results=connector.createQueryRunner().query(con,this.readStmt,readParams,connector.createResultSetHandler());
    return (List)results;
  }
  finally {
    JdbcUtils.close(con);
  }
}
