{
  Map props=CollectionUtil.mapWithKeysAndValues(HashMap.class,new Object[]{"foo"},new Object[]{"bar"});
  assertEquals("{foo=bar}",PropertiesHelper.propertiesToString(props,false));
  assertEquals("{" + SystemUtils.LINE_SEPARATOR + "foo=bar"+ SystemUtils.LINE_SEPARATOR+ "}",PropertiesHelper.propertiesToString(props,true));
}
