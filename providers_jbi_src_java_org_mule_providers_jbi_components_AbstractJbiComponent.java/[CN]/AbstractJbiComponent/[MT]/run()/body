{
  while (started.get()) {
    try {
      final MessageExchange me=deliveryChannel.accept();
      if (me != null) {
        getWorkManager().scheduleWork(new MessageExchangeWorker(me,(MessageExchangeListener)this));
      }
    }
 catch (    Exception e) {
      logger.error(e.getMessage(),e);
    }
  }
}
