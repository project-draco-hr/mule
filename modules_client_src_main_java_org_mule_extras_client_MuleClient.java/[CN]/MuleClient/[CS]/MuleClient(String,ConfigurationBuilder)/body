{
  if (MuleManager.isInstanciated()) {
    throw new ConfigurationException(new Message(Messages.MANAGER_IS_ALREADY_CONFIGURED));
  }
  if (builder == null) {
    logger.info("Builder passed in was null, using default builder: " + SpringConfigurationBuilder.class.getName());
    builder=new SpringConfigurationBuilder();
  }
  manager=builder.configure(configResources,null);
  registry=MuleManager.getRegistry();
}
