{
  UMOEvent event=getEvent(message,url,true,false);
  event.setTimeout(timeout);
  try {
    UMOMessage msg=event.getSession().sendEvent(event);
    if (msg == null) {
      msg=new MuleMessage(NullPayload.getInstance());
    }
    return msg;
  }
 catch (  UMOException e) {
    throw e;
  }
catch (  Exception e) {
    throw new DispatchException(ClientMessages.failedToDispatchClientEvent(),event.getMessage(),event.getEndpoint(),e);
  }
}
