{
  try {
    if (localContext != null) {
      return ((QueueTransactionContext)localContext).poll(queue,timeout);
    }
 else {
      Object id=queue.poll(timeout);
      if (id != null) {
        Object item=doLoad(queue,id);
        doRemove(queue,id);
        return item;
      }
      return null;
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
