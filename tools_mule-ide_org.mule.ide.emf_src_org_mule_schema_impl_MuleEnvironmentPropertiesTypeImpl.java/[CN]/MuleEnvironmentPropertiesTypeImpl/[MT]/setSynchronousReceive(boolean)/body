{
  boolean oldSynchronousReceive=synchronousReceive;
  synchronousReceive=newSynchronousReceive;
  boolean oldSynchronousReceiveESet=synchronousReceiveESet;
  synchronousReceiveESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.MULE_ENVIRONMENT_PROPERTIES_TYPE__SYNCHRONOUS_RECEIVE,oldSynchronousReceive,synchronousReceive,!oldSynchronousReceiveESet));
}
