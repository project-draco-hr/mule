{
  SimpleFlowConstruct flow=muleContext.getRegistry().lookupObject("pojoFlow");
  SimpleFlowConstruct flow2=muleContext.getRegistry().lookupObject("pojoFlow2");
  assertEquals("start method2Arg1config1Val arg2Valmethod2Arg1Arg2config2Val ",flow.process(getTestEvent("start ")).getMessageAsString());
  assertEquals("start method2Arg1config1Val arg2Valmethod2Arg1Arg2null ",flow2.process(getTestEvent("start ")).getMessageAsString());
}
