{
  Object cookie=null;
  try {
    cookie=cookieProtocol.read(is);
  }
 catch (  Exception e) {
    helpUser(e);
  }
  if (null != cookie) {
    if (!(cookie instanceof byte[] && ((byte[])cookie).length == COOKIE.length() && COOKIE.equals(new String((byte[])cookie)))) {
      helpUser();
    }
 else {
      return true;
    }
  }
  return false;
}
