{
  String correlationId="id";
  int correlationGroupSize=3;
  flowRunner("aggregator").withPayload(TEST_MESSAGE).withSourceCorrelationId(correlationId).withCorrelation(new GroupCorrelation(correlationGroupSize,1)).nonBlocking().runNoVerify();
  flowRunner("aggregator").withPayload(TEST_MESSAGE).withSourceCorrelationId(correlationId).withCorrelation(new GroupCorrelation(correlationGroupSize,2)).nonBlocking().runNoVerify();
  flowRunner("aggregator").withPayload(TEST_MESSAGE).withSourceCorrelationId(correlationId).withCorrelation(new GroupCorrelation(correlationGroupSize,3)).nonBlocking().run();
}
