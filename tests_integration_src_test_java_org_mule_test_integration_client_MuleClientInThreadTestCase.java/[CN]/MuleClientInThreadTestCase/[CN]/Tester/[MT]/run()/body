{
  try {
    MuleClient client=new MuleClient(muleContext);
    for (int i=0; i < numMessages; ++i) {
      client.dispatch("vm://in","test",null);
    }
    MuleMessage msg;
    for (int i=0; i < numMessages; ++i) {
      msg=client.request("vm://out",5000);
      assertNotNull(msg);
    }
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
}
