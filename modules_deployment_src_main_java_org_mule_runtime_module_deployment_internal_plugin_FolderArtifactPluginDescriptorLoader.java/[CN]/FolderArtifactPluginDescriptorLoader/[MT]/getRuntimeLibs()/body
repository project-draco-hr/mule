{
  List<URL> urls=new ArrayList<>();
  final File libDir=new File(pluginLocation,LIB);
  if (libDir.exists()) {
    final File[] jars=libDir.listFiles((FilenameFilter)new SuffixFileFilter(JAR_EXTENSION));
    for (    File jar : jars) {
      urls.add(jar.toURI().toURL());
    }
  }
  return urls;
}
