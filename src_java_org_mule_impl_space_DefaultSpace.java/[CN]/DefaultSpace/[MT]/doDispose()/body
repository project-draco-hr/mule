{
  for (Iterator iterator=children.iterator(); iterator.hasNext(); ) {
    DefaultSpace childSpace=(DefaultSpace)iterator.next();
    childSpace.dispose();
  }
  children.clear();
  if (localQueueManager) {
    try {
      queueManager.stop();
    }
 catch (    ResourceManagerSystemException e) {
      logger.warn(e);
    }
  }
}
