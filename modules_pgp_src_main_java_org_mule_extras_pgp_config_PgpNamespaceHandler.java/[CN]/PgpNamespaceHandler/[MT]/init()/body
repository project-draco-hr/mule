{
  registerBeanDefinitionParser("pgp-security-provider",new ChildDefinitionParser("providers",PGPSecurityProvider.class));
  registerBeanDefinitionParser("pgp-security-filter",new ChildDefinitionParser("securityFilter",PGPSecurityFilter.class));
  registerBeanDefinitionParser("keybased-encryption-strategy",new ChildDefinitionParser("encryptionStrategies",KeyBasedEncryptionStrategy.class));
}
