{
  logger.debug("polling....");
  try {
    Object result=invokedMethod.invoke(remoteObject,connector.getEjbAble().arguments());
    if (null != result)     routeMessage(new MuleMessage(connector.getMessageAdapter(result).getPayload(),null),endpoint.isSynchronous());
  }
 catch (  Exception e) {
    handleException(e);
  }
}
