{
  if (message == null) {
    return false;
  }
  final UMOExceptionPayload exceptionPayload=message.getExceptionPayload();
  if (exceptionPayload != null) {
    logger.info("Failure returned, will try next endpoint. Exception payload is: " + exceptionPayload);
    return true;
  }
 else {
    return false;
  }
}
