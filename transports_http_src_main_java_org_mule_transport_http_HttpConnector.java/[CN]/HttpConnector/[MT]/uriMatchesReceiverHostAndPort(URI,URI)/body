{
  return receiverUri.getPort() == uri.getPort() && StringUtils.equalsIgnoreCase(receiverUri.getScheme(),uri.getScheme()) && (HttpConnector.BIND_TO_ALL_INTERFACES_IP.equals(receiverUri.getHost()) || HttpConnector.BIND_TO_ALL_INTERFACES_IP.equals(uri.getHost()) || StringUtils.equalsIgnoreCase(receiverUri.getHost(),uri.getHost()));
}
