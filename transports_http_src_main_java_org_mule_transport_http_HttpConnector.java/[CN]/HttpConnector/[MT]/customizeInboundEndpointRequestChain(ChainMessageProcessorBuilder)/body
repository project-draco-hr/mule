{
  builder.replaceMessageProcessor(InboundFilterMessageProcessor.class,new InboundFilterMessageProcessor(){
    @Override protected MuleMessage handleUnacceptedFilter(    MuleMessage message,    InboundEndpoint endpoint){
      if (logger.isDebugEnabled()) {
        logger.debug("Message request '" + message.getProperty(HttpConnector.HTTP_REQUEST_PROPERTY) + "' is being rejected since it does not match the filter on this endpoint: "+ endpoint);
      }
      message.setProperty(HttpConnector.HTTP_STATUS_PROPERTY,String.valueOf(HttpConstants.SC_NOT_ACCEPTABLE));
      return message;
    }
  }
);
}
