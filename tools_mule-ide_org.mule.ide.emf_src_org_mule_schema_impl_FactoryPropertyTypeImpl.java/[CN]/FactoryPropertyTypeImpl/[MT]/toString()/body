{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", factory: ");
  result.append(factory);
  result.append(", name: ");
  result.append(name);
  result.append(')');
  return result.toString();
}
