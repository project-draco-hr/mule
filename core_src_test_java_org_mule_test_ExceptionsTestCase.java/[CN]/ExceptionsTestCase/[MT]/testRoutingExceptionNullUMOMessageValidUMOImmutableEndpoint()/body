{
  UMOImmutableEndpoint endpoint=managementContext.getRegistry().getOrCreateEndpointForUri("test://outbound",UMOImmutableEndpoint.ENDPOINT_TYPE_SENDER);
  assertNotNull(endpoint);
  RoutingException rex=new RoutingException(null,endpoint);
  assertNotNull(rex);
  assertSame(endpoint,rex.getEndpoint());
}
