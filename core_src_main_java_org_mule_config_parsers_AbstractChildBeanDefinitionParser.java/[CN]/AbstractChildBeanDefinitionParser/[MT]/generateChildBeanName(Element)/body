{
  String parentId=((Element)e.getParentNode()).getAttribute("id");
  String id=e.getAttribute("id");
  if (StringUtils.isBlank(id)) {
    String idref=e.getAttribute("idref");
    if (StringUtils.isBlank(idref)) {
      id=e.getLocalName();
    }
 else {
      id="ref:" + idref;
    }
    if (!parentId.startsWith(".")) {
      parentId="." + parentId;
    }
    return parentId + ":" + id;
  }
 else {
    return id;
  }
}
