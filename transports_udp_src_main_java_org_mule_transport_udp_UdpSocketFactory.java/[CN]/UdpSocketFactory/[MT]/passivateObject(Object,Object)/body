{
  ImmutableEndpoint ep=(ImmutableEndpoint)key;
  boolean keepSocketOpen=MapUtils.getBooleanValue(ep.getProperties(),UdpConnector.KEEP_SEND_SOCKET_OPEN_PROPERTY,((UdpConnector)ep.getConnector()).isKeepSendSocketOpen());
  DatagramSocket socket=(DatagramSocket)object;
  if (!keepSocketOpen) {
    if (socket != null) {
      socket.close();
    }
  }
}
