{
  MuleEvent result=flowRunner("SubFlowViaFlowRef").withPayload("").run();
  assertEquals("1xyz2",result.getMessageAsString());
  assertEquals("[setMuleContext, setService, setMuleContext, initialise, start]",result.getMessage().getOutboundProperty(LifecycleTrackerProcessor.LIFECYCLE_TRACKER_PROCESSOR_PROPERTY));
  assertEquals(muleContext.getRegistry().lookupFlowConstruct("SubFlowViaFlowRef"),result.getFlowVariable(LifecycleTrackerProcessor.FLOW_CONSRUCT_PROPERTY));
}
