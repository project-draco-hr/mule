{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", className: ");
  result.append(className);
  result.append(", name: ");
  result.append(name);
  result.append(", ref: ");
  result.append(ref);
  result.append(", type: ");
  if (typeESet)   result.append(type);
 else   result.append("<unset>");
  result.append(')');
  return result.toString();
}
