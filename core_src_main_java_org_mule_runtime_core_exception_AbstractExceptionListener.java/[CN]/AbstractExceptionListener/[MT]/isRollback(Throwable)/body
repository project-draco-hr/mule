{
  t=ExceptionHelper.getRootException(t);
  if (rollbackTxFilter == null && commitTxFilter == null) {
    return true;
  }
 else {
    return (rollbackTxFilter != null && rollbackTxFilter.accept(t.getClass().getName())) || (commitTxFilter != null && !commitTxFilter.accept(t.getClass().getName()));
  }
}
