{
  boolean isRenamed=false;
  if (srcFile != null && destFile != null) {
    if (!destFile.exists()) {
      if (srcFile.isFile()) {
        isRenamed=srcFile.renameTo(destFile);
        if (!isRenamed && srcFile.exists()) {
          isRenamed=renameFileHard(srcFile,destFile);
        }
      }
    }
  }
  return isRenamed;
}
