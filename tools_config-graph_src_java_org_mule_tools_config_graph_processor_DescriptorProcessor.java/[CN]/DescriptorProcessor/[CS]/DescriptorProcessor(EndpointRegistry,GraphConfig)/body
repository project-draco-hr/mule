{
  super(config);
  this.shortestNotationHandler=new ShortestNotationHandler(endpointRegistry);
  this.inboundRoutersProcessor=new InboundRoutersProcessor(endpointRegistry,config);
  this.responseRouterProcessor=new ResponseRouterProcessor(endpointRegistry);
  exceptionStrategyProcessor=new ExceptionStrategyProcessor(endpointRegistry,config);
  OutBoundRouterEndpointsHandler outBoundRouterEndpointsHandler=new OutBoundRouterEndpointsHandler(endpointRegistry,config);
  this.outBoundRoutersProcessor=new OutBoundRoutersProcessor(config,exceptionStrategyProcessor,outBoundRouterEndpointsHandler,endpointRegistry);
}
