{
  definitionBuilder.withIdentifier(sourceDsl.getElementName()).withTypeDefinition(fromType(ExtensionMessageSource.class)).withObjectFactoryType(ExtensionSourceObjectFactory.class).withConstructorParameterDefinition(fromFixedValue(extensionModel).build()).withConstructorParameterDefinition(fromFixedValue(sourceModel).build()).withConstructorParameterDefinition(fromFixedValue(muleContext).build()).withSetterParameterDefinition("retryPolicyTemplate",fromChildConfiguration(RetryPolicyTemplate.class).build()).withSetterParameterDefinition("configurationProviderName",fromSimpleParameter(CONFIG_ATTRIBUTE).build());
  parseParameters(sourceModel.getParameterModels());
}
