{
  final MuleRegistry registry=muleContext.getRegistry();
  Object object=registry.get("cxfConnector");
  assertTrue(object instanceof CxfConnector);
  CxfConnector connector=(CxfConnector)object;
  assertTrue(connector.getDispatcherFactory() instanceof TestCxfMessageDispatcherFactory);
  return (TestCxfMessageDispatcherFactory)connector.getDispatcherFactory();
}
