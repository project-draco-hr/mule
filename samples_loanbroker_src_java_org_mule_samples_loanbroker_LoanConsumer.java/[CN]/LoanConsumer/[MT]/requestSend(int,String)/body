{
  List results=new ArrayList(number);
  UMOMessage result;
  int batchSize=5;
  for (int batch=0; batch < (number / batchSize); batch++) {
    for (int i=0; i < batchSize; i++) {
      result=client.send(endpoint,createRequest(),null);
      if (result != null) {
        results.add(result.getPayload());
      }
    }
    Thread.sleep(50);
  }
  return results;
}
