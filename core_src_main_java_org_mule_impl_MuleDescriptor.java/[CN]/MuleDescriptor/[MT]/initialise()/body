{
  super.initialise();
  if (StringUtils.isNotEmpty(modelName)) {
    UMOModel model=managementContext.getRegistry().lookupModel(modelName);
    if (model != null) {
      try {
        model.registerComponent(this);
      }
 catch (      UMOException e) {
        throw new InitialisationException(e,this);
      }
    }
  }
}
