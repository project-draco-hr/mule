{
  properties=props;
  String delegate=(String)properties.get(MULE_PROPERTY_DOT_PROPERTIES);
  if (delegate != null) {
    try {
      FileInputStream is=new FileInputStream(FileUtils.newFile(delegate));
      Properties dProps=new Properties();
      dProps.load(is);
      properties.putAll(dProps);
    }
 catch (    Exception e) {
      logger.warn(MULE_PROPERTY_DOT_PROPERTIES + " was set  to " + delegate+ " but the file could not be read, exception is: "+ e.getMessage());
    }
  }
}
