{
  assertEquals(1,qr.update(jdbcConnector.getConnection(),"INSERT INTO TEST(TYPE, DATA, ACK, RESULT) VALUES (3, '" + TEST_MESSAGE + "', NULL, NULL)"));
  prober.check(new Probe(){
    @Override public boolean isSatisfied(){
      try {
        Object[] queryResult=(Object[])qr.query(jdbcConnector.getConnection(),"SELECT DATA FROM TEST WHERE TYPE = 3 AND ACK IS NULL",new ArrayHandler());
        return (queryResult == null);
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
    @Override public String describeFailure(){
      return "Row should be acknowledged (marked read)";
    }
  }
);
}
