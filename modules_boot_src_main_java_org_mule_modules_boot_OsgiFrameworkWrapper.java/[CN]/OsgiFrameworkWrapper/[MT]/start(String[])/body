{
  Properties SYSTEM_PROPS=new Properties();
  SYSTEM_PROPS.put("org.osgi.framework.dir","../conf/knopflerfish");
  for (Iterator iter=SYSTEM_PROPS.entrySet().iterator(); iter.hasNext(); ) {
    Map.Entry entry=(Map.Entry)iter.next();
    System.setProperty((String)entry.getKey(),(String)entry.getValue());
  }
  try {
    org.knopflerfish.framework.Main.main(args);
    if (startGui) {
      org.knopflerfish.framework.Main.main(new String[]{"-istart",DESKTOP_BUNDLE});
    }
    return null;
  }
 catch (  Exception e) {
    e.printStackTrace();
    return new Integer(1);
  }
}
