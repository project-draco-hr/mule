{
  ProcessMessageDispatcher dispatcher=(ProcessMessageDispatcher)lookupDispatcher(toUrl(processName,processId));
  if (dispatcher == null) {
    dispatcher=(ProcessMessageDispatcher)lookupDispatcher(toUrl(processName,null));
  }
  if (dispatcher == null) {
    dispatcher=(ProcessMessageDispatcher)lookupDispatcher(toUrl(null,null));
  }
  return dispatcher;
}
