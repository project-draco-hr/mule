{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", doThreading: ");
  if (doThreadingESet)   result.append(doThreading);
 else   result.append("<unset>");
  result.append(", id: ");
  if (idESet)   result.append(id);
 else   result.append("<unset>");
  result.append(", maxBufferSize: ");
  result.append(maxBufferSize);
  result.append(", maxThreadsActive: ");
  result.append(maxThreadsActive);
  result.append(", maxThreadsIdle: ");
  result.append(maxThreadsIdle);
  result.append(", poolExhaustedAction: ");
  if (poolExhaustedActionESet)   result.append(poolExhaustedAction);
 else   result.append("<unset>");
  result.append(", threadTTL: ");
  result.append(threadTTL);
  result.append(')');
  return result.toString();
}
