{
  try {
    muleContext.getInjector().inject(delegate);
  }
 catch (  Exception e) {
    throw new InitialisationException(createStaticMessage("Could not inject executor of type " + delegate.getClass().getName()),e,this);
  }
  injectInterceptors();
  initialiseIfNeeded(delegate,muleContext);
  super.initialise();
}
