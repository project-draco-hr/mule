{
  flowRunner(CLIENT_COOKIES_ENABLED_FLOW).withPayload(TEST_MESSAGE).withFlowVariable("path","/path").run();
  assertNoCookiesSent();
  flowRunner(CLIENT_COOKIES_ENABLED_FLOW).withPayload(TEST_MESSAGE).withFlowVariable("path","/path").run();
  assertCookiesSent(COOKIE_ROOT_PATH_LOCAL_DOMAIN,COOKIE_CUSTOM_PATH_LOCAL_DOMAIN);
}
