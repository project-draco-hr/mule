{
  Set<String> repeatedParameters=collectRepeatedNames(model.getParameterModels());
  if (!repeatedParameters.isEmpty()) {
    throw new IllegalModelDefinitionException(format("Extension '%s' defines the %s '%s' which has parameters " + "with repeated names. Offending parameters are: [%s]",extensionModel.getName(),model.getClass().getSimpleName(),model.getName(),Joiner.on(",").join(repeatedParameters)));
  }
}
