{
  MuleClient client=new MuleClient(muleContext);
  client.dispatch("in",getMatchMessage(),null);
  MuleMessage message=client.request("vm://match1?connector=queue",RECEIVE_TIMEOUT);
  assertNotNull(message);
  assertFalse(message.getPayload() instanceof NullPayload);
  if (!matchSingle) {
    message=client.request("vm://match2?connector=queue",RECEIVE_TIMEOUT);
    assertNotNull(message);
    assertFalse(message.getPayload() instanceof NullPayload);
  }
}
