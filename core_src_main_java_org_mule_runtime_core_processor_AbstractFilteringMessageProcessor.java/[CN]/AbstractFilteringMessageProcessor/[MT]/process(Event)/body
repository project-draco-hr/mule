{
  boolean accepted;
  Builder builder=Event.builder(event);
  try {
    accepted=accept(event,builder);
  }
 catch (  Exception ex) {
    throw filterFailureException(builder.build(),ex);
  }
  if (accepted) {
    return processNext(builder.build());
  }
 else {
    return handleUnaccepted(builder.build());
  }
}
