{
  int noOfMessages=1;
  boolean queue=false;
  if (args.length == 5) {
    noOfMessages=Integer.parseInt(args[4]);
    if (args[3].equals("-q"))     queue=true;
    String data=args[0];
    if (data.endsWith(".xml")) {
      try {
        data=Utility.fileToString(data);
      }
 catch (      IOException e) {
        e.printStackTrace();
        System.exit(0);
      }
    }
    JmsWriter Jms=new JmsWriter(args[1],queue);
    Jms.write(data,args[2],noOfMessages);
    Jms.close();
  }
 else {
    usage();
  }
}
