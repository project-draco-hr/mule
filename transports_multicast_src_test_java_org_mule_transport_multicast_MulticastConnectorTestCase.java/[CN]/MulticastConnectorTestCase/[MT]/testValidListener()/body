{
  Flow flow=new Flow("orange",muleContext);
  Connector connector=getConnector();
  InboundEndpoint endpoint2=muleContext.getEndpointFactory().getInboundEndpoint("multicast://228.2.3.4:10100");
  connector.registerListener(endpoint2,getSensingNullMessageProcessor(),flow);
  try {
    connector.registerListener(endpoint2,getSensingNullMessageProcessor(),flow);
    fail("cannot register on the same endpointUri");
  }
 catch (  Exception e) {
  }
}
