{
  if (properties == null || properties.length == 0) {
    return null;
  }
  Properties props=new Properties();
  for (  String property : properties) {
    StringTokenizer st=new StringTokenizer(property,"=");
    if (st.hasMoreTokens()) {
      props.setProperty(st.nextToken().trim(),st.nextToken().trim());
    }
  }
  return props;
}
