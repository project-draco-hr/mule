{
  String basePath=getBasePath();
  if (basePath != null) {
    try {
      client.changeWorkingDirectory(Paths.get(basePath).toString());
    }
 catch (    IOException e) {
      throw new MuleRuntimeException(createStaticMessage(format("Failed to perform CWD to the base directory '%s'",basePath)),e);
    }
  }
}
