{
  try {
    Object proxy=Proxy.newProxyInstance(getInterface().getClassLoader(),new Class[]{getInterface()},new NestedInvocationHandler(this));
    logger.debug("Have proxy?: " + (null != proxy));
    return proxy;
  }
 catch (  Exception e) {
    throw new MuleRuntimeException(CoreMessages.failedToCreateProxyFor(target),e);
  }
}
