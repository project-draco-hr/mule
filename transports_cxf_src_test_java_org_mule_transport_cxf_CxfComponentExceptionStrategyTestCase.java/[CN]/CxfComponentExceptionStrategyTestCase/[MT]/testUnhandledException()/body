{
  MuleClient client=new MuleClient();
  try {
    client.send("cxf:http://localhost:63181/services/CxfWithExceptionStrategy?method=testNonXFireException","TEST",null);
  }
 catch (  DispatchException ex) {
    assertTrue(ex.getCause() instanceof FatalConnectException);
    assertTrue(ex.getCause().getCause() instanceof Fault);
  }
}
