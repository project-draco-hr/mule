{
  Flow mainFlow=(Flow)deploymentService.getApplications().get(0).getMuleContext().getRegistry().lookupFlowConstruct(flowName);
  InternalMessage muleMessage=InternalMessage.builder().payload(TEST_MESSAGE).build();
  mainFlow.process(Event.builder(DefaultEventContext.create(mainFlow,TEST_CONNECTOR)).message(muleMessage).exchangePattern(REQUEST_RESPONSE).flow(mainFlow).build());
}
