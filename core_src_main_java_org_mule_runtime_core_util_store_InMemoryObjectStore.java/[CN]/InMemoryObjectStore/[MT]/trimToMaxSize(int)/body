{
  if (maxEntries < 0) {
    return currentSize;
  }
  int excess=(currentSize - maxEntries);
  if (excess > 0) {
    while (currentSize > maxEntries) {
      store.pollFirstEntry();
      currentSize--;
    }
    if (logger.isDebugEnabled()) {
      logger.debug("Expired " + excess + " excess entries");
    }
  }
  return currentSize;
}
