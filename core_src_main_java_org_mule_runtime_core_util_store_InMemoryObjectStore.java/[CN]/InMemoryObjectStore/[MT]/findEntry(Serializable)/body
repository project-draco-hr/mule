{
  Iterator<?> entryIterator=store.entrySet().iterator();
  while (entryIterator.hasNext()) {
    Map.Entry<?,?> entry=(Map.Entry<?,?>)entryIterator.next();
    StoredObject<T> object=(StoredObject<T>)entry.getValue();
    if (object.getId().equals(key)) {
      return entry;
    }
  }
  return null;
}
