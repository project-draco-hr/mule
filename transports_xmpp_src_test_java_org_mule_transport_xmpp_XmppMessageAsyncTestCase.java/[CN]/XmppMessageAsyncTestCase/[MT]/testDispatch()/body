{
  MuleClient client=new MuleClient();
  client.dispatch("vm://in",TEST_MESSAGE,null);
  assertTrue(latch.await(RECEIVE_TIMEOUT,TimeUnit.MILLISECONDS));
  List<Message> receivedMessages=jabberClient.getReceivedMessages();
