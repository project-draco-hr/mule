{
  if (args.length == 0 || args[0].equals(GraphConfig.ARG_HELP)) {
    printUsage();
    System.exit(0);
  }
  MuleGrapher grapher=null;
  try {
    GraphEnvironment env=new GraphConfig().init(args);
    grapher=new MuleGrapher(env);
  }
 catch (  IOException e) {
    e.printStackTrace();
    System.exit(0);
  }
  grapher.run();
}
