{
  Element properties=element.getChild("properties");
  if (properties != null) {
    for (Iterator iterator=properties.getChildren("property").iterator(); iterator.hasNext(); ) {
      Element property=(Element)iterator.next();
      caption.append(property.getAttributeValue("name") + " :" + property.getAttributeValue("value")+ "\n");
    }
  }
  for (Iterator iterator=element.getAttributes().iterator(); iterator.hasNext(); ) {
    Attribute a=(Attribute)iterator.next();
    if (!ignoreAttribute(a.getName())) {
      caption.append(a.getName() + " :" + a.getValue()+ "\n");
    }
  }
}
