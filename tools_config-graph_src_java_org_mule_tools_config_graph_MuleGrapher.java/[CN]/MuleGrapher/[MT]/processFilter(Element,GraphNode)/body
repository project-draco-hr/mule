{
  if (router.getChild("filter") != null) {
    String filterCaptions="expectedType : " + router.getChild("filter").getAttributeValue("expectedType") + "\n";
    filterCaptions+="className : " + router.getChild("filter").getAttributeValue("className") + "\n";
    filterCaptions+="expression : " + router.getChild("filter").getAttributeValue("expression");
    routerNode.getInfo().setCaption(filterCaptions);
  }
}
