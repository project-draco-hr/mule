{
  try {
    AbstractBeanDefinition result=super.doSingleBean(index,parser,element,parserContext);
    if (0 == index) {
      firstDefinition=result;
    }
    if (size() == index + 1) {
      return firstDefinition;
    }
 else {
      return null;
    }
  }
 catch (  RuntimeException e) {
    firstDefinition=null;
    throw e;
  }
}
