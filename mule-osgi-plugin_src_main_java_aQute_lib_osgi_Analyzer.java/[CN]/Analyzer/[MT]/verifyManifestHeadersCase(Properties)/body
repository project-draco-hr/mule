{
  for (Iterator<Object> i=properties.keySet().iterator(); i.hasNext(); ) {
    String header=(String)i.next();
    for (int j=0; j < headers.length; j++) {
      if (!headers[j].equals(header) && headers[j].equalsIgnoreCase(header)) {
        warning("Using a standard OSGi header with the wrong case (bnd is case sensitive!), using: " + header + " and expecting: "+ headers[j]);
        break;
      }
    }
  }
}
