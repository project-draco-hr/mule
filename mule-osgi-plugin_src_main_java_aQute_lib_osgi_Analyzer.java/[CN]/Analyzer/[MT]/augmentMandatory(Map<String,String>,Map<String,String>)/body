{
  String mandatory=(String)exporter.get("mandatory:");
  if (mandatory != null) {
    String[] attrs=mandatory.split("\\s*,\\s*");
    for (int i=0; i < attrs.length; i++) {
      if (!currentAttributes.containsKey(attrs[i]))       currentAttributes.put(attrs[i],exporter.get(attrs[i]));
    }
  }
}
