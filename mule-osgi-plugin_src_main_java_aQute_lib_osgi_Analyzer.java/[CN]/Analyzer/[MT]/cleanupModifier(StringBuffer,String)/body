{
  Matcher m=fuzzyModifier.matcher(modifier);
  if (m.matches())   modifier=m.group(2);
  for (int i=0; i < modifier.length(); i++) {
    char c=modifier.charAt(i);
    if ((c >= '0' && c <= '9') || (c >= 'a' && c <= 'z') || (c >= 'A' && c <= 'Z')|| c == '_' || c == '-')     result.append(c);
  }
}
