{
  String value=getProperty(BUNDLE_SYMBOLICNAME);
  if (value == null) {
    if (getPropertiesFile() != null)     value=getPropertiesFile().getName();
    if (value == null || value.equals("bnd.bnd"))     value=getBase().getName();
 else     if (value.endsWith(".bnd"))     value=value.substring(0,value.length() - 4);
  }
  if (value == null)   return "untitled";
  int n=value.indexOf(';');
  if (n > 0)   value=value.substring(0,n);
  return value.trim();
}
