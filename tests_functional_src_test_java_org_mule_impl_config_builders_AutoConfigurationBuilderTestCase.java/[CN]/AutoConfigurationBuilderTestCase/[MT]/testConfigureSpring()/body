{
  ConfigurationBuilder configurationBuilder=new AutoConfigurationBuilder("org/mule/test/spring/config1/test-xml-mule2-config.xml");
  configurationBuilder.configure(muleContext);
  MessagingExceptionHandler es=muleContext.getRegistry().lookupModel("main").getExceptionListener();
  assertNotNull(es);
  assertTrue(es instanceof TestExceptionStrategy);
}
