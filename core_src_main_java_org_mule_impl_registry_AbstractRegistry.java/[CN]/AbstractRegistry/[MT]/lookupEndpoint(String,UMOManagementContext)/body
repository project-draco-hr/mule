{
  Object o=lookupObject(name);
  if (o instanceof UMOEndpointBuilder) {
    try {
      o=lookupEndpointFactory().createInboundEndpoint((UMOEndpointBuilder)o,managementContext);
    }
 catch (    UMOException e) {
      logger.error(e);
      return null;
    }
  }
  return (UMOImmutableEndpoint)o;
}
