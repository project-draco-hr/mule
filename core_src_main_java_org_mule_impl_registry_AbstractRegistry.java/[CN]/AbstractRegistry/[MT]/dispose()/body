{
  if (isDisposed()) {
    return;
  }
  try {
    doDispose();
    lifecycleManager.firePhase(MuleServer.getManagementContext(),Disposable.PHASE_NAME);
    if (getParent() != null) {
      parent.dispose();
    }
 else {
      RegistryContext.setRegistry(null);
    }
  }
 catch (  UMOException e) {
    logger.error("Failed to cleanly dispose: " + e.getMessage(),e);
  }
}
