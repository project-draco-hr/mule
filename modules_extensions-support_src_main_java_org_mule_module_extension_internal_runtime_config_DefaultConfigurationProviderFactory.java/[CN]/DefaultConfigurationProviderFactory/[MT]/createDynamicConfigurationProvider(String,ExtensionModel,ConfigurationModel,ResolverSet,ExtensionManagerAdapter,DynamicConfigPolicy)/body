{
  ConfigurationObjectBuilder configurationObjectBuilder=new ConfigurationObjectBuilder(configurationModel,resolverSet);
  ConfigurationProvider<T> configurationProvider=new DynamicConfigurationProvider<>(name,extensionModel,configurationModel,extensionManager,configurationObjectBuilder,resolverSet,dynamicConfigPolicy.getExpirationPolicy());
  register(extensionModel,configurationProvider,extensionManager);
  return configurationProvider;
}
