{
  try {
    return endpoint.process(new DefaultMuleEvent(event.getMessage(),new NullInboundEndpoint(endpoint.getExchangePattern(),muleContext),event.getSession()));
  }
 catch (  MessagingException e) {
    throw e;
  }
catch (  MuleException e) {
    throw new MessagingException(e.getI18nMessage(),event,e);
  }
}
