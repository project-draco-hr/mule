{
  try {
    if (!file.exists()) {
      try {
        file.createNewFile();
      }
 catch (      IOException e) {
        throw new MuleRuntimeException(e);
      }
    }
    queueFile=new RandomAccessFile(file,"rw");
  }
 catch (  FileNotFoundException e) {
    throw new MuleRuntimeException(e);
  }
}
