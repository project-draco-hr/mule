{
  String name=null;
  if (obj instanceof NamedObject) {
    name=((NamedObject)obj).getName();
  }
 else   if (obj instanceof FlowConstruct) {
    name=((FlowConstruct)obj).getName();
  }
  if (StringUtils.isBlank(name)) {
    name=obj.getClass().getName() + ":" + UUID.getUUID();
  }
  return name;
}
