{
  Object obj=registry.lookupObject(name);
  if (obj instanceof ImmutableEndpoint) {
    return (ImmutableEndpoint)obj;
  }
 else {
    logger.debug("No endpoint with the name: " + name + "found.  If "+ name+ " is a global endpoint you should use the EndpointFactory to create endpoint instances from global endpoints.");
    return null;
  }
}
