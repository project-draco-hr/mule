{
  if (isDisposed()) {
    return;
  }
  if (getParent() != null) {
    parent.dispose();
  }
  try {
    doDispose();
    lifecycleManager.firePhase(getManagementContext(),Disposable.PHASE_NAME);
  }
 catch (  UMOException e) {
    logger.error("Failed to cleanly dispose: " + e.getMessage(),e);
  }
}
