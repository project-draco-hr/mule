{
synchronized (components) {
    MuleProxy proxy=null;
    for (int i=0; i < components.size(); i++) {
      try {
        proxy=(MuleProxy)components.get(i);
        proxy.dispose();
      }
 catch (      UMOException e) {
        logger.warn("failed to dispose object instance: " + proxy.getDescriptor().getName() + ". "+ e.getMessage());
      }
    }
  }
  components.clear();
  pool.clear();
}
