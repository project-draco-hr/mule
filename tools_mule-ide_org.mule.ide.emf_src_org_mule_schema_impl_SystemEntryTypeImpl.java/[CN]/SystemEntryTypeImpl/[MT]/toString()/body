{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", defaultValue: ");
  result.append(defaultValue);
  result.append(", key: ");
  result.append(key);
  result.append(')');
  return result.toString();
}
