{
  if (getStatistics().isEnabled()) {
    getStatistics().incrementNoRoutedMessage();
  }
  if (next != null) {
    if (logger.isDebugEnabled()) {
      logger.debug("Message did not match any routers on: " + event.getFlowConstruct().getName() + " - invoking catch all strategy");
    }
    if (getStatistics().isEnabled()) {
      getStatistics().incrementCaughtMessage();
    }
    return processNext(event);
  }
 else {
    logger.warn("Message did not match any routers on: " + event.getFlowConstruct().getName() + " and there is no catch all strategy configured on this router.  Disposing message: "+ event);
    if (logger.isDebugEnabled()) {
      try {
        logger.warn("Message fragment is: " + StringMessageUtils.truncate(event.getMessageAsString(),100,true));
      }
 catch (      MuleException e) {
      }
    }
    return null;
  }
}
