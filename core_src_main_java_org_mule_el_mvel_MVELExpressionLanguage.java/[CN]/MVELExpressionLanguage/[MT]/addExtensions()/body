{
  if (!extensionsAdded) {
    extensionsAdded=true;
    for (    MVELExpressionLanguageExtension extension : muleContext.getRegistry().lookupObjectsForLifecycle(MVELExpressionLanguageExtension.class)) {
      extension.configureParserContext(parserContext);
      extension.configureStaticVariableResolverFactory(appVariableResolverFactory);
    }
  }
}
