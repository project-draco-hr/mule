{
  super(beanDefinitionRegistry);
  setResourceLoader(new MuleResourceLoader());
  setValidationMode(VALIDATION_DTD);
  setEntityResolver(createEntityResolver());
  this.configCount=configCount;
  ((DefaultListableBeanFactory)beanDefinitionRegistry).registerCustomEditor(UMOTransformer.class,new TransformerPropertyEditor((DefaultListableBeanFactory)beanDefinitionRegistry));
}
