{
  final MuleEvent responseEvent=flowRunner("returnsCustomArray").withPayload(TEST_MESSAGE).run();
  final MuleMessage response=responseEvent.getMessage();
  assertRecords(response.getPayload(),new Record(new Field("CONTACT_NAME",CONTACT1.getName()),new Field("DETAILS",CONTACT1.getDetails())),new Record(new Field("CONTACT_NAME",CONTACT2.getName()),new Field("DETAILS",CONTACT2.getDetails())));
}
