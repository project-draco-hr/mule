{
  final MuleEvent responseEvent=flowRunner("returnsDefaultArray").withPayload(TEST_MESSAGE).run();
  final MuleMessage response=responseEvent.getMessage();
  assertRecords(response.getPayload(),new Record(new Field("REGION_NAME",NORTHWEST.getName()),new Field("ZIPS",NORTHWEST.getZips())),new Record(new Field("REGION_NAME",SOUTHWEST.getName()),new Field("ZIPS",SOUTHWEST.getZips())));
}
