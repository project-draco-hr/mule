{
  Object readMsg=null;
  try {
    readMsg=protocol.read(dataIn);
    if (dataIn.isStreaming()) {
      moreMessages=false;
    }
    return readMsg;
  }
 catch (  SocketTimeoutException e) {
    if (!socket.getKeepAlive()) {
      return null;
    }
  }
 finally {
    if (readMsg == null) {
      dataIn.close();
    }
  }
  return null;
}
