{
  URL url=ClassUtils.getResource(resourceName,callingClass);
  if (url == null) {
    url=(URL)AccessController.doPrivileged(new PrivilegedAction(){
      public Object run(){
        File f=newFile(resourceName);
        if (f.exists()) {
          try {
            return f.toURL();
          }
 catch (          MalformedURLException e) {
            return null;
          }
        }
        return null;
      }
    }
);
  }
  return url;
}
