{
  RedeliveryHandler result;
  if (handler.get() == null) {
    final CountingRedeliveryHandler newInstance=new CountingRedeliveryHandler();
    boolean ok=handler.compareAndSet(null,newInstance);
    if (!ok) {
      result=handler.get();
    }
 else {
      result=newInstance;
    }
  }
 else {
    result=handler.get();
  }
  return result;
}
