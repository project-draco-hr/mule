{
  endpoint.getConnector().start();
  Mock mockComponent=new Mock(Component.class);
  mockComponent.expectAndReturn("getResponseRouter",null);
  read.deleteOnExit();
  move.deleteOnExit();
  return new FileMessageReceiver(endpoint.getConnector(),(Component)mockComponent.proxy(),endpoint,read.getAbsolutePath(),move.getAbsolutePath(),null,1000);
}
