{
  endpoint.getConnector().start();
  Mock mockComponent=new Mock(Service.class);
  mockComponent.expectAndReturn("getInboundRouter",null);
  mockComponent.expectAndReturn("getResponseRouter",null);
  read.deleteOnExit();
  move.deleteOnExit();
  return new FileMessageReceiver(endpoint.getConnector(),(Service)mockComponent.proxy(),endpoint,read.getAbsolutePath(),move.getAbsolutePath(),null,1000);
}
