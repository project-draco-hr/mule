{
  final Connection c=feedableBodyGenerator.context.getConnection();
  if (isReady()) {
    boolean notReady=writeUntilFullOrDone(c);
    if (!isDone()) {
      if (notReady) {
        notifyReadyToFeed(new ReadyToFeedListenerImpl());
      }
 else {
        c.notifyCanWrite(new WriteHandlerImpl());
      }
    }
  }
 else {
    notifyReadyToFeed(new ReadyToFeedListenerImpl());
  }
}
