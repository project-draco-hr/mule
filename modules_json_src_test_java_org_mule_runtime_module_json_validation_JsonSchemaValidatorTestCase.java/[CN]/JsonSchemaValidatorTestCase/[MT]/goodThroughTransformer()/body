{
  muleContext.getRegistry().registerTransformer(new AppleToJson(goodJson));
  try {
    validator.validate(eventBuilder().message(InternalMessage.of(new Apple())).build(),muleContext);
  }
 catch (  JsonSchemaValidationException e) {
