{
  if (!"insert".equalsIgnoreCase(writeStmt.substring(0,6)) && !"update".equalsIgnoreCase(writeStmt.substring(0,6)) && !"delete".equalsIgnoreCase(writeStmt.substring(0,6))&& !"merge".equalsIgnoreCase(writeStmt.substring(0,5))&& !"call".equalsIgnoreCase(writeStmt.substring(0,4))) {
    return false;
  }
  return true;
}
