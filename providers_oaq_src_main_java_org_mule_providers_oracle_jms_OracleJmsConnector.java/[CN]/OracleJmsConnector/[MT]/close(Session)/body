{
  if (session != null) {
    java.sql.Connection conn=((AQjmsSession)session).getDBConnection();
    try {
      if (conn != null && conn.isClosed() == false) {
        conn.commit();
        conn.close();
      }
    }
 catch (    SQLException e) {
      JMSException ex=new JMSException(e.getMessage());
      ex.setLinkedException(e);
      throw ex;
    }
  }
}
