{
  try {
    logger.debug("Getting queue/topic connection from pool, URL = " + getJdbcConnectionPool().getURL() + ", user = "+ getJdbcConnectionPool().getUser());
    return getJdbcConnectionPool().getConnection();
  }
 catch (  SQLException e) {
    throw new JMSException("Unable to open JDBC connection: " + e.getMessage());
  }
}
