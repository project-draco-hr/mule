{
  ChainMessageProcessorBuilder builder=new ChainMessageProcessorBuilder();
  builder.chain(new AppendingMP("1"),new ChainMessageProcessorBuilder().chain(new AppendingInterceptingMP("a"),new AppendingMP("b")).build(),new AppendingInterceptingMP("2"));
  assertEquals("01beforeabafterabefore2after2",builder.build().process(getTestEvent("0")).getMessageAsString());
}
