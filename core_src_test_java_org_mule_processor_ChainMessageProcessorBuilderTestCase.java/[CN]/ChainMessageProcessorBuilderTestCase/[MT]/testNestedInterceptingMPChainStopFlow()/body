{
  ChainMessageProcessorBuilder builder=new ChainMessageProcessorBuilder();
  builder.chain(new AppendingInterceptingMP("1"),new ChainMessageProcessorBuilder().chain(new AppendingInterceptingMP("a",true),new AppendingInterceptingMP("b")).build(),new AppendingInterceptingMP("3"));
  assertEquals("0before1before3after3after1",builder.build().process(getTestEvent("0")).getMessageAsString());
}
