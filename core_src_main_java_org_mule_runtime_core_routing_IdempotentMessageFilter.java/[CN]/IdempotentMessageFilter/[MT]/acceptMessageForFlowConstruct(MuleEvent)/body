{
  if (flowConstruct.getName().equals(event.getFlowConstruct().getName())) {
    return true;
  }
 else {
    logger.error("This IdempotentMessageFilter was configured on the service: " + storePrefix + " but has received an event for service: "+ flowConstruct.getName()+ ". Please check your config to make sure each service"+ "has its own instance of IdempotentMessageFilter.");
    return false;
  }
}
