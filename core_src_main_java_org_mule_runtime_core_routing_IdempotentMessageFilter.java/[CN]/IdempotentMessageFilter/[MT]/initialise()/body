{
  if (storePrefix == null) {
    storePrefix=String.format("%s.%s.%s",ThreadNameHelper.getPrefix(muleContext),(flowConstruct == null ? "" : flowConstruct.getName()),this.getClass().getName());
  }
  if (store == null) {
    this.store=createMessageIdStore();
  }
  LifecycleUtils.initialiseIfNeeded(store);
}
