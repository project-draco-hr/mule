{
  final MuleMessage result=muleContext.getClient().send("vm://EchoService.In","foo",null);
  final LifecycleTrackerTransformer ltt=(LifecycleTrackerTransformer)result.getPayload();
  muleContext.dispose();
  assertEquals("[setProperty, setMuleContext, setMuleContext, initialise, setMuleContext, initialise, start, start, stop, stop, dispose]",ltt.getTracker().toString());
}
