{
  muleContext.getClient().send("vm://in-flowDynamic",new DefaultMuleMessage("payload",muleContext));
  assertThat(FlowStackAsserter.stackToAssert,not(nullValue()));
  assertStackElements(FlowStackAsserter.stackToAssert,isFlowStackElement("flow","/flow/processors/0"),isFlowStackElement("flowDynamic","/flowDynamic/processors/0"));
}
