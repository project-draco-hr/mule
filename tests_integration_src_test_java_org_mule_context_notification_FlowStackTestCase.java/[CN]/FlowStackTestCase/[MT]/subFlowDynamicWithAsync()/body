{
  muleContext.getClient().send("vm://in-subFlowDynamicWithAsync",new DefaultMuleMessage("payload",muleContext));
  FlowStackAsyncAsserter.latch.await(1,TimeUnit.SECONDS);
  assertThat(FlowStackAsserter.stackToAssert,not(nullValue()));
  assertStackElements(FlowStackAsserter.stackToAssert,isFlowStackElement("subFlowInAsync","/subFlowDynamicWithAsync/processors/0/0/subFlowInAsync/subprocessors/0"),isFlowStackElement("subFlowDynamicWithAsync","/subFlowDynamicWithAsync/processors/0/0"));
}
