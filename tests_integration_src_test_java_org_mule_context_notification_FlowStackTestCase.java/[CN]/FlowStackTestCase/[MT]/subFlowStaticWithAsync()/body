{
  flowRunner("subFlowStaticWithAsync").withPayload("payload").run();
  FlowStackAsyncAsserter.latch.await(1,TimeUnit.SECONDS);
  assertThat(FlowStackAsserter.stackToAssert,not(nullValue()));
  assertStackElements(FlowStackAsserter.stackToAssert,isFlowStackElement("subFlowInAsync","/subFlowStaticWithAsync/processors/0/0/subFlowInAsync/subprocessors/0"),isFlowStackElement("subFlowStaticWithAsync","/subFlowStaticWithAsync/processors/0/0"));
}
