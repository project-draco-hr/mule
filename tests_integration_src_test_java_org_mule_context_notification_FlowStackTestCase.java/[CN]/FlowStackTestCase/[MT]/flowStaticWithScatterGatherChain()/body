{
  muleContext.getClient().send("vm://in-flowStaticWithScatterGatherChain",new DefaultMuleMessage("payload",muleContext));
  assertThat(FlowStackAsserter.stackToAssert,not(nullValue()));
  assertStackElements(FlowStackAsserter.stackToAssert,isFlowStackElement("flow","/flow/processors/0"),isFlowStackElement("flowStaticWithScatterGatherChain","/flowStaticWithScatterGatherChain/processors/0/1/0"));
}
