{
  for (int i=0; i < numMessages; i++) {
    MuleMessage msg=MuleMessage.builder().payload(TEST_MESSAGE + messageNumber.getAndIncrement()).build();
    MuleEvent event=MuleEvent.builder(DefaultMessageContext.create(flowConstruct,TEST_CONNECTOR)).message(msg).exchangePattern(REQUEST_RESPONSE).flow(flowConstruct).session(session).build();
    try {
      target.process(event);
    }
 catch (    MuleException e) {
    }
  }
}
