{
  if (getFilter() == null) {
    return true;
  }
  try {
    message.applyTransformers(transformers);
  }
 catch (  TransformerException e) {
    throw new RoutingException(CoreMessages.transformFailedBeforeFilter(),message,(UMOEndpoint)endpoints.get(0),e);
  }
  return getFilter().accept(message);
}
