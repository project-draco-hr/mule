{
  MuleMessage msg=event.getMessage();
  processCookies(msg.removeProperty(HttpConnector.HTTP_COOKIES_PROPERTY),(String)msg.removeProperty(HttpConnector.HTTP_COOKIE_SPEC_PROPERTY));
  processCookies(endpoint.getProperty(HttpConnector.HTTP_COOKIES_PROPERTY),(String)endpoint.getProperty(HttpConnector.HTTP_COOKIE_SPEC_PROPERTY));
}
