{
  if (dispatchers == null) {
    dispatchers=new ConcurrentHashMap();
  }
  if (receivers == null) {
    receivers=new ConcurrentHashMap();
  }
  super.doInitialise();
  try {
    MuleManager.getInstance().registerListener(this,getName());
  }
 catch (  NotificationException nex) {
    throw new InitialisationException(nex,this);
  }
}
