{
  final Map<String,JettyHttpConnector> connectorMap=muleContext.getRegistry().lookupByType(JettyHttpConnector.class);
  if (connectorMap.isEmpty()) {
    unregisterMeQuietly();
  }
  for (  JettyHttpConnector c : connectorMap.values()) {
    if (!c.canHostFullWars()) {
      unregisterMeQuietly();
      break;
    }
  }
}
