{
  lifecycleManager.checkPhase(Startable.PHASE_NAME);
  if (isInitialStateStopped()) {
    logger.info("Connector not started because 'initialStateStopped' is true");
    return;
  }
  if (!this.isConnected()) {
    startOnConnect=true;
    try {
      connect();
    }
 catch (    Exception e) {
      throw new LifecycleException(e,this);
    }
  }
 else {
    startAfterConnect();
  }
}
