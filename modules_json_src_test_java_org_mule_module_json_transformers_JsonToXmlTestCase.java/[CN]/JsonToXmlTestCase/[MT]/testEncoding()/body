{
  JsonToXml jsonToXmlString=new JsonToXml();
  jsonToXmlString.initialise();
  String result=null;
  result=(String)jsonToXmlString.doTransform("foo","utf-8");
  assertTrue(result.indexOf("utf-8") >= 0);
  result=(String)jsonToXmlString.doTransform("foo","UTF-8");
  assertTrue(result.indexOf("utf-8") < 0);
}
