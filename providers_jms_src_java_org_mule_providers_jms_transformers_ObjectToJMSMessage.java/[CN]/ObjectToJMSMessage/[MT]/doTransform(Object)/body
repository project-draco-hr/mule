{
  Object result=null;
  try {
    if (logger.isDebugEnabled()) {
      logger.debug("Source object is " + src.getClass().getName());
    }
    result=transformToMessage(src);
    if (logger.isDebugEnabled()) {
      logger.debug("Resulting object is " + result.getClass().getName());
    }
  }
 catch (  Exception e) {
    throw new TransformerException(this,e);
  }
  return result;
}
