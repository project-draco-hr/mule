{
  ConcurrentSkipListMap<Long,Serializable> partition=expiryInfoPartition.get(partitionName);
  if (partition == null) {
    partition=new ConcurrentSkipListMap<Long,Serializable>();
    ConcurrentSkipListMap<Long,Serializable> previous=expiryInfoPartition.putIfAbsent(partitionName,partition);
    if (previous != null) {
      partition=previous;
    }
  }
  return partition;
}
