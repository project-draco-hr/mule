{
  Object cookie=null;
  try {
    cookie=cookieProtocol.read(inputStream);
  }
 catch (  Exception e) {
    helpUser(e);
  }
  if (null != cookie) {
    String parsedCookie=IOUtils.toString((InputStream)cookie);
    if (parsedCookie.length() != COOKIE.length() || !COOKIE.equals(parsedCookie)) {
      helpUser();
    }
 else {
      return true;
    }
  }
  return false;
}
