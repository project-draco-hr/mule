{
  List<String> msgs=new ArrayList<String>();
  msgs.add("This");
  msgs.add("is a");
  msgs.add("Boiler Plate");
  String plate=StringMessageUtils.getBoilerPlate(msgs,'*',12);
  assertEquals(SystemUtils.LINE_SEPARATOR + "************" + SystemUtils.LINE_SEPARATOR+ "* This     *"+ SystemUtils.LINE_SEPARATOR+ "* is a     *"+ SystemUtils.LINE_SEPARATOR+ "* Boiler   *"+ SystemUtils.LINE_SEPARATOR+ "* Plate    *"+ SystemUtils.LINE_SEPARATOR+ "************",plate);
}
