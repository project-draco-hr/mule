{
  byte[] buf=uncompressMessage(src);
  if (getReturnClass().equals(String.class)) {
    if (encoding != null) {
      try {
        return new String(buf,encoding);
      }
 catch (      UnsupportedEncodingException ex) {
        return new String(buf);
      }
    }
 else {
      return new String(buf);
    }
  }
  return buf;
}
