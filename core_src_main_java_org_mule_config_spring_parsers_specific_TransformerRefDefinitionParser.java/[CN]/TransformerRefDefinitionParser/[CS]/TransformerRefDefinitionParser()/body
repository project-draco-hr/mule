{
  super(TransformerDefinitionParser.RESPONSE_TRANSFORMERS,new ParentDefinitionParser().addAlias(AbstractMuleBeanDefinitionParser.ATTRIBUTE_REF,TransformerDefinitionParser.RESPONSE_TRANSFORMER));
  otherwise(new ParentDefinitionParser().addAlias(AbstractMuleBeanDefinitionParser.ATTRIBUTE_REF,TransformerDefinitionParser.TRANSFORMER));
}
