{
  ServerSocket serverSocket=null;
  try {
    serverSocket=new ServerSocket(Integer.parseInt(proxyPort.getValue()));
    proxyReadyLatch.countDown();
    serverSocket.accept().close();
    latch.release();
  }
 catch (  IOException e) {
  }
 finally {
    if (serverSocket != null) {
      try {
        serverSocket.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
