{
  Element properties=element.getChild(MuleTag.ELEMENT_PROPERTIES);
  if (properties != null) {
    for (Iterator iterator=properties.getChildren(MuleTag.ELEMENT_PROPERTY).iterator(); iterator.hasNext(); ) {
      Element property=(Element)iterator.next();
      caption.append(property.getAttributeValue(MuleTag.ATTRIBUTE_NAME) + " :" + lookupPropertyTemplate(property.getAttributeValue(MuleTag.ATTRIBUTE_VALUE))+ "\n");
    }
  }
  for (Iterator iterator=element.getAttributes().iterator(); iterator.hasNext(); ) {
    Attribute a=(Attribute)iterator.next();
    if (!ignoreAttribute(a.getName())) {
      caption.append(a.getName() + " :" + a.getValue()+ "\n");
    }
  }
}
