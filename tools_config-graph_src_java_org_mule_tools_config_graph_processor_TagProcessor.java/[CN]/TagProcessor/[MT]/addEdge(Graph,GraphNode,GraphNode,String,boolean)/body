{
  GraphEdge ge=graph.addEdge(src,dest);
  if (twoway) {
    ge.getInfo().setArrowTailNormal();
  }
  if (caption != null) {
    if ("in".equalsIgnoreCase(caption) && twoway) {
      caption+=" / out";
    }
 else     if ("out".equalsIgnoreCase(caption) && twoway) {
      caption+=" / in";
    }
    ge.getInfo().setCaption(caption);
  }
}
