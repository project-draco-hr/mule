{
  for (  Map.Entry<Key<?>,Binding<?>> entry : injector.getBindings().entrySet()) {
    if (entry.getKey().getAnnotation() instanceof Named) {
      String name=((Named)entry.getKey().getAnnotation()).value();
      if (name.equals(key)) {
        Object o=entry.getValue().getProvider().get();
        return (T)o;
      }
    }
  }
  return null;
}
