{
  Collection<K> entries=this.entries.removeAll(txId);
  if (logger.isDebugEnabled()) {
    logger.debug("Evicted from tx log file " + entries.size() + " entries from txid "+ txId);
  }
  if (this.entries.isEmpty()) {
    if (journalOperations > 50000) {
      clear();
      journalOperations=0;
    }
  }
}
