{
  File file=new File(filename);
  if (!file.canWrite()) {
    String dirName=file.getPath();
    int i=dirName.lastIndexOf(File.separator);
    if (i > -1) {
      dirName=dirName.substring(0,i);
      File dir=new File(dirName);
      dir.mkdirs();
    }
    file.createNewFile();
  }
  return file;
}
