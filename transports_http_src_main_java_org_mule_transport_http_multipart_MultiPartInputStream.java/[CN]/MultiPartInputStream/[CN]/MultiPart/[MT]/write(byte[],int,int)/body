{
  if (MultiPartInputStream.this._config.getMaxFileSize() > 0 && _size + length > MultiPartInputStream.this._config.getMaxFileSize())   throw new IOException("Multipart Mime part " + _name + " exceeds max filesize");
  if (MultiPartInputStream.this._config.getFileSizeThreshold() > 0 && _size + length > MultiPartInputStream.this._config.getFileSizeThreshold() && _file == null)   createFile();
  _out.write(bytes,offset,length);
  _size+=length;
}
