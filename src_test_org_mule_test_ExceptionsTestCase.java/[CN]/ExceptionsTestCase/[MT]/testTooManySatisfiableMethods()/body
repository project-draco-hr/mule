{
  Orange object=new Orange();
  TooManySatisfiableMethodsException exception=new TooManySatisfiableMethodsException(object,new IOException("something bad happened"));
  assertNotNull(exception.getCause());
  assertTrue(exception.getCause() instanceof IOException);
  assertNotNull(exception.getMessage());
  exception=new TooManySatisfiableMethodsException(object);
  assertNull(exception.getCause());
  assertNotNull(exception.getMessage());
}
