{
  if (getFilters().size() == 0) {
    return false;
  }
  int counter=0;
  for (Iterator iterator=getFilters().iterator(); iterator.hasNext(); ) {
    Filter umoFilter=(Filter)iterator.next();
    if (umoFilter.accept(message)) {
      counter++;
    }
  }
  return counter == getFilters().size();
}
