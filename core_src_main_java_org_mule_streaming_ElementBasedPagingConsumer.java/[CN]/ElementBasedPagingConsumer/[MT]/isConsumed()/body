{
  if (this.index >= this.pageSize) {
    this.loadNextPage(this.producer);
    if (this.pageSize == 0) {
      try {
        this.close();
      }
 catch (      MuleException e) {
        if (logger.isWarnEnabled()) {
          logger.warn("Expection was trapped trying to close consumer",e);
        }
      }
      return true;
    }
  }
  return false;
}
