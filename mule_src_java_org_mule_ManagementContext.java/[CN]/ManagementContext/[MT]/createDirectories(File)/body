  FileUtils.createFile(getAutoInstallDir(rootDir).getAbsolutePath());
  FileUtils.createFile(getAutoDeployDir(rootDir).getAbsolutePath());
  FileUtils.createFile(getAutoDeployProcessedDir(rootDir).getAbsolutePath());
}
