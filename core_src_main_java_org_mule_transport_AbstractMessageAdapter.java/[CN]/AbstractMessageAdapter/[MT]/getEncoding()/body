{
  assertAccess(READ);
  String encoding=(String)getProperty(MuleProperties.MULE_ENCODING_PROPERTY,PropertyScope.OUTBOUND);
  if (encoding != null) {
    return encoding;
  }
 else {
    MuleContext muleContext=MuleServer.getMuleContext();
    if (muleContext != null) {
      return muleContext.getConfiguration().getDefaultEncoding();
    }
 else {
      return CharSetUtils.defaultCharsetName();
    }
  }
}
