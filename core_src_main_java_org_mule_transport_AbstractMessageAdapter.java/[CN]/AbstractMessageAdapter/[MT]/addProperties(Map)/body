{
  addProperties(props,properties.getDefaultScope());
  assertAccess(WRITE);
  if (props != null) {
synchronized (props) {
      for (Iterator iter=props.entrySet().iterator(); iter.hasNext(); ) {
        Map.Entry entry=(Map.Entry)iter.next();
        setProperty((String)entry.getKey(),entry.getValue());
      }
    }
  }
}
