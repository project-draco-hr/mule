{
  StringBuilder key=new StringBuilder(48);
  for (int i=0; i < payload.length; i++) {
    Object o=payload[i];
    if (o != null) {
      key.append(o.getClass().getName());
    }
 else {
      key.append("null");
    }
  }
  key.append(".").append(ClassUtils.getClassName(component.getClass()));
  return key.toString();
}
