{
  int i=name.indexOf(".");
  String key;
  if (i > 0) {
    key=name.substring(0,i);
  }
 else {
    key=name;
  }
  Object o=morphDynaBean.get(key);
  if (o instanceof MorphDynaBean && i > 0) {
    return new JsonDynaBean((MorphDynaBean)o).get(name.substring(i + 1));
  }
 else {
    return o;
  }
}
