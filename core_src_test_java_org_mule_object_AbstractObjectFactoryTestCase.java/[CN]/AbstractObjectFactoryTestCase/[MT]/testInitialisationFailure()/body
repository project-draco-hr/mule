{
  AbstractObjectFactory factory=getUninitialisedObjectFactory();
  try {
    factory.initialise();
    fail("expected InitialisationException");
  }
 catch (  InitialisationException iex) {
  }
  try {
    factory.getInstance();
    fail("expected InitialisationException");
  }
 catch (  InitialisationException iex) {
  }
}
