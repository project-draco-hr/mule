{
  List messages=new ArrayList();
  if (hasErrorReport()) {
    messages.add("ERRORS");
    messages.addAll(errors);
    messages.add(StringUtils.repeat('=',76));
  }
  if (hasWarningReport()) {
    messages.add("WARNINGS");
    messages.addAll(warnings);
  }
  if (messages.size() > 0) {
    return StringMessageUtils.getBoilerPlate(messages,'#',80);
  }
 else {
    return null;
  }
}
