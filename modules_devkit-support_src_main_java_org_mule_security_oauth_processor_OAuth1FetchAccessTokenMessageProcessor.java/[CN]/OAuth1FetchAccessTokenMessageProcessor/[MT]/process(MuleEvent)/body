{
  try {
    this.oauthManager.setOauthVerifier(((String)event.getMessage().getInvocationProperty("_oauthVerifier")));
    this.oauthManager.fetchAccessToken(requestTokenUrl,accessTokenUrl,authorizationUrl,redirectUri);
  }
 catch (  Exception e) {
    throw new MessagingException(MessageFactory.createStaticMessage("Unable to fetch access token"),event,e);
  }
  return event;
}
