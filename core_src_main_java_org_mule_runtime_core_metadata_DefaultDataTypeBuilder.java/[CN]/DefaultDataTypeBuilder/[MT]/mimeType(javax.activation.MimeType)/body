{
  if (mimeType != null) {
    this.mediaPrimaryType=mimeType.getPrimaryType();
    this.mediaSubType=mimeType.getSubType();
    if (charset == null && mimeType.getParameter(CHARSET_PARAM) != null) {
      doCharset(Charset.forName(mimeType.getParameter(CHARSET_PARAM)));
    }
  }
  return this;
}
