{
  manager=(MuleManager)process(configResources);
  try {
    setContainerProperties();
    setTransformers();
    setGlobalEndpoints();
    manager.start();
  }
 catch (  Exception e) {
    throw new ConfigurationException(new Message(Messages.X_FAILED_TO_INITIALISE,"MuleManager"),e);
  }
  return manager;
}
