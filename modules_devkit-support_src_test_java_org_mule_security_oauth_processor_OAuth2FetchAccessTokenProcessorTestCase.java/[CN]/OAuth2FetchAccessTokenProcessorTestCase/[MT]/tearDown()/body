{
  if (!exception) {
    Mockito.verify(this.manager).restoreAuthorizationEvent(eventId);
    Assert.assertSame(RequestContext.getEvent(),this.restoredEvent);
    Mockito.verify(this.muleContext).fireNotification(Mockito.argThat(new OAuthNotificationMatcher(OAuthAuthorizeNotification.OAUTH_AUTHORIZATION_END,this.event)));
  }
}
