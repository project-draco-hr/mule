{
  enableAttributes(endpointParser,endpointAttributes);
  enableAttribute(endpointParser,AbstractMuleBeanDefinitionParser.ATTRIBUTE_NAME);
  addDelegate(endpointParser);
  if (uriProperties) {
    MuleChildDefinitionParser addressParser=new AddressWithPropertiesParser(metaOrProtocol,isMeta,endpointAttributes,requiredAddressAttributes,requiredProperties);
    addHandledException(BlockAttribute.BlockAttributeException.class);
    addChildDelegate(addressParser);
  }
 else {
    MuleChildDefinitionParser addressParser=new AddressParser(metaOrProtocol,isMeta,requiredAddressAttributes);
    addHandledException(BlockAttribute.BlockAttributeException.class);
    addChildDelegate(addressParser);
    MuleChildDefinitionParser propertiesParser=new PropertiesParser(PROPERTIES,endpointAttributes,requiredAddressAttributes,requiredProperties);
    addChildDelegate(propertiesParser);
  }
}
