{
  RegistryLifecycleCallback callback=new RegistryLifecycleCallback();
  registerPhase(Initialisable.PHASE_NAME,new MuleContextInitialisePhase(),new EmptyLifecycleCallback<AbstractRegistryBroker>());
  registerPhase(Startable.PHASE_NAME,new MuleContextStartPhase(),callback);
  registerPhase(Stoppable.PHASE_NAME,new MuleContextStopPhase(),callback);
  registerPhase(Disposable.PHASE_NAME,new MuleContextDisposePhase(),new EmptyLifecycleCallback<AbstractRegistryBroker>());
}
