{
  try {
    if (size() > 0) {
      DefaultMuleEvent muleEvent=new DefaultMuleEvent(toMessageCollection(),events.retrieve(events.allKeys().get(0)));
      if (getCommonRootId() != null) {
        muleEvent.getMessage().setMessageRootId(commonRootId);
      }
      return muleEvent;
    }
 else {
      return null;
    }
  }
 catch (  ObjectStoreException e) {
    return null;
  }
}
