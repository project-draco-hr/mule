{
  try {
    if (size() > 0) {
      MuleEvent lastEvent=retrieveLastStoredEvent();
      DefaultMuleEvent muleEvent=new DefaultMuleEvent(toMessageCollection(),lastEvent,getMergedSession());
      if (getCommonRootId() != null) {
        muleEvent.getMessage().setMessageRootId(commonRootId);
      }
      return muleEvent;
    }
 else {
      return VoidMuleEvent.getInstance();
    }
  }
 catch (  ObjectStoreException e) {
    return VoidMuleEvent.getInstance();
  }
}
