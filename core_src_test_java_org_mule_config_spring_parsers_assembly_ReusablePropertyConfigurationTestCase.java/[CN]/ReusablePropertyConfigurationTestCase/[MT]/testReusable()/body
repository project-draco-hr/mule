{
  ReusablePropertyConfiguration config=new ReusablePropertyConfiguration();
  setTestValues(REFERENCE,config);
  verifyTestValues(REFERENCE,config);
  verifyIgnored(REFERENCE,config);
  config.reset();
  verifyTestValues(REFERENCE,config);
  setTestValues(WRAPPER,config);
  verifyTestValues(REFERENCE,config);
  verifyTestValues(WRAPPER,config);
  verifyIgnored(WRAPPER,config);
  config.reset();
  verifyMissing(WRAPPER,config);
  verifyTestValues(REFERENCE,config);
  setTestValues(WRAPPER,config);
  verifyTestValues(REFERENCE,config);
  verifyTestValues(WRAPPER,config);
  config.reset();
  verifyMissing(WRAPPER,config);
  verifyTestValues(REFERENCE,config);
}
