{
  UMOEndpoint endpoint=new MuleEndpoint();
  endpoint.setConnector(new FileConnector());
  try {
    endpoint.setEndpointURI(new MuleEndpointURI("file://./simple"));
  }
 catch (  EndpointException e) {
    fail(e.getMessage());
  }
  endpoint.setName(name);
  endpoint.setType(type);
  return endpoint;
}
