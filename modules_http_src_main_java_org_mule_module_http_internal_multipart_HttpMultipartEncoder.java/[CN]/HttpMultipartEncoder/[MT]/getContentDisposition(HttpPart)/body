{
  StringBuilder builder=new StringBuilder();
  builder.append(FORM_DATA);
  builder.append("; name=\"");
  builder.append(part.getName());
  builder.append("\"");
  if (part.getFileName() != null) {
    builder.append("; filename=\"");
    builder.append(part.getFileName());
    builder.append("\"");
  }
  return builder.toString();
}
