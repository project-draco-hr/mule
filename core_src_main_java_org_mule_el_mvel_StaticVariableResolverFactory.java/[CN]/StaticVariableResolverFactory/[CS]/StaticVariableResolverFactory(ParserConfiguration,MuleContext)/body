{
  this.muleContext=muleContext;
  regexFunction=new FunctionInstance(new MVELFunctionAdaptor(REGEX,new RegexExpressionLanguageFuntion(),new ParserContext(parserConfiguration)));
  dateTimeFunction=new FunctionInstance(new MVELFunctionAdaptor(DATE_TIME,new DateTimeExpressionLanguageFuntion(),new ParserContext(parserConfiguration)));
}
