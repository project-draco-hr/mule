{
  String query=wsdlAddress;
  int idx=wsdlAddress.lastIndexOf('?');
  if (idx != -1) {
    query=wsdlAddress.substring(idx + 1);
  }
 else {
    return null;
  }
  String[] params=query.split("&");
  for (  String p : params) {
    if (p.startsWith("method=")) {
      return p.substring(7);
    }
  }
  return null;
}
