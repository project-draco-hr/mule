{
  CacheAwareQueueStore queueStore=queues.get(name);
  if (queueStore != null) {
    return queueStore;
  }
  queuesLock.lock();
  try {
    queueStore=queues.get(name);
    if (queueStore == null) {
      queueStore=new CacheAwareQueueStore(createQueueStore(name,config),this);
      queues.put(name,queueStore);
    }
    return queueStore;
  }
  finally {
    queuesLock.unlock();
  }
}
