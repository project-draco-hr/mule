{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", className: ");
  result.append(className);
  result.append(", ignoreBadInput: ");
  if (ignoreBadInputESet)   result.append(ignoreBadInput);
 else   result.append("<unset>");
  result.append(", name: ");
  result.append(name);
  result.append(", ref: ");
  result.append(ref);
  result.append(", returnClass: ");
  if (returnClassESet)   result.append(returnClass);
 else   result.append("<unset>");
  result.append(')');
  return result.toString();
}
