{
  boolean oldIgnoreBadInput=ignoreBadInput;
  ignoreBadInput=newIgnoreBadInput;
  boolean oldIgnoreBadInputESet=ignoreBadInputESet;
  ignoreBadInputESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.TRANSFORMER_TYPE__IGNORE_BAD_INPUT,oldIgnoreBadInput,ignoreBadInput,!oldIgnoreBadInputESet));
}
