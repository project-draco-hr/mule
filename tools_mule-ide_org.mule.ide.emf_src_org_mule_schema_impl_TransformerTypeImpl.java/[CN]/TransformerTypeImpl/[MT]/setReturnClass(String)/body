{
  String oldReturnClass=returnClass;
  returnClass=newReturnClass;
  boolean oldReturnClassESet=returnClassESet;
  returnClassESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,SchemaPackage.TRANSFORMER_TYPE__RETURN_CLASS,oldReturnClass,returnClass,!oldReturnClassESet));
}
