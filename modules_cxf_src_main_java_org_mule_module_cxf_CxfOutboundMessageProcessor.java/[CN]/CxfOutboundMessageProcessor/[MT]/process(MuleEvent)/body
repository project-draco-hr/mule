{
  try {
    MuleEvent res;
    if (!isClientProxyAvailable()) {
      res=doSendWithClient(event);
    }
 else {
      res=doSendWithProxy(event);
    }
    return res;
  }
 catch (  MessagingException e) {
    throw e;
  }
catch (  MuleException e) {
    throw e;
  }
catch (  Exception e) {
    throw new DispatchException(event,this,e);
  }
 finally {
    cleanup();
  }
}
