{
  if (transportResponse == null) {
    return null;
  }
  Object payload;
  if (response == null || response.length == 0) {
    payload=null;
  }
 else   if (response.length == 1) {
    payload=response[0];
  }
 else {
    payload=response;
  }
  MuleMessage message=transportResponse.getMessage();
  message.setPayload(payload);
  return transportResponse;
}
