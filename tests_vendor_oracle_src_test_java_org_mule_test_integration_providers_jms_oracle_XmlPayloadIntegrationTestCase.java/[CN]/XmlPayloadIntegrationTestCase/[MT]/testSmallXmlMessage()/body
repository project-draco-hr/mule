{
  AQUtil.createOrReplaceXmlQueue(jmsSession,jmsConnector.getUsername(),TestConfig.QUEUE_XML,false);
  MuleUtil.sendXmlMessageToQueue(muleClient,TestConfig.QUEUE_XML,TestConfig.XML_MESSAGE);
  assertXMLEqual(TestConfig.XML_MESSAGE,MuleUtil.receiveXmlMessageAsString(muleClient,TestConfig.QUEUE_XML,2000));
  AQUtil.dropQueue(jmsSession,jmsConnector.getUsername(),TestConfig.QUEUE_XML,false);
}
