{
  Object result;
  try {
    if (compiledScript != null) {
      result=compiledScript.eval(bindings);
    }
 else {
      result=scriptEngine.eval(scriptText,bindings);
    }
    if (result == null) {
      result=bindings.get("result");
    }
  }
 catch (  ScriptException e) {
    throw e;
  }
catch (  Exception ex) {
    throw new ScriptException(ex);
  }
  return result;
}
