{
  ImmutableEndpoint endpoint=null;
  if (null != getMuleContext() && null != getMuleContext().getRegistry()) {
    endpoint=getMuleContext().getRegistry().lookupEndpointFactory().getEndpoint(uri,ImmutableEndpoint.ENDPOINT_TYPE_SENDER);
  }
  if (null != endpoint) {
    MuleServer.getMuleContext().applyLifecycle(endpoint);
  }
  return endpoint;
}
