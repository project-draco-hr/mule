{
  UMOImmutableEndpoint endpoint=null;
  if (null != getManagementContext() && null != getManagementContext().getRegistry()) {
    endpoint=getManagementContext().getRegistry().lookupEndpointFactory().getEndpoint(uri,UMOEndpoint.ENDPOINT_TYPE_SENDER,getManagementContext());
  }
  if (null != endpoint) {
    MuleServer.getManagementContext().applyLifecycle(endpoint);
  }
  return endpoint;
}
