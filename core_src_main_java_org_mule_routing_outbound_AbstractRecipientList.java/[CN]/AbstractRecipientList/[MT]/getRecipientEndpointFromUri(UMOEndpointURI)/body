{
  UMOEndpoint endpoint=null;
  if (null != getManagementContext() && null != getManagementContext().getRegistry()) {
    endpoint=getManagementContext().getRegistry().getOrCreateEndpointForUri(uri,UMOEndpoint.ENDPOINT_TYPE_SENDER);
  }
  if (null != endpoint) {
    endpoint.initialise();
  }
  return endpoint;
}
