{
  if (value == null) {
    throw new IllegalArgumentException("parameter value may not be null");
  }
  Entry tab[]=table;
  for (int i=tab.length; i-- > 0; ) {
    for (Entry e=tab[i]; e != null; e=e.next) {
      if (e.value.equals(value)) {
        return true;
      }
    }
  }
  return false;
}
