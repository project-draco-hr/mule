{
  StringBuffer buffer=new StringBuffer();
  if (name != null) {
    buffer.append(name);
  }
 else   if (next instanceof NamedObject) {
    buffer.append(((NamedObject)next).getName());
  }
 else   if (next != null) {
    buffer.append(next.getClass().getName());
    buffer.append(":");
    buffer.append(next.hashCode());
  }
  return buffer.toString();
}
