{
  if (queue == null) {
    return null;
  }
  if (logger.isTraceEnabled()) {
    logger.trace(MessageFormat.format("{0}: Polling queue {1}, timeout = {2}",getStageName(),getStageDescription(),queueTimeout));
  }
  MuleEvent event=(MuleEvent)queue.poll(queueTimeout);
  if (event != null && lifecycleManager.isPhaseComplete(Pausable.PHASE_NAME)) {
    queue.untake(event);
    return null;
  }
  return event;
}
