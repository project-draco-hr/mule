{
  super.doStart();
  if (Startable.class.isAssignableFrom(objectFactory.getObjectClass())) {
    try {
      ((Startable)objectFactory.getInstance()).start();
    }
 catch (    Exception e) {
      throw new ModelException(CoreMessages.failedToStart("Service '" + service.getName() + "'"),e);
    }
  }
}
