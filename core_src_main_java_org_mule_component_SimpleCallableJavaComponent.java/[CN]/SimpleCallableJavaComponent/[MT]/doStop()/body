{
  super.doStop();
  if (started && Stoppable.class.isAssignableFrom(objectFactory.getObjectClass())) {
    try {
      ((Stoppable)objectFactory.getInstance()).stop();
    }
 catch (    Exception e) {
      throw new ModelException(CoreMessages.failedToStop("Service '" + service.getName() + "'"),e);
    }
  }
}
