{
  if (data instanceof String) {
    printWriter.write(data.toString());
    printWriter.flush();
  }
 else   if (data instanceof byte[]) {
    BufferedOutputStream bos=new BufferedOutputStream(dataOut);
    bos.write((byte[])data);
    bos.flush();
  }
 else {
    dataOut.write(Utility.objectToByteArray(data));
    dataOut.flush();
  }
}
