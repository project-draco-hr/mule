{
  initialised.set(false);
  try {
    if (dataOut != null)     dataOut.close();
  }
 catch (  IOException e) {
    logger.error("failed to dispose Tcp dispatcher: " + e.getMessage());
  }
  if (printWriter != null)   printWriter.close();
  try {
    if (socket != null)     socket.close();
  }
 catch (  IOException e) {
    logger.error("failed to dispose Tcp dispatcher: " + e.getMessage());
  }
}
