{
  MuleClient client=muleContext.getClient();
  MuleMessage result=client.send(String.format(URL,"/helloworld/throwException"),new DefaultMuleMessage("",muleContext),newOptions().disableStatusCodeValidation().build());
  assertEquals(expectedErrorCode,result.getInboundProperty(HttpConnector.HTTP_STATUS_PROPERTY,0));
  assertTrue(result.getPayloadAsString().contains(expectedData));
}
