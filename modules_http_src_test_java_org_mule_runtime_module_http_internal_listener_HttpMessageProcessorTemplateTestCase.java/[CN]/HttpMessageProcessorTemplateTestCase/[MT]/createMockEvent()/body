{
  InternalMessage testMessage=InternalMessage.builder().payload("").build();
  Event testEvent=spy(Event.builder(DefaultEventContext.create(MuleTestUtils.getTestFlow(muleContext),TEST_CONNECTOR)).message(testMessage).build());
  when(muleContext.getTransformationService().transform(any(InternalMessage.class),any(DataType.class))).thenReturn(InternalMessage.builder().payload("".getBytes(UTF_8)).build());
  return testEvent;
}
