{
  findAllStores();
  for (  ListableObjectStore store : stores) {
    try {
      store.close();
    }
 catch (    ObjectStoreException e) {
      logger.error("Error closing persistent store",e);
    }
  }
synchronized (this) {
    queues.clear();
  }
  return super.shutdown(mode,timeoutMSecs);
}
