{
  if (persistenceStrategy != null) {
    try {
      List msgs=persistenceStrategy.restore();
      for (      Object msg : msgs) {
        Holder h=(Holder)msg;
        getQueue(h.getQueue()).putNow(h.getId());
      }
    }
 catch (    Exception e) {
      throw new ResourceManagerSystemException(e);
    }
  }
}
