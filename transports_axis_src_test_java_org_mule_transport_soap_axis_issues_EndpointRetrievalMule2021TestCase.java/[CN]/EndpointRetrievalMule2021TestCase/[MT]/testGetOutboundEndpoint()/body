{
  ImmutableEndpoint endpoint1=muleContext.getEndpointFactory().getOutboundEndpoint("Endpoint");
  assertEndpointOk(endpoint1);
  ImmutableEndpoint endpoint2=muleContext.getEndpointFactory().getOutboundEndpoint("axis:http://localhost:" + dynamicPort.getNumber() + "/mule/Service?method=toString");
  assertEndpointOk(endpoint2);
}
