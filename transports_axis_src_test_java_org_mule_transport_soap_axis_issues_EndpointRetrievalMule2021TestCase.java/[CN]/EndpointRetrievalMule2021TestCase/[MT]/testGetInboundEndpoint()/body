{
  ImmutableEndpoint endpoint1=muleContext.getRegistry().lookupEndpointFactory().getInboundEndpoint("Endpoint");
  assertEndpointOk(endpoint1);
  ImmutableEndpoint endpoint2=muleContext.getRegistry().lookupEndpointFactory().getInboundEndpoint("axis:http://localhost:" + getPorts().get(0) + "/mule/Service?method=toString");
  assertEndpointOk(endpoint2);
}
