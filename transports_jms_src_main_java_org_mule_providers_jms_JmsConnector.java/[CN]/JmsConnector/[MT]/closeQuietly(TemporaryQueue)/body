{
  try {
    close(tempQueue);
  }
 catch (  JMSException e) {
    if (logger.isErrorEnabled()) {
      String queueName="";
      try {
        queueName=tempQueue.getQueueName();
      }
 catch (      JMSException innerEx) {
      }
      logger.info(MessageFormat.format("Faled to delete a temporary queue '{0}' Reason: {1}",new Object[]{queueName,e.getMessage()}));
    }
  }
}
