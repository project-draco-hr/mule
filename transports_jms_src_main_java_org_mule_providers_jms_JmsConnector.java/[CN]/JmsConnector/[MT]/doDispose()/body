{
  if (_connection != null) {
    try {
      _connection.close();
    }
 catch (    JMSException e) {
      logger.error("Jms connector failed to dispose properly: ",e);
    }
    _connection=null;
  }
  if (connectionFactory != null && connectionFactory instanceof Disposable) {
    ((Disposable)connectionFactory).dispose();
  }
}
