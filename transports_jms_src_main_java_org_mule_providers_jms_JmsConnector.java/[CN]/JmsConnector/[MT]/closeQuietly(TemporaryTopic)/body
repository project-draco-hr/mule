{
  try {
    close(tempTopic);
  }
 catch (  JMSException e) {
    if (logger.isErrorEnabled()) {
      String topicName="";
      try {
        topicName=tempTopic.getTopicName();
      }
 catch (      JMSException innerEx) {
      }
      logger.error("Faled to delete a temporary topic " + topicName,e);
    }
  }
}
