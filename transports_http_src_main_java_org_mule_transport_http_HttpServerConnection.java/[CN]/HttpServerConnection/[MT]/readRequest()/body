{
  if (cachedRequest != null) {
    return cachedRequest;
  }
  try {
    String line=readLine();
    if (line == null) {
      return null;
    }
    cachedRequest=new HttpRequest(RequestLine.parseLine(line),HttpParser.parseHeaders(this.in,encoding),this.in,encoding);
    return cachedRequest;
  }
 catch (  IOException e) {
    close();
    throw e;
  }
}
