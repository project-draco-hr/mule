{
  for (  T artifact : artifacts) {
    if (artifactTimestampListener.isArtifactResourceUpdated(artifact)) {
      try {
        artifactArchiveDeployer.redeploy(artifact);
      }
 catch (      DeploymentException e) {
        if (logger.isDebugEnabled()) {
          logger.debug(e);
        }
      }
    }
  }
}
