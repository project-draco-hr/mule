{
  if (src == null) {
    return null;
  }
 else {
    if (numberFormat != null) {
      try {
        return NumberUtils.convertNumberToTargetClass(numberFormat.parse((String)src),getType());
      }
 catch (      Exception e) {
        throw new TransformerException(this,e);
      }
    }
 else {
      return NumberUtils.parseNumber((String)src,getType());
    }
  }
}
